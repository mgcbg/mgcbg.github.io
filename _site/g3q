[32m+[m[32m// CSS image replacement[m
[32m+[m[32m//[m
[32m+[m[32m// Heads up! v3 launched with only `.hide-text()`, but per our pattern for[m
[32m+[m[32m// mixins being reused as classes with the same name, this doesn't hold up. As[m
[32m+[m[32m// of v3.0.1 we have added `.text-hide()` and deprecated `.hide-text()`.[m
[32m+[m[32m//[m
[32m+[m[32m// Source: https://github.com/h5bp/html5-boilerplate/commit/aa0396eae757[m
[32m+[m
[32m+[m[32m// Deprecated as of v3.0.1 (has been removed in v4)[m
[32m+[m[32m@mixin hide-text() {[m
[32m+[m[32m  font: 0/0 a;[m
[32m+[m[32m  color: transparent;[m
[32m+[m[32m  text-shadow: none;[m
[32m+[m[32m  background-color: transparent;[m
[32m+[m[32m  border: 0;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// New mixin to use as of v3.0.1[m
[32m+[m[32m@mixin text-hide() {[m
[32m+[m[32m  @include hide-text;[m
[32m+[m[32m}[m
[1mdiff --git a/_sass/bootstrap/mixins/_image.scss b/_sass/bootstrap/mixins/_image.scss[m
[1mnew file mode 100644[m
[1mindex 0000000..c8dcf5e[m
[1m--- /dev/null[m
[1m+++ b/_sass/bootstrap/mixins/_image.scss[m
[36m@@ -0,0 +1,33 @@[m
[32m+[m[32m// Image Mixins[m
[32m+[m[32m// - Responsive image[m
[32m+[m[32m// - Retina image[m
[32m+[m
[32m+[m
[32m+[m[32m// Responsive image[m
[32m+[m[32m//[m
[32m+[m[32m// Keep images from scaling beyond the width of their parents.[m
[32m+[m[32m@mixin img-responsive($display: block) {[m
[32m+[m[32m  display: $display;[m
[32m+[m[32m  max-width: 100%; // Part 1: Set a maximum relative to the parent[m
[32m+[m[32m  height: auto; // Part 2: Scale the height according to the width, otherwise you get stretching[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Retina image[m
[32m+[m[32m//[m
[32m+[m[32m// Short retina mixin for setting background-image and -size. Note that the[m
[32m+[m[32m// spelling of `min--moz-device-pixel-ratio` is intentional.[m
[32m+[m[32m@mixin img-retina($file-1x, $file-2x, $width-1x, $height-1x) {[m
[32m+[m[32m  background-image: url(if($bootstrap-sass-asset-helper, twbs-image-path("#{$file-1x}"), "#{$file-1x}"));[m
[32m+[m
[32m+[m[32m  @media[m
[32m+[m[32m  only screen and (-webkit-min-device-pixel-ratio: 2),[m
[32m+[m[32m  only screen and (   min--moz-device-pixel-ratio: 2),[m
[32m+[m[32m  only screen and (     -o-min-device-pixel-ratio: 2/1),[m
[32m+[m[32m  only screen and (        min-device-pixel-ratio: 2),[m
[32m+[m[32m  only screen and (                min-resolution: 192dpi),[m
[32m+[m[32m  only screen and (                min-resolution: 2dppx) {[m
[32m+[m[32m    background-image: url(if($bootstrap-sass-asset-helper, twbs-image-path("#{$file-2x}"), "#{$file-2x}"));[m
[32m+[m[32m    background-size: $width-1x $height-1x;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
