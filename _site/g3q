[1mnew file mode 100644[m
[1mindex 0000000..c478eeb[m
[1m--- /dev/null[m
[1m+++ b/_sass/bootstrap/mixins/_list-group.scss[m
[36m@@ -0,0 +1,32 @@[m
[32m+[m[32m// List Groups[m
[32m+[m
[32m+[m[32m@mixin list-group-item-variant($state, $background, $color) {[m
[32m+[m[32m  .list-group-item-#{$state} {[m
[32m+[m[32m    color: $color;[m
[32m+[m[32m    background-color: $background;[m
[32m+[m
[32m+[m[32m    // [converter] extracted a&, button& to a.list-group-item-#{$state}, button.list-group-item-#{$state}[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  a.list-group-item-#{$state},[m
[32m+[m[32m  button.list-group-item-#{$state} {[m
[32m+[m[32m    color: $color;[m
[32m+[m
[32m+[m[32m    .list-group-item-heading {[m
[32m+[m[32m      color: inherit;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    &:hover,[m
[32m+[m[32m    &:focus {[m
[32m+[m[32m      color: $color;[m
[32m+[m[32m      background-color: darken($background, 5%);[m
[32m+[m[32m    }[m
[32m+[m[32m    &.active,[m
[32m+[m[32m    &.active:hover,[m
[32m+[m[32m    &.active:focus {[m
[32m+[m[32m      color: #fff;[m
[32m+[m[32m      background-color: $color;[m
[32m+[m[32m      border-color: $color;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
