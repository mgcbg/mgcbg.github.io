[32m+[m[32m  padding: 2px 4px;[m
[32m+[m[32m  font-size: 90%;[m
[32m+[m[32m  color: $code-color;[m
[32m+[m[32m  background-color: $code-bg;[m
[32m+[m[32m  border-radius: $border-radius-base;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// User input typically entered via keyboard[m
[32m+[m[32mkbd {[m
[32m+[m[32m  padding: 2px 4px;[m
[32m+[m[32m  font-size: 90%;[m
[32m+[m[32m  color: $kbd-color;[m
[32m+[m[32m  background-color: $kbd-bg;[m
[32m+[m[32m  border-radius: $border-radius-small;[m
[32m+[m[32m  box-shadow: inset 0 -1px 0 rgba(0,0,0,.25);[m
[32m+[m
[32m+[m[32m  kbd {[m
[32m+[m[32m    padding: 0;[m
[32m+[m[32m    font-size: 100%;[m
[32m+[m[32m    font-weight: bold;[m
[32m+[m[32m    box-shadow: none;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Blocks of code[m
[32m+[m[32mpre {[m
[32m+[m[32m  display: block;[m
[32m+[m[32m  padding: (($line-height-computed - 1) / 2);[m
[32m+[m[32m  margin: 0 0 ($line-height-computed / 2);[m
[32m+[m[32m  font-size: ($font-size-base - 1); // 14px to 13px[m
[32m+[m[32m  line-height: $line-height-base;[m
[32m+[m[32m  word-break: break-all;[m
[32m+[m[32m  word-wrap: break-word;[m
[32m+[m[32m  color: $pre-color;[m
[32m+[m[32m  background-color: $pre-bg;[m
[32m+[m[32m  border: 1px solid $pre-border-color;[m
[32m+[m[32m  border-radius: $border-radius-base;[m
[32m+[m
[32m+[m[32m  // Account for some code outputs that place code tags in pre tags[m
[32m+[m[32m  code {[m
[32m+[m[32m    padding: 0;[m
[32m+[m[32m    font-size: inherit;[m
[32m+[m[32m    color: inherit;[m
[32m+[m[32m    white-space: pre-wrap;[m
[32m+[m[32m    background-color: transparent;[m
[32m+[m[32m    border-radius: 0;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Enable scrollable blocks of code[m
