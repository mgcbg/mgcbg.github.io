[32m+[m[32m//[m
[32m+[m[32m// Labels[m
[32m+[m[32m// --------------------------------------------------[m
[32m+[m
[32m+[m[32m.label {[m
[32m+[m[32m  display: inline;[m
[32m+[m[32m  padding: .2em .6em .3em;[m
[32m+[m[32m  font-size: 75%;[m
[32m+[m[32m  font-weight: bold;[m
[32m+[m[32m  line-height: 1;[m
[32m+[m[32m  color: $label-color;[m
[32m+[m[32m  text-align: center;[m
[32m+[m[32m  white-space: nowrap;[m
[32m+[m[32m  vertical-align: baseline;[m
[32m+[m[32m  border-radius: .25em;[m
[32m+[m
[32m+[m[32m  // [converter] extracted a& to a.label[m
[32m+[m
[32m+[m[32m  // Empty labels collapse automatically (not available in IE8)[m
[32m+[m[32m  &:empty {[m
[32m+[m[32m    display: none;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Quick fix for labels in buttons[m
[32m+[m[32m  .btn & {[m
[32m+[m[32m    position: relative;[m
[32m+[m[32m    top: -1px;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Add hover effects, but only for links[m
[32m+[m[32ma.label {[m
[32m+[m[32m  &:hover,[m
[32m+[m[32m  &:focus {[m
[32m+[m[32m    color: $label-link-hover-color;[m
[32m+[m[32m    text-decoration: none;[m
[32m+[m[32m    cursor: pointer;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Colors[m
[32m+[m[32m// Contextual variations (linked labels get darker on :hover)[m
[32m+[m
[32m+[m[32m.label-default {[m
[32m+[m[32m  @include label-variant($label-default-bg);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.label-primary {[m
[32m+[m[32m  @include label-variant($label-primary-bg);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.label-success {[m
[32m+[m[32m  @include label-variant($label-success-bg);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.label-info {[m
[32m+[m[32m  @include label-variant($label-info-bg);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.label-warning {[m
[32m+[m[32m  @include label-variant($label-warning-bg);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.label-danger {[m
[32m+[m[32m  @include label-variant($label-danger-bg);[m
[32m+[m[32m}[m
[1mdiff --git a/_sass/bootstrap/_list-group.scss b/_sass/bootstrap/_list-group.scss[m
[1mnew file mode 100644[m
[1mindex 0000000..7cb83aa[m
[1m--- /dev/null[m
[1m+++ b/_sass/bootstrap/_list-group.scss[m
[36m@@ -0,0 +1,130 @@[m
[32m+[m[32m//[m
[32m+[m[32m// List groups[m
[32m+[m[32m// --------------------------------------------------[m
[32m+[m
[32m+[m
[32m+[m[32m// Base class[m
[32m+[m[32m//[m
[32m+[m[32m// Easily usable on <ul>, <ol>, or <div>.[m
[32m+[m
[32m+[m[32m.list-group {[m
[32m+[m[32m  // No need to set list-style: none; since .list-group-item is block level[m
[32m+[m[32m  margin-bottom: 20px;[m
[32m+[m[32m  padding-left: 0; // reset padding because ul and ol[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Individual list items[m
[32m+[m[32m//[m
[32m+[m[32m// Use on `li`s or `div`s within the `.list-group` parent.[m
[32m+[m
[32m+[m[32m.list-group-item {[m
[32m+[m[32m  position: relative;[m
[32m+[m[32m  display: block;[m
[32m+[m[32m  padding: 10px 15px;[m
[32m+[m[32m  // Place the border on the list items and negative margin up for better styling[m
[32m+[m[32m  margin-bottom: -1px;[m
[32m+[m[32m  background-color: $list-group-bg;[m
[32m+[m[32m  border: 1px solid $list-group-border;[m
[32m+[m
[32m+[m[32m  // Round the first and last items[m
[32m+[m[32m  &:first-child {[m
[32m+[m[32m    @include border-top-radius($list-group-border-radius);[m
[32m+[m[32m  }[m
[32m+[m[32m  &:last-child {[m
[32m+[m[32m    margin-bottom: 0;[m
[32m+[m[32m    @include border-bottom-radius($list-group-border-radius);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Interactive list items[m
[32m+[m[32m//[m
[32m+[m[32m// Use anchor or button elements instead of `li`s or `div`s to create interactive items.[m
[32m+[m[32m// Includes an extra `.active` modifier class for showing selected items.[m
[32m+[m
[32m+[m[32ma.list-group-item,[m
[32m+[m[32mbutton.list-group-item {[m
[32m+[m[32m  color: $list-group-link-color;[m
[32m+[m
[32m+[m[32m  .list-group-item-heading {[m
[32m+[m[32m    color: $list-group-link-heading-color;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Hover state[m
[32m+[m[32m  &:hover,[m
[32m+[m[32m  &:focus {[m
[32m+[m[32m    text-decoration: none;[m
[32m+[m[32m    color: $list-group-link-hover-color;[m
[32m+[m[32m    background-color: $list-group-hover-bg;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mbutton.list-group-item {[m
[32m+[m[32m  width: 100%;[m
[32m+[m[32m  text-align: left;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.list-group-item {[m
[32m+[m[32m  // Disabled state[m
[32m+[m[32m  &.disabled,[m
[32m+[m[32m  &.disabled:hover,[m
[32m+[m[32m  &.disabled:focus {[m
[32m+[m[32m    background-color: $list-group-disabled-bg;[m
[32m+[m[32m    color: $list-group-disabled-color;[m
[32m+[m[32m    cursor: $cursor-disabled;[m
[32m+[m
[32m+[m[32m    // Force color to inherit for custom content[m
[32m+[m[32m    .list-group-item-heading {[m
[32m+[m[32m      color: inherit;[m
[32m+[m[32m    }[m
[32m+[m[32m    .list-group-item-text {[m
[32m+[m[32m      color: $list-group-disabled-text-color;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Active class on item itself, not parent[m
[32m+[m[32m  &.active,[m
[32m+[m[32m  &.active:hover,[m
[32m+[m[32m  &.active:focus {[m
[32m+[m[32m    z-index: 2; // Place active items above their siblings for proper border styling[m
[32m+[m[32m    color: $list-group-active-color;[m
[32m+[m[32m    background-color: $list-group-active-bg;[m
[32m+[m[32m    border-color: $list-group-active-border;[m
[32m+[m
[32m+[m[32m    // Force color to inherit for custom content[m
[32m+[m[32m    .list-group-item-heading,[m
[32m+[m[32m    .list-group-item-heading > small,[m
[32m+[m[32m    .list-group-item-heading > .small {[m
[32m+[m[32m      color: inherit;[m
[32m+[m[32m    }[m
[32m+[m[32m    .list-group-item-text {[m
[32m+[m[32m      color: $list-group-active-text-color;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Contextual variants[m
[32m+[m[32m//[m
[32m+[m[32m// Add modifier classes to change text and background color on individual items.[m
[32m+[m[32m// Organizationally, this must come after the `:hover` states.[m
[32m+[m
[32m+[m[32m@include list-group-item-variant(success, $state-success-bg, $state-success-text);[m
[32m+[m[32m@include list-group-item-variant(info, $state-info-bg, $state-info-text);[m
[32m+[m[32m@include list-group-item-variant(warning, $state-warning-bg, $state-warning-text);[m
[32m+[m[32m@include list-group-item-variant(danger, $state-danger-bg, $state-danger-text);[m
[32m+[m
[32m+[m
[32m+[m[32m// Custom content options[m
[32m+[m[32m//[m
[32m+[m[32m// Extra classes for creating well-formatted content within `.list-group-item`s.[m
[32m+[m
[32m+[m[32m.list-group-item-heading {[m
[32m+[m[32m  margin-top: 0;[m
[32m+[m[32m  margin-bottom: 5px;[m
[32m+[m[32m}[m
[32m+[m[32m.list-group-item-text {[m
[32m+[m[32m  margin-bottom: 0;[m
[32m+[m[32m  line-height: 1.3;[m
[32m+[m[32m}[m
