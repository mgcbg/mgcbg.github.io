[32m+[m[32m      background-color: $background;[m
[32m+[m[32m          border-color: $border;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  .badge {[m
[32m+[m[32m    color: $background;[m
[32m+[m[32m    background-color: $color;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
