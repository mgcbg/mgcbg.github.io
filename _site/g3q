[32m+[m[32m    display: inline-block;[m
[32m+[m
[32m+[m[32m    + li:before {[m
[32m+[m[32m      // [converter] Workaround for https://github.com/sass/libsass/issues/1115[m
[32m+[m[32m      $nbsp: "\00a0";[m
[32m+[m[32m      content: "#{$breadcrumb-separator}#{$nbsp}"; // Unicode space added since inline-block means non-collapsing white-space[m
[32m+[m[32m      padding: 0 5px;[m
[32m+[m[32m      color: $breadcrumb-color;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
