[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    thead {[m
[32m+[m[32m        display: table-header-group; // h5bp.com/t[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    tr,[m
[32m+[m[32m    img {[m
[32m+[m[32m        page-break-inside: avoid;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    img {[m
[32m+[m[32m        max-width: 100% !important;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    p,[m
[32m+[m[32m    h2,[m
[32m+[m[32m    h3 {[m
[32m+[m[32m        orphans: 3;[m
