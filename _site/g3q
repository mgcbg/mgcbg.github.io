[32m+[m[32m//[m
[32m+[m[32m// Tables[m
[32m+[m[32m// --------------------------------------------------[m
[32m+[m
[32m+[m
[32m+[m[32mtable {[m
[32m+[m[32m  background-color: $table-bg;[m
[32m+[m[32m}[m
[32m+[m[32mcaption {[m
[32m+[m[32m  padding-top: $table-cell-padding;[m
[32m+[m[32m  padding-bottom: $table-cell-padding;[m
[32m+[m[32m  color: $text-muted;[m
[32m+[m[32m  text-align: left;[m
[32m+[m[32m}[m
[32m+[m[32mth {[m
[32m+[m[32m  text-align: left;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Baseline styles[m
[32m+[m
[32m+[m[32m.table {[m
[32m+[m[32m  width: 100%;[m
[32m+[m[32m  max-width: 100%;[m
[32m+[m[32m  margin-bottom: $line-height-computed;[m
[32m+[m[32m  // Cells[m
[32m+[m[32m  > thead,[m
[32m+[m[32m  > tbody,[m
[32m+[m[32m  > tfoot {[m
[32m+[m[32m    > tr {[m
[32m+[m[32m      > th,[m
[32m+[m[32m      > td {[m
[32m+[m[32m        padding: $table-cell-padding;[m
[32m+[m[32m        line-height: $line-height-base;[m
[32m+[m[32m        vertical-align: top;[m
[32m+[m[32m        border-top: 1px solid $table-border-color;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  // Bottom align for column headings[m
[32m+[m[32m  > thead > tr > th {[m
[32m+[m[32m    vertical-align: bottom;[m
[32m+[m[32m    border-bottom: 2px solid $table-border-color;[m
[32m+[m[32m  }[m
[32m+[m[32m  // Remove top border from thead by default[m
[32m+[m[32m  > caption + thead,[m
[32m+[m[32m  > colgroup + thead,[m
[32m+[m[32m  > thead:first-child {[m
[32m+[m[32m    > tr:first-child {[m
[32m+[m[32m      > th,[m
[32m+[m[32m      > td {[m
[32m+[m[32m        border-top: 0;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  // Account for multiple tbody instances[m
[32m+[m[32m  > tbody + tbody {[m
[32m+[m[32m    border-top: 2px solid $table-border-color;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Nesting[m
[32m+[m[32m  .table {[m
[32m+[m[32m    background-color: $body-bg;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Condensed table w/ half padding[m
[32m+[m
[32m+[m[32m.table-condensed {[m
[32m+[m[32m  > thead,[m
[32m+[m[32m  > tbody,[m
[32m+[m[32m  > tfoot {[m
[32m+[m[32m    > tr {[m
[32m+[m[32m      > th,[m
[32m+[m[32m      > td {[m
[32m+[m[32m        padding: $table-condensed-cell-padding;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Bordered version[m
[32m+[m[32m//[m
[32m+[m[32m// Add borders all around the table and between all the columns.[m
[32m+[m
[32m+[m[32m.table-bordered {[m
[32m+[m[32m  border: 1px solid $table-border-color;[m
[32m+[m[32m  > thead,[m
[32m+[m[32m  > tbody,[m
[32m+[m[32m  > tfoot {[m
[32m+[m[32m    > tr {[m
[32m+[m[32m      > th,[m
[32m+[m[32m      > td {[m
[32m+[m[32m        border: 1px solid $table-border-color;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  > thead > tr {[m
[32m+[m[32m    > th,[m
[32m+[m[32m    > td {[m
[32m+[m[32m      border-bottom-width: 2px;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Zebra-striping[m
[32m+[m[32m//[m
[32m+[m[32m// Default zebra-stripe styles (alternating gray and transparent backgrounds)[m
[32m+[m
[32m+[m[32m.table-striped {[m
[32m+[m[32m  > tbody > tr:nth-of-type(odd) {[m
[32m+[m[32m    background-color: $table-bg-accent;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Hover effect[m
[32m+[m[32m//[m
[32m+[m[32m// Placed here since it has to come after the potential zebra striping[m
[32m+[m
[32m+[m[32m.table-hover {[m
[32m+[m[32m  > tbody > tr:hover {[m
[32m+[m[32m    background-color: $table-bg-hover;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Table cell sizing[m
[32m+[m[32m//[m
[32m+[m[32m// Reset default table behavior[m
[32m+[m
[32m+[m[32mtable col[class*="col-"] {[m
[32m+[m[32m  position: static; // Prevent border hiding in Firefox and IE9-11 (see https://github.com/twbs/bootstrap/issues/11623)[m
[32m+[m[32m  float: none;[m
[32m+[m[32m  display: table-column;[m
[32m+[m[32m}[m
[32m+[m[32mtable {[m
[32m+[m[32m  td,[m
[32m+[m[32m  th {[m
[32m+[m[32m    &[class*="col-"] {[m
[32m+[m[32m      position: static; // Prevent border hiding in Firefox and IE9-11 (see https://github.com/twbs/bootstrap/issues/11623)[m
[32m+[m[32m      float: none;[m
[32m+[m[32m      display: table-cell;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Table backgrounds[m
[32m+[m[32m//[m
[32m+[m[32m// Exact selectors below required to override `.table-striped` and prevent[m
[32m+[m[32m// inheritance to nested tables.[m
[32m+[m
[32m+[m[32m// Generate the contextual variants[m
[32m+[m[32m@include table-row-variant('active', $table-bg-active);[m
[32m+[m[32m@include table-row-variant('success', $state-success-bg);[m
[32m+[m[32m@include table-row-variant('info', $state-info-bg);[m
[32m+[m[32m@include table-row-variant('warning', $state-warning-bg);[m
[32m+[m[32m@include table-row-variant('danger', $state-danger-bg);[m
[32m+[m
[32m+[m
[32m+[m[32m// Responsive tables[m
[32m+[m[32m//[m
[32m+[m[32m// Wrap your tables in `.table-responsive` and we'll make them mobile friendly[m
[32m+[m[32m// by enabling horizontal scrolling. Only applies <768px. Everything above that[m
[32m+[m[32m// will display normally.[m
[32m+[m
[32m+[m[32m.table-responsive {[m
[32m+[m[32m  overflow-x: auto;[m
[32m+[m[32m  min-height: 0.01%; // Workaround for IE9 bug (see https://github.com/twbs/bootstrap/issues/14837)[m
[32m+[m
[32m+[m[32m  @media screen and (max-width: $screen-xs-max) {[m
[32m+[m[32m    width: 100%;[m
[32m+[m[32m    margin-bottom: ($line-height-computed * 0.75);[m
[32m+[m[32m    overflow-y: hidden;[m
[32m+[m[32m    -ms-overflow-style: -ms-autohiding-scrollbar;[m
[32m+[m[32m    border: 1px solid $table-border-color;[m
[32m+[m
[32m+[m[32m    // Tighten up spacing[m
[32m+[m[32m    > .table {[m
[32m+[m[32m      margin-bottom: 0;[m
[32m+[m
[32m+[m[32m      // Ensure the content doesn't wrap[m
[32m+[m[32m      > thead,[m
[32m+[m[32m      > tbody,[m
[32m+[m[32m      > tfoot {[m
[32m+[m[32m        > tr {[m
[32m+[m[32m          > th,[m
[32m+[m[32m          > td {[m
[32m+[m[32m            white-space: nowrap;[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Special overrides for the bordered tables[m
[32m+[m[32m    > .table-bordered {[m
[32m+[m[32m      border: 0;[m
[32m+[m
[32m+[m[32m      // Nuke the appropriate borders so that the parent can handle them[m
[32m+[m[32m      > thead,[m
[32m+[m[32m      > tbody,[m
[32m+[m[32m      > tfoot {[m
[32m+[m[32m        > tr {[m
[32m+[m[32m          > th:first-child,[m
[32m+[m[32m          > td:first-child {[m
[32m+[m[32m            border-left: 0;[m
[32m+[m[32m          }[m
[32m+[m[32m          > th:last-child,[m
[32m+[m[32m          > td:last-child {[m
[32m+[m[32m            border-right: 0;[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      // Only nuke the last row's bottom-border in `tbody` and `tfoot` since[m
[32m+[m[32m      // chances are there will be only one `tr` in a `thead` and that would[m
[32m+[m[32m      // remove the border altogether.[m
[32m+[m[32m      > tbody,[m
[32m+[m[32m      > tfoot {[m
[32m+[m[32m        > tr:last-child {[m
[32m+[m[32m          > th,[m
[32m+[m[32m          > td {[m
[32m+[m[32m            border-bottom: 0;[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
