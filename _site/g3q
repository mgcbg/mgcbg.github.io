[32m+[m[41m[m
[32m+[m[32m/*[m[41m[m
[32m+[m[32m    A simple jQuery modal (http://github.com/kylefox/jquery-modal)[m[41m[m
[32m+[m[32m    Version 0.9.1[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32m(function (factory) {[m[41m[m
[32m+[m[32m  // Making your jQuery plugin work better with npm tools[m[41m[m
[32m+[m[32m  // http://blog.npmjs.org/post/112712169830/making-your-jquery-plugin-work-better-with-npm[m[41m[m
[32m+[m[32m  if(typeof module === "object" && typeof module.exports === "object") {[m[41m[m
[32m+[m[32m    factory(require("jquery"), window, document);[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[32m  else {[m[41m[m
[32m+[m[32m    factory(jQuery, window, document);[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[32m}(function($, window, document, undefined) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  var modals = [],[m[41m[m
[32m+[m[32m      getCurrent = function() {[m[41m[m
[32m+[m[32m        return modals.length ? modals[modals.length - 1] : null;[m[41m[m
[32m+[m[32m      },[m[41m[m
[32m+[m[32m      selectCurrent = function() {[m[41m[m
[32m+[m[32m        var i,[m[41m[m
[32m+[m[32m            selected = false;[m[41m[m
[32m+[m[32m        for (i=modals.length-1; i>=0; i--) {[m[41m[m
[32m+[m[32m          if (modals[i].$blocker) {[m[41m[m
[32m+[m[32m            modals[i].$blocker.toggleClass('current',!selected).toggleClass('behind',selected);[m[41m[m
[32m+[m[32m            selected = true;[m[41m[m
[32m+[m[32m          }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m      };[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  $.modal = function(el, options) {[m[41m[m
[32m+[m[32m    var remove, target;[m[41m[m
[32m+[m[32m    this.$body = $('body');[m[41m[m
[32m+[m[32m    this.options = $.extend({}, $.modal.defaults, options);[m[41m[m
[32m+[m[32m    this.options.doFade = !isNaN(parseInt(this.options.fadeDuration, 10));[m[41m[m
[32m+[m[32m    this.$blocker = null;[m[41m[m
[32m+[m[32m    if (this.options.closeExisting)[m[41m[m
[32m+[m[32m      while ($.modal.isActive())[m[41m[m
[32m+[m[32m        $.modal.close(); // Close any open modals.[m[41m[m
[32m+[m[32m    modals.push(this);[m[41m[m
[32m+[m[32m    if (el.is('a')) {[m[41m[m
[32m+[m[32m      target = el.attr('href');[m[41m[m
[32m+[m[32m      this.anchor = el;[m[41m[m
[32m+[m[32m      //Select element by id from href[m[41m[m
[32m+[m[32m      if (/^#/.test(target)) {[m[41m[m
[32m+[m[32m        this.$elm = $(target);[m[41m[m
[32m+[m[32m        if (this.$elm.length !== 1) return null;[m[41m[m
[32m+[m[32m        this.$body.append(this.$elm);[m[41m[m
[32m+[m[32m        this.open();[m[41m[m
[32m+[m[32m      //AJAX[m[41m[m
[32m+[m[32m      } else {[m[41m[m
[32m+[m[32m        this.$elm = $('<div>');[m[41m[m
[32m+[m[32m        this.$body.append(this.$elm);[m[41m[m
[32m+[m[32m        remove = function(event, modal) { modal.elm.remove(); };[m[41m[m
[32m+[m[32m        this.showSpinner();[m[41m[m
[32m+[m[32m        el.trigger($.modal.AJAX_SEND);[m[41m[m
[32m+[m[32m        $.get(target).done(function(html) {[m[41m[m
[32m+[m[32m          if (!$.modal.isActive()) return;[m[41m[m
[32m+[m[32m          el.trigger($.modal.AJAX_SUCCESS);[m[41m[m
[32m+[m[32m          var current = getCurrent();[m[41m[m
[32m+[m[32m          current.$elm.empty().append(html).on($.modal.CLOSE, remove);[m[41m[m
[32m+[m[32m          current.hideSpinner();[m[41m[m
[32m+[m[32m          current.open();[m[41m[m
[32m+[m[32m          el.trigger($.modal.AJAX_COMPLETE);[m[41m[m
[32m+[m[32m        }).fail(function() {[m[41m[m
[32m+[m[32m          el.trigger($.modal.AJAX_FAIL);[m[41m[m
[32m+[m[32m          var current = getCurrent();[m[41m[m
[32m+[m[32m          current.hideSpinner();[m[41m[m
[32m+[m[32m          modals.pop(); // remove expected modal from the list[m[41m[m
[32m+[m[32m          el.trigger($.modal.AJAX_COMPLETE);[m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[32m      }[m[41m[m
[32m+[m[32m    } else {[m[41m[m
[32m+[m[32m      this.$elm = el;[m[41m[m
[32m+[m[32m      this.anchor = el;[m[41m[m
[32m+[m[32m      this.$body.append(this.$elm);[m[41m[m
[32m+[m[32m      this.open();[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m  };[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  $.modal.prototype = {[m[41m[m
[32m+[m[32m    constructor: $.modal,[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    open: function() {[m[41m[m
[32m+[m[32m      var m = this;[m[41m[m
[32m+[m[32m      this.block();[m[41m[m
[32m+[m[32m      this.anchor.blur();[m[41m[m
[32m+[m[32m      if(this.options.doFade) {[m[41m[m
[32m+[m[32m        setTimeout(function() {[m[41m[m
[32m+[m[32m          m.show();[m[41m[m
[32m+[m[32m        }, this.options.fadeDuration * this.options.fadeDelay);[m[41m[m
[32m+[m[32m      } else {[m[41m[m
[32m+[m[32m        this.show();[m[41m[m
[32m+[m[32m      }[m[41m[m
[32m+[m[32m      $(document).off('keydown.modal').on('keydown.modal', function(event) {[m[41m[m
[32m+[m[32m        var current = getCurrent();[m[41m[m
[32m+[m[32m        if (event.which === 27 && current.options.escapeClose) current.close();[m[41m[m
[32m+[m[32m      });[m[41m[m
[32m+[m[32m      if (this.options.clickClose)[m[41m[m
[32m+[m[32m        this.$blocker.click(function(e) {[m[41m[m
[32m+[m[32m          if (e.target === this)[m[41m[m
[32m+[m[32m            $.modal.close();[m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[32m    },[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    close: function() {[m[41m[m
[32m+[m[32m      modals.pop();[m[41m[m
[32m+[m[32m      this.unblock();[m[41m[m
[32m+[m[32m      this.hide();[m[41m[m
[32m+[m[32m      if (!$.modal.isActive())[m[41m[m
[32m+[m[32m        $(document).off('keydown.modal');[m[41m[m
[32m+[m[32m    },[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    block: function() {[m[41m[m
[32m+[m[32m      this.$elm.trigger($.modal.BEFORE_BLOCK, [this._ctx()]);[m[41m[m
[32m+[m[32m      this.$body.css('overflow','hidden');[m[41m[m
[32m+[m[32m      this.$blocker = $('<div class="' + this.options.blockerClass + ' blocker current"></div>').appendTo(this.$body);[m[41m[m
[32m+[m[32m      selectCurrent();[m[41m[m
[32m+[m[32m      if(this.options.doFade) {[m[41m[m
[32m+[m[32m        this.$blocker.css('opacity',0).animate({opacity: 1}, this.options.fadeDuration);[m[41m[m
[32m+[m[32m      }[m[41m[m
[32m+[m[32m      this.$elm.trigger($.modal.BLOCK, [this._ctx()]);[m[41m[m
[32m+[m[32m    },[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    unblock: function(now) {[m[41m[m
[32m+[m[32m      if (!now && this.options.doFade)[m[41m[m
[32m+[m[32m        this.$blocker.fadeOut(this.options.fadeDuration, this.unblock.bind(this,true));[m[41m[m
[32m+[m[32m      else {[m[41m[m
[32m+[m[32m        this.$blocker.children().appendTo(this.$body);[m[41m[m
[32m+[m[32m        this.$blocker.remove();[m[41m[m
[32m+[m[32m        this.$blocker = null;[m[41m[m
[32m+[m[32m        selectCurrent();[m[41m[m
[32m+[m[32m        if (!$.modal.isActive())[m[41m[m
[32m+[m[32m          this.$body.css('overflow','');[m[41m[m
[32m+[m[32m      }[m[41m[m
[32m+[m[32m    },[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    show: function() {[m[41m[m
[32m+[m[32m      this.$elm.trigger($.modal.BEFORE_OPEN, [this._ctx()]);[m[41m[m
[32m+[m[32m      if (this.options.showClose) {[m[41m[m
[32m+[m[32m        this.closeButton = $('<a href="#close-modal" rel="modal:close" class="close-modal ' + this.options.closeClass + '">' + this.options.closeText + '</a>');[m[41m[m
[32m+[m[32m        this.$elm.append(this.closeButton);[m[41m[m
[32m+[m[32m      }[m[41m[m
[32m+[m[32m      this.$elm.addClass(this.options.modalClass).appendTo(this.$blocker);[m[41m[m
[32m+[m[32m      if(this.options.doFade) {[m[41m[m
[32m+[m[32m        this.$elm.css({opacity: 0, display: 'inline-block'}).animate({opacity: 1}, this.options.fadeDuration);[m[41m[m
[32m+[m[32m      } else {[m[41m[m
[32m+[m[32m        this.$elm.css('display', 'inline-block');[m[41m[m
[32m+[m[32m      }[m[41m[m
[32m+[m[32m      this.$elm.trigger($.modal.OPEN, [this._ctx()]);[m[41m[m
[32m+[m[32m    },[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    hide: function() {[m[41m[m
[32m+[m[32m      this.$elm.trigger($.modal.BEFORE_CLOSE, [this._ctx()]);[m[41m[m
[32m+[m[32m      if (this.closeButton) this.closeButton.remove();[m[41m[m
[32m+[m[32m      var _this = this;[m[41m[m
[32m+[m[32m      if(this.options.doFade) {[m[41m[m
[32m+[m[32m        this.$elm.fadeOut(this.options.fadeDuration, function () {[m[41m[m
[32m+[m[32m          _this.$elm.trigger($.modal.AFTER_CLOSE, [_this._ctx()]);[m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[32m      } else {[m[41m[m
[32m+[m[32m        this.$elm.hide(0, function () {[m[41m[m
[32m+[m[32m          _this.$elm.trigger($.modal.AFTER_CLOSE, [_this._ctx()]);[m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[32m      }[m[41m[m
[32m+[m[32m      this.$elm.trigger($.modal.CLOSE, [this._ctx()]);[m[41m[m
[32m+[m[32m    },[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    showSpinner: function() {[m[41m[m
[32m+[m[32m      if (!this.options.showSpinner) return;[m[41m[m
[32m+[m[32m      this.spinner = this.spinner || $('<div class="' + this.options.modalClass + '-spinner"></div>')[m[41m[m
[32m+[m[32m        .append(this.options.spinnerHtml);[m[41m[m
[32m+[m[32m      this.$body.append(this.spinner);[m[41m[m
[32m+[m[32m      this.spinner.show();[m[41m[m
[32m+[m[32m    },[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    hideSpinner: function() {[m[41m[m
[32m+[m[32m      if (this.spinner) this.spinner.remove();[m[41m[m
[32m+[m[32m    },[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    //Return context for custom events[m[41m[m
[32m+[m[32m    _ctx: function() {[m[41m[m
[32m+[m[32m      return { elm: this.$elm, $elm: this.$elm, $blocker: this.$blocker, options: this.options };[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m  };[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  $.modal.close = function(event) {[m[41m[m
[32m+[m[32m    if (!$.modal.isActive()) return;[m[41m[m
[32m+[m[32m    if (event) event.preventDefault();[m[41m[m
[32m+[m[32m    var current = getCurrent();[m[41m[m
[32m+[m[32m    current.close();[m[41m[m
[32m+[m[32m    return current.$elm;[m[41m[m
[32m+[m[32m  };[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  // Returns if there currently is an active modal[m[41m[m
[32m+[m[32m  $.modal.isActive = function () {[m[41m[m
[32m+[m[32m    return modals.length > 0;[m[41m[m
[32m+[m[32m  };[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  $.modal.getCurrent = getCurrent;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  $.modal.defaults = {[m[41m[m
[32m+[m[32m    closeExisting: true,[m[41m[m
[32m+[m[32m    escapeClose: true,[m[41m[m
[32m+[m[32m    clickClose: true,[m[41m[m
[32m+[m[32m    closeText: null,[m[41m[m
[32m+[m[32m    closeClass: '',[m[41m[m
[32m+[m[32m    modalClass: "modal",[m[41m[m
[32m+[m[32m    blockerClass: "jquery-modal",[m[41m[m
[32m+[m[32m    spinnerHtml: '<div class="rect1"></div><div class="rect2"></div><div class="rect3"></div><div class="rect4"></div>',[m[41m[m
[32m+[m[32m    showSpinner: true,[m[41m[m
[32m+[m[32m    showClose: true,[m[41m[m
[32m+[m[32m    fadeDuration: 1000,   // Number of milliseconds the fade animation takes.[m[41m[m
[32m+[m[32m    fadeDelay: 0.5,        // Point during the overlay's fade-in that the modal begins to fade in (.5 = 50%, 1.5 = 150%, etc.)[m[41m[m
[32m+[m[32m  };[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  // Event constants[m[41m[m
[32m+[m[32m  $.modal.BEFORE_BLOCK = 'modal:before-block';[m[41m[m
[32m+[m[32m  $.modal.BLOCK = 'modal:block';[m[41m[m
[32m+[m[32m  $.modal.BEFORE_OPEN = 'modal:before-open';[m[41m[m
[32m+[m[32m  $.modal.OPEN = 'modal:open';[m[41m[m
[32m+[m[32m  $.modal.BEFORE_CLOSE = 'modal:before-close';[m[41m[m
[32m+[m[32m  $.modal.CLOSE = 'modal:close';[m[41m[m
[32m+[m[32m  $.modal.AFTER_CLOSE = 'modal:after-close';[m[41m[m
[32m+[m[32m  $.modal.AJAX_SEND = 'modal:ajax:send';[m[41m[m
[32m+[m[32m  $.modal.AJAX_SUCCESS = 'modal:ajax:success';[m[41m[m
[32m+[m[32m  $.modal.AJAX_FAIL = 'modal:ajax:fail';[m[41m[m
[32m+[m[32m  $.modal.AJAX_COMPLETE = 'modal:ajax:complete';[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  $.fn.modal = function(options){[m[41m[m
[32m+[m[32m    if (this.length === 1) {[m[41m[m
[32m+[m[32m      new $.modal(this, options);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m    return this;[m[41m[m
[32m+[m[32m  };[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  // Automatically bind links with rel="modal:close" to, well, close the modal.[m[41m[m
[32m+[m[32m  $(document).on('click.modal', 'a[rel~="modal:close"]', $.modal.close);[m[41m[m
[32m+[m[32m  $(document).on('click.modal', 'a[rel~="modal:open"]', function(event) {[m[41m[m
[32m+[m[32m    event.preventDefault();[m[41m[m
[32m+[m[32m    $(this).modal();[m[41m[m
[32m+[m[32m  });[m[41m[m
[32m+[m[32m}));[m[41m[m
