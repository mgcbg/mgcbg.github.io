[32m+[m
[32m+[m
[32m+[m[32m// Base class[m
[32m+[m[32m.badge {[m
[32m+[m[32m  display: inline-block;[m
[32m+[m[32m  min-width: 10px;[m
[32m+[m[32m  padding: 3px 7px;[m
[32m+[m[32m  font-size: $font-size-small;[m
[32m+[m[32m  font-weight: $badge-font-weight;[m
[32m+[m[32m  color: $badge-color;[m
[32m+[m[32m  line-height: $badge-line-height;[m
[32m+[m[32m  vertical-align: middle;[m
[32m+[m[32m  white-space: nowrap;[m
[32m+[m[32m  text-align: center;[m
[32m+[m[32m  background-color: $badge-bg;[m
[32m+[m[32m  border-radius: $badge-border-radius;[m
[32m+[m
[32m+[m[32m  // Empty badges collapse automatically (not available in IE8)[m
[32m+[m[32m  &:empty {[m
[32m+[m[32m    display: none;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Quick fix for badges in buttons[m
[32m+[m[32m  .btn & {[m
[32m+[m[32m    position: relative;[m
[32m+[m[32m    top: -1px;[m
[32m+[m[32m  }[m
[32m+[m
