[32m+[m[32m// Move borders to anchors instead of bottom of list[m
[32m+[m[32m//[m
[32m+[m[32m// Mixin for adding on top the shared `.nav-justified` styles for our tabs[m
[32m+[m[32m.nav-tabs-justified {[m
[32m+[m[32m  border-bottom: 0;[m
[32m+[m
[32m+[m[32m  > li > a {[m
[32m+[m[32m    // Override margin from .nav-tabs[m
[32m+[m[32m    margin-right: 0;[m
[32m+[m[32m    border-radius: $border-radius-base;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  > .active > a,[m
[32m+[m[32m  > .active > a:hover,[m
[32m+[m[32m  > .active > a:focus {[m
[32m+[m[32m    border: 1px solid $nav-tabs-justified-link-border-color;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  @media (min-width: $screen-sm-min) {[m
[32m+[m[32m    > li > a {[m
[32m+[m[32m      border-bottom: 1px solid $nav-tabs-justified-link-border-color;[m
[32m+[m[32m      border-radius: $border-radius-base $border-radius-base 0 0;[m
[32m+[m[32m    }[m
