[32m+[m[32m// Responsive utilities[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// More easily include all the states for responsive-utilities.less.[m
[32m+[m[32m// [converter] $parent hack[m
[32m+[m[32m@mixin responsive-visibility($parent) {[m
[32m+[m[32m  #{$parent} {[m
[32m+[m[32m    display: block !important;[m
[32m+[m[32m  }[m
[32m+[m[32m  table#{$parent}  { display: table !important; }[m
[32m+[m[32m  tr#{$parent}     { display: table-row !important; }[m
[32m+[m[32m  th#{$parent},[m
[32m+[m[32m  td#{$parent}     { display: table-cell !important; }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// [converter] $parent hack[m
[32m+[m[32m@mixin responsive-invisibility($parent) {[m
[32m+[m[32m  #{$parent} {[m
[32m+[m[32m    display: none !important;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
