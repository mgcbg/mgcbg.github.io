[36m@@ -0,0 +1,271 @@[m
[32m+[m[32m//[m
[32m+[m[32m// Panels[m
[32m+[m[32m// --------------------------------------------------[m
[32m+[m
[32m+[m
[32m+[m[32m// Base class[m
[32m+[m[32m.panel {[m
[32m+[m[32m  margin-bottom: $line-height-computed;[m
[32m+[m[32m  background-color: $panel-bg;[m
[32m+[m[32m  border: 1px solid transparent;[m
[32m+[m[32m  border-radius: $panel-border-radius;[m
[32m+[m[32m  @include box-shadow(0 1px 1px rgba(0,0,0,.05));[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Panel contents[m
[32m+[m[32m.panel-body {[m
[32m+[m[32m  padding: $panel-body-padding;[m
[32m+[m[32m  @include clearfix;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Optional heading[m
[32m+[m[32m.panel-heading {[m
[32m+[m[32m  padding: $panel-heading-padding;[m
[32m+[m[32m  border-bottom: 1px solid transparent;[m
[32m+[m[32m  @include border-top-radius(($panel-border-radius - 1));[m
[32m+[m
[32m+[m[32m  > .dropdown .dropdown-toggle {[m
[32m+[m[32m    color: inherit;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Within heading, strip any `h*` tag of its default margins for spacing.[m
[32m+[m[32m.panel-title {[m
[32m+[m[32m  margin-top: 0;[m
[32m+[m[32m  margin-bottom: 0;[m
[32m+[m[32m  font-size: ceil(($font-size-base * 1.125));[m
[32m+[m[32m  color: inherit;[m
[32m+[m
[32m+[m[32m  > a,[m
[32m+[m[32m  > small,[m
[32m+[m[32m  > .small,[m
[32m+[m[32m  > small > a,[m
[32m+[m[32m  > .small > a {[m
[32m+[m[32m    color: inherit;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Optional footer (stays gray in every modifier class)[m
[32m+[m[32m.panel-footer {[m
[32m+[m[32m  padding: $panel-footer-padding;[m
[32m+[m[32m  background-color: $panel-footer-bg;[m
[32m+[m[32m  border-top: 1px solid $panel-inner-border;[m
[32m+[m[32m  @include border-bottom-radius(($panel-border-radius - 1));[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// List groups in panels[m
[32m+[m[32m//[m
[32m+[m[32m// By default, space out list group content from panel headings to account for[m
[32m+[m[32m// any kind of custom content between the two.[m
[32m+[m
[32m+[m[32m.panel {[m
[32m+[m[32m  > .list-group,[m
[32m+[m[32m  > .panel-collapse > .list-group {[m
[32m+[m[32m    margin-bottom: 0;[m
[32m+[m
[32m+[m[32m    .list-group-item {[m
[32m+[m[32m      border-width: 1px 0;[m
[32m+[m[32m      border-radius: 0;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Add border top radius for first one[m
[32m+[m[32m    &:first-child {[m
[32m+[m[32m      .list-group-item:first-child {[m
[32m+[m[32m        border-top: 0;[m
[32m+[m[32m        @include border-top-radius(($panel-border-radius - 1));[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Add border bottom radius for last one[m
[32m+[m[32m    &:last-child {[m
[32m+[m[32m      .list-group-item:last-child {[m
[32m+[m[32m        border-bottom: 0;[m
[32m+[m[32m        @include border-bottom-radius(($panel-border-radius - 1));[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  > .panel-heading + .panel-collapse > .list-group {[m
[32m+[m[32m    .list-group-item:first-child {[m
[32m+[m[32m      @include border-top-radius(0);[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m// Collapse space between when there's no additional content.[m
[32m+[m[32m.panel-heading + .list-group {[m
[32m+[m[32m  .list-group-item:first-child {[m
[32m+[m[32m    border-top-width: 0;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m.list-group + .panel-footer {[m
[32m+[m[32m  border-top-width: 0;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Tables in panels[m
[32m+[m[32m//[m
[32m+[m[32m// Place a non-bordered `.table` within a panel (not within a `.panel-body`) and[m
[32m+[m[32m// watch it go full width.[m
[32m+[m
[32m+[m[32m.panel {[m
[32m+[m[32m  > .table,[m
[32m+[m[32m  > .table-responsive > .table,[m
[32m+[m[32m  > .panel-collapse > .table {[m
[32m+[m[32m    margin-bottom: 0;[m
[32m+[m
[32m+[m[32m    caption {[m
[32m+[m[32m      padding-left: $panel-body-padding;[m
[32m+[m[32m      padding-right: $panel-body-padding;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  // Add border top radius for first one[m
[32m+[m[32m  > .table:first-child,[m
[32m+[m[32m  > .table-responsive:first-child > .table:first-child {[m
[32m+[m[32m    @include border-top-radius(($panel-border-radius - 1));[m
[32m+[m
[32m+[m[32m    > thead:first-child,[m
[32m+[m[32m    > tbody:first-child {[m
[32m+[m[32m      > tr:first-child {[m
[32m+[m[32m        border-top-left-radius: ($panel-border-radius - 1);[m
[32m+[m[32m        border-top-right-radius: ($panel-border-radius - 1);[m
[32m+[m
[32m+[m[32m        td:first-child,[m
[32m+[m[32m        th:first-child {[m
[32m+[m[32m          border-top-left-radius: ($panel-border-radius - 1);[m
[32m+[m[32m        }[m
[32m+[m[32m        td:last-child,[m
[32m+[m[32m        th:last-child {[m
[32m+[m[32m          border-top-right-radius: ($panel-border-radius - 1);[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  // Add border bottom radius for last one[m
[32m+[m[32m  > .table:last-child,[m
[32m+[m[32m  > .table-responsive:last-child > .table:last-child {[m
[32m+[m[32m    @include border-bottom-radius(($panel-border-radius - 1));[m
[32m+[m
[32m+[m[32m    > tbody:last-child,[m
[32m+[m[32m    > tfoot:last-child {[m
[32m+[m[32m      > tr:last-child {[m
[32m+[m[32m        border-bottom-left-radius: ($panel-border-radius - 1);[m
[32m+[m[32m        border-bottom-right-radius: ($panel-border-radius - 1);[m
[32m+[m
[32m+[m[32m        td:first-child,[m
[32m+[m[32m        th:first-child {[m
[32m+[m[32m          border-bottom-left-radius: ($panel-border-radius - 1);[m
[32m+[m[32m        }[m
[32m+[m[32m        td:last-child,[m
[32m+[m[32m        th:last-child {[m
[32m+[m[32m          border-bottom-right-radius: ($panel-border-radius - 1);[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  > .panel-body + .table,[m
[32m+[m[32m  > .panel-body + .table-responsive,[m
[32m+[m[32m  > .table + .panel-body,[m
[32m+[m[32m  > .table-responsive + .panel-body {[m
[32m+[m[32m    border-top: 1px solid $table-border-color;[m
[32m+[m[32m  }[m
[32m+[m[32m  > .table > tbody:first-child > tr:first-child th,[m
[32m+[m[32m  > .table > tbody:first-child > tr:first-child td {[m
[32m+[m[32m    border-top: 0;[m
[32m+[m[32m  }[m
[32m+[m[32m  > .table-bordered,[m
[32m+[m[32m  > .table-responsive > .table-bordered {[m
[32m+[m[32m    border: 0;[m
[32m+[m[32m    > thead,[m
[32m+[m[32m    > tbody,[m
[32m+[m[32m    > tfoot {[m
[32m+[m[32m      > tr {[m
[32m+[m[32m        > th:first-child,[m
[32m+[m[32m        > td:first-child {[m
[32m+[m[32m          border-left: 0;[m
[32m+[m[32m        }[m
[32m+[m[32m        > th:last-child,[m
[32m+[m[32m        > td:last-child {[m
[32m+[m[32m          border-right: 0;[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    > thead,[m
[32m+[m[32m    > tbody {[m
[32m+[m[32m      > tr:first-child {[m
[32m+[m[32m        > td,[m
[32m+[m[32m        > th {[m
[32m+[m[32m          border-bottom: 0;[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    > tbody,[m
[32m+[m[32m    > tfoot {[m
[32m+[m[32m      > tr:last-child {[m
[32m+[m[32m        > td,[m
[32m+[m[32m        > th {[m
[32m+[m[32m          border-bottom: 0;[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  > .table-responsive {[m
[32m+[m[32m    border: 0;[m
[32m+[m[32m    margin-bottom: 0;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Collapsible panels (aka, accordion)[m
[32m+[m[32m//[m
[32m+[m[32m// Wrap a series of panels in `.panel-group` to turn them into an accordion with[m
[32m+[m[32m// the help of our collapse JavaScript plugin.[m
[32m+[m
[32m+[m[32m.panel-group {[m
[32m+[m[32m  margin-bottom: $line-height-computed;[m
[32m+[m
[32m+[m[32m  // Tighten up margin so it's only between panels[m
[32m+[m[32m  .panel {[m
[32m+[m[32m    margin-bottom: 0;[m
[32m+[m[32m    border-radius: $panel-border-radius;[m
[32m+[m
[32m+[m[32m    + .panel {[m
[32m+[m[32m      margin-top: 5px;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  .panel-heading {[m
[32m+[m[32m    border-bottom: 0;[m
[32m+[m
[32m+[m[32m    + .panel-collapse > .panel-body,[m
[32m+[m[32m    + .panel-collapse > .list-group {[m
[32m+[m[32m      border-top: 1px solid $panel-inner-border;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  .panel-footer {[m
[32m+[m[32m    border-top: 0;[m
[32m+[m[32m    + .panel-collapse .panel-body {[m
[32m+[m[32m      border-bottom: 1px solid $panel-inner-border;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Contextual variations[m
[32m+[m[32m.panel-default {[m
[32m+[m[32m  @include panel-variant($panel-default-border, $panel-default-text, $panel-default-heading-bg, $panel-default-border);[m
[32m+[m[32m}[m
[32m+[m[32m.panel-primary {[m
[32m+[m[32m  @include panel-variant($panel-primary-border, $panel-primary-text, $panel-primary-heading-bg, $panel-primary-border);[m
[32m+[m[32m}[m
[32m+[m[32m.panel-success {[m
[32m+[m[32m  @include panel-variant($panel-success-border, $panel-success-text, $panel-success-heading-bg, $panel-success-border);[m
[32m+[m[32m}[m
[32m+[m[32m.panel-info {[m
[32m+[m[32m  @include panel-variant($panel-info-border, $panel-info-text, $panel-info-heading-bg, $panel-info-border);[m
[32m+[m[32m}[m
[32m+[m[32m.panel-warning {[m
[32m+[m[32m  @include panel-variant($panel-warning-border, $panel-warning-text, $panel-warning-heading-bg, $panel-warning-border);[m
[32m+[m[32m}[m
[32m+[m[32m.panel-danger {[m
[32m+[m[32m  @include panel-variant($panel-danger-border, $panel-danger-text, $panel-danger-heading-bg, $panel-danger-border);[m
[32m+[m[32m}[m
