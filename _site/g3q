[32m+[m[32m//[m
[32m+[m[32m// Load core variables and mixins[m
[32m+[m[32m// --------------------------------------------------[m
[32m+[m
[32m+[m[32m@import "variables";[m
[32m+[m[32m@import "mixins";[m
[32m+[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Buttons[m
[32m+[m[32m// --------------------------------------------------[m
[32m+[m
[32m+[m[32m// Common styles[m
[32m+[m[32m.btn-default,[m
[32m+[m[32m.btn-primary,[m
[32m+[m[32m.btn-success,[m
[32m+[m[32m.btn-info,[m
[32m+[m[32m.btn-warning,[m
[32m+[m[32m.btn-danger {[m
[32m+[m[32m  text-shadow: 0 -1px 0 rgba(0,0,0,.2);[m
[32m+[m[32m  $shadow: inset 0 1px 0 rgba(255,255,255,.15), 0 1px 1px rgba(0,0,0,.075);[m
[32m+[m[32m  @include box-shadow($shadow);[m
[32m+[m
[32m+[m[32m  // Reset the shadow[m
[32m+[m[32m  &:active,[m
[32m+[m[32m  &.active {[m
[32m+[m[32m    @include box-shadow(inset 0 3px 5px rgba(0,0,0,.125));[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  &.disabled,[m
[32m+[m[32m  &[disabled],[m
[32m+[m[32m  fieldset[disabled] & {[m
[32m+[m[32m    @include box-shadow(none);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  .badge {[m
[32m+[m[32m    text-shadow: none;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Mixin for generating new styles[m
[32m+[m[32m@mixin btn-styles($btn-color: #555) {[m
[32m+[m[32m  @include gradient-vertical($start-color: $btn-color, $end-color: darken($btn-color, 12%));[m
[32m+[m[32m  @include reset-filter; // Disable gradients for IE9 because filter bleeds through rounded corners; see https://github.com/twbs/bootstrap/issues/10620[m
[32m+[m[32m  background-repeat: repeat-x;[m
[32m+[m[32m  border-color: darken($btn-color, 14%);[m
[32m+[m
[32m+[m[32m  &:hover,[m
[32m+[m[32m  &:focus  {[m
[32m+[m[32m    background-color: darken($btn-color, 12%);[m
[32m+[m[32m    background-position: 0 -15px;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  &:active,[m
[32m+[m[32m  &.active {[m
[32m+[m[32m    background-color: darken($btn-color, 12%);[m
[32m+[m[32m    border-color: darken($btn-color, 14%);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  &.disabled,[m
[32m+[m[32m  &[disabled],[m
[32m+[m[32m  fieldset[disabled] & {[m
[32m+[m[32m    &,[m
[32m+[m[32m    &:hover,[m
[32m+[m[32m    &:focus,[m
[32m+[m[32m    &.focus,[m
[32m+[m[32m    &:active,[m
[32m+[m[32m    &.active {[m
[32m+[m[32m      background-color: darken($btn-color, 12%);[m
[32m+[m[32m      background-image: none;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Common styles[m
[32m+[m[32m.btn {[m
[32m+[m[32m  // Remove the gradient for the pressed/active state[m
[32m+[m[32m  &:active,[m
[32m+[m[32m  &.active {[m
[32m+[m[32m    background-image: none;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Apply the mixin to the buttons[m
[32m+[m[32m.btn-default { @include btn-styles($btn-default-bg); text-shadow: 0 1px 0 #fff; border-color: #ccc; }[m
[32m+[m[32m.btn-primary { @include btn-styles($btn-primary-bg); }[m
[32m+[m[32m.btn-success { @include btn-styles($btn-success-bg); }[m
