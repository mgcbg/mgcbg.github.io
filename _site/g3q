[32m+[m[32m  > li.active > a:hover,[m
[32m+[m[32m  > li.active > a:focus,[m
[32m+[m[32m  .open > a,[m
[32m+[m[32m  .open > a:hover,[m
[32m+[m[32m  .open > a:focus {[m
[32m+[m[32m    padding-bottom: ($padding-large-vertical + 3);[m
[32m+[m[32m    margin-top: 0;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  .open > a,[m
[32m+[m[32m  .open > a:hover,[m
[32m+[m[32m  .open > a:focus {[m
[32m+[m[32m    border-color: $navbar-default-border;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  > li.disabled > a:hover,[m
[32m+[m[32m  > li.disabled > a:focus {[m
[32m+[m[32m    padding-top: 10px;[m
[32m+[m[32m    padding-bottom: 10px;[m
[32m+[m[32m    margin-top: 6px;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  &.nav-justified > li {[m
[32m+[m[32m    vertical-align: bottom;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
