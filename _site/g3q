[32m+[m[32m/*[m[41m[m
[32m+[m[32m// When the user clicks on div, open the popup[m[41m[m
[32m+[m[32mfunction myFunction() {[m[41m[m
[32m+[m[32m  var popup = document.getElementById("myPopup");[m[41m[m
[32m+[m[32m  popup.classList.toggle("show");[m[41m[m
[32m+[m[32m}[m[41m[m
