[32m+[m[32m/* Fade in an element */[m
[32m+[m[32m.anim-fade-in { animation-name: fade-in; animation-duration: 1s; animation-timing-function: ease-in-out; }[m
[32m+[m[32m.anim-fade-in.fast { animation-duration: 300ms; }[m
[32m+[m
[32m+[m[32m@keyframes fade-in { 0% { opacity: 0; }[m
[32m+[m[32m  100% { opacity: 1; } }[m
[32m+[m[32m/* Fade out an element */[m
[32m+[m[32m.anim-fade-out { animation-name: fade-out; animation-duration: 1s; animation-timing-function: ease-out; }[m
[32m+[m[32m.anim-fade-out.fast { animation-duration: 0.3s; }[m
[32m+[m
[32m+[m[32m@keyframes fade-out { 0% { opacity: 1; }[m
[32m+[m[32m  100% { opacity: 0; } }[m
[32m+[m[32m/* Fade in and slide up an element */[m
[32m+[m[32m.anim-fade-up { opacity: 0; animation-name: fade-up; animation-duration: 0.3s; animation-fill-mode: forwards; animation-timing-function: ease-out; animation-delay: 1s; }[m
[32m+[m
[32m+[m[32m@keyframes fade-up { 0% { opacity: 0.8; transform: translateY(100%); }[m
[32m+[m[32m  100% { opacity: 1; transform: translateY(0); } }[m
[32m+[m[32m/* Fade an element out and slide down */[m
[32m+[m[32m.anim-fade-down { animation-name: fade-down; animation-duration: 0.3s; animation-fill-mode: forwards; animation-timing-function: ease-in; }[m
[32m+[m
[32m+[m[32m@keyframes fade-down { 0% { opacity: 1; transform: translateY(0); }[m
[32m+[m[32m  100% { opacity: 0.5; transform: translateY(100%); } }[m
[32m+[m[32m/* Grow an element width from 0 to 100% */[m
[32m+[m[32m.anim-grow-x { width: 0%; animation-name: grow-x; animation-duration: 0.3s; animation-fill-mode: forwards; animation-timing-function: ease; animation-delay: 0.5s; }[m
[32m+[m
[32m+[m[32m@keyframes grow-x { to { width: 100%; } }[m
[32m+[m[32m/* Shrink an element from 100% to 0% */[m
[32m+[m[32m.anim-shrink-x { animation-name: shrink-x; animation-duration: 0.3s; animation-fill-mode: forwards; animation-timing-function: ease-in-out; animation-delay: 0.5s; }[m
[32m+[m
[32m+[m[32m@keyframes shrink-x { to { width: 0%; } }[m
[32m+[m[32m/* Fade in an element and scale it fast */[m
[32m+[m[32m.anim-scale-in { animation-name: scale-in; animation-duration: 0.15s; animation-timing-function: cubic-bezier(0.2, 0, 0.13, 1.5); }[m
[32m+[m
[32m+[m[32m@keyframes scale-in { 0% { opacity: 0; transform: scale(0.5); }[m
[32m+[m[32m  100% { opacity: 1; transform: scale(1); } }[m
[32m+[m[32m/* Pulse an element's opacity */[m
[32m+[m[32m.anim-pulse { animation-name: pulse; animation-duration: 2s; animation-timing-function: linear; animation-iteration-count: infinite; }[m
[32m+[m
[32m+[m[32m@keyframes pulse { 0% { opacity: 0.3; }[m
[32m+[m[32m  10% { opacity: 1; }[m
[32m+[m[32m  100% { opacity: 0.3; } }[m
[32m+[m[32m/* Pulse in an element */[m
[32m+[m[32m.anim-pulse-in { animation-name: pulse-in; animation-duration: 0.5s; }[m
[32m+[m
[32m+[m[32m@keyframes pulse-in { 0% { transform: scale3d(1, 1, 1); }[m
[32m+[m[32m  50% { transform: scale3d(1.1, 1.1, 1.1); }[m
[32m+[m[32m  100% { transform: scale3d(1, 1, 1); } }[m
