[32m+[m[32m//[m
[32m+[m[32m// Modals[m
[32m+[m[32m// --------------------------------------------------[m
[32m+[m
[32m+[m[32m// .modal-open      - body class for killing the scroll[m
[32m+[m[32m// .modal           - container to scroll within[m
[32m+[m[32m// .modal-dialog    - positioning shell for the actual modal[m
[32m+[m[32m// .modal-content   - actual modal w/ bg and corners and shit[m
[32m+[m
[32m+[m[32m// Kill the scroll on the body[m
[32m+[m[32m.modal-open {[m
[32m+[m[32m  overflow: hidden;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Container that the modal scrolls within[m
[32m+[m[32m.modal {[m
[32m+[m[32m  display: none;[m
[32m+[m[32m  overflow: hidden;[m
[32m+[m[32m  position: fixed;[m
[32m+[m[32m  top: 0;[m
[32m+[m[32m  right: 0;[m
[32m+[m[32m  bottom: 0;[m
[32m+[m[32m  left: 0;[m
[32m+[m[32m  z-index: $zindex-modal;[m
[32m+[m[32m  -webkit-overflow-scrolling: touch;[m
[32m+[m
[32m+[m[32m  // Prevent Chrome on Windows from adding a focus outline. For details, see[m
[32m+[m[32m  // https://github.com/twbs/bootstrap/pull/10951.[m
[32m+[m[32m  outline: 0;[m
[32m+[m
[32m+[m[32m  // When fading in the modal, animate it to slide down[m
[32m+[m[32m  &.fade .modal-dialog {[m
[32m+[m[32m    @include translate(0, -25%);[m
[32m+[m[32m    @include transition-transform(0.3s ease-out);[m
[32m+[m[32m  }[m
[32m+[m[32m  &.in .modal-dialog { @include translate(0, 0) }[m
[32m+[m[32m}[m
[32m+[m[32m.modal-open .modal {[m
[32m+[m[32m  overflow-x: hidden;[m
[32m+[m[32m  overflow-y: auto;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Shell div to position the modal with bottom padding[m
[32m+[m[32m.modal-dialog {[m
[32m+[m[32m  position: relative;[m
[32m+[m[32m  width: auto;[m
[32m+[m[32m  margin: 10px;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Actual modal[m
[32m+[m[32m.modal-content {[m
[32m+[m[32m  position: relative;[m
[32m+[m[32m  background-color: $modal-content-bg;[m
[32m+[m[32m  border: 1px solid $modal-content-fallback-border-color; //old browsers fallback (ie8 etc)[m
[32m+[m[32m  border: 1px solid $modal-content-border-color;[m
[32m+[m[32m  border-radius: $border-radius-large;[m
[32m+[m[32m  @include box-shadow(0 3px 9px rgba(0,0,0,.5));[m
[32m+[m[32m  background-clip: padding-box;[m
[32m+[m[32m  // Remove focus outline from opened modal[m
[32m+[m[32m  outline: 0;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Modal background[m
[32m+[m[32m.modal-backdrop {[m
[32m+[m[32m  position: fixed;[m
[32m+[m[32m  top: 0;[m
[32m+[m[32m  right: 0;[m
[32m+[m[32m  bottom: 0;[m
[32m+[m[32m  left: 0;[m
[32m+[m[32m  z-index: $zindex-modal-background;[m
[32m+[m[32m  background-color: $modal-backdrop-bg;[m
[32m+[m[32m  // Fade for backdrop[m
[32m+[m[32m  &.fade { @include opacity(0); }[m
[32m+[m[32m  &.in { @include opacity($modal-backdrop-opacity); }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Modal header[m
[32m+[m[32m// Top section of the modal w/ title and dismiss[m
[32m+[m[32m.modal-header {[m
[32m+[m[32m  padding: $modal-title-padding;[m
[32m+[m[32m  border-bottom: 1px solid $modal-header-border-color;[m
[32m+[m[32m  @include clearfix;[m
[32m+[m[32m}[m
[32m+[m[32m// Close icon[m
[32m+[m[32m.modal-header .close {[m
[32m+[m[32m  margin-top: -2px;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Title text within header[m
[32m+[m[32m.modal-title {[m
[32m+[m[32m  margin: 0;[m
[32m+[m[32m  line-height: $modal-title-line-height;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Modal body[m
[32m+[m[32m// Where all modal content resides (sibling of .modal-header and .modal-footer)[m
[32m+[m[32m.modal-body {[m
[32m+[m[32m  position: relative;[m
[32m+[m[32m  padding: $modal-inner-padding;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Footer (for actions)[m
[32m+[m[32m.modal-footer {[m
[32m+[m[32m  padding: $modal-inner-padding;[m
[32m+[m[32m  text-align: right; // right align buttons[m
[32m+[m[32m  border-top: 1px solid $modal-footer-border-color;[m
[32m+[m[32m  @include clearfix; // clear it in case folks use .pull-* classes on buttons[m
[32m+[m
[32m+[m[32m  // Properly space out buttons[m
[32m+[m[32m  .btn + .btn {[m
[32m+[m[32m    margin-left: 5px;[m
[32m+[m[32m    margin-bottom: 0; // account for input[type="submit"] which gets the bottom margin like all other inputs[m
[32m+[m[32m  }[m
[32m+[m[32m  // but override that for button groups[m
[32m+[m[32m  .btn-group .btn + .btn {[m
[32m+[m[32m    margin-left: -1px;[m
[32m+[m[32m  }[m
[32m+[m[32m  // and override it for block buttons as well[m
[32m+[m[32m  .btn-block + .btn-block {[m
[32m+[m[32m    margin-left: 0;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Measure scrollbar width for padding body during modal show/hide[m
[32m+[m[32m.modal-scrollbar-measure {[m
[32m+[m[32m  position: absolute;[m
[32m+[m[32m  top: -9999px;[m
[32m+[m[32m  width: 50px;[m
[32m+[m[32m  height: 50px;[m
[32m+[m[32m  overflow: scroll;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Scale up the modal[m
[32m+[m[32m@media (min-width: $screen-sm-min) {[m
[32m+[m[32m  // Automatically set modal's width for larger viewports[m
[32m+[m[32m  .modal-dialog {[m
[32m+[m[32m    width: $modal-md;[m
[32m+[m[32m    margin: 30px auto;[m
[32m+[m[32m  }[m
[32m+[m[32m  .modal-content {[m
[32m+[m[32m    @include box-shadow(0 5px 15px rgba(0,0,0,.5));[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Modal sizes[m
[32m+[m[32m  .modal-sm { width: $modal-sm; }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m@media (min-width: $screen-md-min) {[m
[32m+[m[32m  .modal-lg { width: $modal-lg; }[m
[32m+[m[32m}[m
[1mdiff --git a/_sass/bootstrap/_navbar.scss b/_sass/bootstrap/_navbar.scss[m
[1mnew file mode 100644[m
[1mindex 0000000..0f4de2d[m
[1m--- /dev/null[m
[1m+++ b/_sass/bootstrap/_navbar.scss[m
[36m@@ -0,0 +1,667 @@[m
[32m+[m[32m//[m
[32m+[m[32m// Navbars[m
[32m+[m[32m// --------------------------------------------------[m
[32m+[m
[32m+[m
[32m+[m[32m// Wrapper and base class[m
[32m+[m[32m//[m
[32m+[m[32m// Provide a static navbar from which we expand to create full-width, fixed, and[m
[32m+[m[32m// other navbar variations.[m
[32m+[m
[32m+[m[32m.navbar {[m
[32m+[m[32m  position: relative;[m
[32m+[m[32m  min-height: $navbar-height; // Ensure a navbar always shows (e.g., without a .navbar-brand in collapsed mode)[m
[32m+[m[32m  margin-bottom: $navbar-margin-bottom;[m
[32m+[m[32m  border: 5px solid transparent;[m
[32m+[m[32m  -webkit-box-shadow: 0 8px 6px -6px #999;[m
[32m+[m[32m  -moz-box-shadow: 0 8px 6px -6px #999;[m
[32m+[m[32m  box-shadow: 0 8px 6px -6px #999;[m
[32m+[m
[32m+[m
[32m+[m[32m  // Prevent floats from breaking the navbar[m
[32m+[m[32m  @include clearfix;[m
[32m+[m
[32m+[m[32m  @media (min-width: $grid-float-breakpoint) {[m
[32m+[m[32m    border-radius: $navbar-border-radius;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Navbar heading[m
[32m+[m[32m//[m
[32m+[m[32m// Groups `.navbar-brand` and `.navbar-toggle` into a single component for easy[m
[32m+[m[32m// styling of responsive aspects.[m
[32m+[m
[32m+[m[32m.navbar-header {[m
[32m+[m[32m  @include clearfix;[m
[32m+[m
[32m+[m[32m  @media (min-width: $grid-float-breakpoint) {[m
[32m+[m[32m    float: left;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Navbar collapse (body)[m
[32m+[m[32m//[m
[32m+[m[32m// Group your navbar content into this for easy collapsing and expanding across[m
[32m+[m[32m// various device sizes. By default, this content is collapsed when <768px, but[m
[32m+[m[32m// will expand past that for a horizontal display.[m
[32m+[m[32m//[m
[32m+[m[32m// To start (on mobile devices) the navbar links, forms, and buttons are stacked[m
[32m+[m[32m// vertically and include a `max-height` to overflow in case you have too much[m
[32m+[m[32m// content for the user's viewport.[m
[32m+[m
[32m+[m[32m.navbar-collapse {[m
[32m+[m[32m  overflow-x: visible;[m
[32m+[m[32m  padding-right: $navbar-padding-horizontal;[m
[32m+[m[32m  padding-left:  $navbar-padding-horizontal;[m
[32m+[m[32m  border-top: 1px solid transparent;[m
[32m+[m[32m  box-shadow: inset 0 1px 0 rgba(255,255,255,.1);[m
[32m+[m[32m  @include clearfix;[m
[32m+[m[32m  -webkit-overflow-scrolling: touch;[m
[32m+[m
[32m+[m[32m  &.in {[m
[32m+[m[32m    overflow-y: auto;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  @media (min-width: $grid-float-breakpoint) {[m
[32m+[m[32m    width: auto;[m
[32m+[m[32m    border-top: 0;[m
[32m+[m[32m    box-shadow: none;[m
[32m+[m
[32m+[m[32m    &.collapse {[m
[32m+[m[32m      display: block !important;[m
[32m+[m[32m      height: auto !important;[m
[32m+[m[32m      padding-bottom: 0; // Override default setting[m
[32m+[m[32m      overflow: visible !important;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    &.in {[m
[32m+[m[32m      overflow-y: visible;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Undo the collapse side padding for navbars with containers to ensure[m
[32m+[m[32m    // alignment of right-aligned contents.[m
[32m+[m[32m    .navbar-fixed-top &,[m
[32m+[m[32m    .navbar-static-top &,[m
[32m+[m[32m    .navbar-fixed-bottom & {[m
[32m+[m[32m      padding-left: 0;[m
[32m+[m[32m      padding-right: 0;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.navbar-fixed-top,[m
[32m+[m[32m.navbar-fixed-bottom {[m
[32m+[m[32m  .navbar-collapse {[m
[32m+[m[32m    max-height: $navbar-collapse-max-height;[m
[32m+[m
[32m+[m[32m    @media (max-device-width: $screen-xs-min) and (orientation: landscape) {[m
[32m+[m[32m      max-height: 200px;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Both navbar header and collapse[m
[32m+[m[32m//[m
[32m+[m[32m// When a container is present, change the behavior of the header and collapse.[m
[32m+[m
[32m+[m[32m.container,[m
[32m+[m[32m.container-fluid {[m
[32m+[m[32m  > .navbar-header,[m
[32m+[m[32m  > .navbar-collapse {[m
[32m+[m[32m    margin-right: -$navbar-padding-horizontal;[m
[32m+[m[32m    margin-left:  -$navbar-padding-horizontal;[m
[32m+[m
[32m+[m[32m    @media (min-width: $grid-float-breakpoint) {[m
[32m+[m[32m      margin-right: 0;[m
[32m+[m[32m      margin-left:  0;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Navbar alignment options[m
[32m+[m[32m//[m
[32m+[m[32m// Display the navbar across the entirety of the page or fixed it to the top or[m
[32m+[m[32m// bottom of the page.[m
[32m+[m
[32m+[m[32m// Static top (unfixed, but 100% wide) navbar[m
[32m+[m[32m.navbar-static-top {[m
[32m+[m[32m  z-index: $zindex-navbar;[m
[32m+[m[32m  border-width: 0 0 1px;[m
[32m+[m
[32m+[m[32m  @media (min-width: $grid-float-breakpoint) {[m
[32m+[m[32m    border-radius: 0;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Fix the top/bottom navbars when screen real estate supports it[m
[32m+[m[32m.navbar-fixed-top,[m
[32m+[m[32m.navbar-fixed-bottom {[m
[32m+[m[32m  position: fixed;[m
[32m+[m[32m  right: 0;[m
[32m+[m[32m  left: 0;[m
[32m+[m[32m  z-index: $zindex-navbar-fixed;[m
[32m+[m
[32m+[m[32m  // Undo the rounded corners[m
[32m+[m[32m  @media (min-width: $grid-float-breakpoint) {[m
[32m+[m[32m    border-radius: 0;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m.navbar-fixed-top {[m
[32m+[m[32m  top: 0;[m
[32m+[m[32m  border-width: 0 0 1px;[m
[32m+[m[32m}[m
[32m+[m[32m.navbar-fixed-bottom {[m
[32m+[m[32m  bottom: 5;[m
[32m+[m[32m  margin-bottom: 5; // override .navbar defaults[m
[32m+[m[32m  border-width: 1px 0 0;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Brand/project name[m
[32m+[m
[32m+[m[32m.navbar-brand {[m
[32m+[m[32m  float: left;[m
[32m+[m[32m  padding: $navbar-padding-vertical $navbar-padding-horizontal;[m
[32m+[m[32m  font-size: $font-size-large;[m
[32m+[m[32m  line-height: $line-height-computed;[m
[32m+[m[32m  height: $navbar-height;[m
[32m+[m[32m  color: #191970;[m
[32m+[m
[32m+[m[32m  &:hover,[m
[32m+[m[32m  &:focus {[m
[32m+[m[32m    text-decoration: none;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  > img {[m
[32m+[m[32m    display: block;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  @media (min-width: $grid-float-breakpoint) {[m
[32m+[m[32m    .navbar > .container &,[m
[32m+[m[32m    .navbar > .container-fluid & {[m
[32m+[m[32m      margin-left: -$navbar-padding-horizontal;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Navbar toggle[m
[32m+[m[32m//[m
[32m+[m[32m// Custom button for toggling the `.navbar-collapse`, powered by the collapse[m
[32m+[m[32m// JavaScript plugin.[m
[32m+[m
[32m+[m[32m.navbar-toggle {[m
[32m+[m[32m  position: relative;[m
[32m+[m[32m  float: right;[m
[32m+[m[32m  margin-right: $navbar-padding-horizontal;[m
[32m+[m[32m  padding: 9px 10px;[m
[32m+[m[32m  @include navbar-vertical-align(34px);[m
[32m+[m[32m  background-color: transparent;[m
[32m+[m[32m  background-image: none; // Reset unusual Firefox-on-Android default style; see https://github.com/necolas/normalize.css/issues/214[m
[32m+[m[32m  border: 1px solid transparent;[m
[32m+[m[32m  border-radius: $border-radius-base;[m
[32m+[m
[32m+[m[32m  // We remove the `outline` here, but later compensate by attaching `:hover`[m
[32m+[m[32m  // styles to `:focus`.[m
[32m+[m[32m  &:focus {[m
[32m+[m[32m    outline: 0;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Bars[m
[32m+[m[32m  .icon-bar {[m
[32m+[m[32m    display: block;[m
[32m+[m[32m    width: 22px;[m
[32m+[m[32m    height: 2px;[m
[32m+[m[32m    border-radius: 1px;[m
[32m+[m[32m  }[m
[32m+[m[32m  .icon-bar + .icon-bar {[m
[32m+[m[32m    margin-top: 4px;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  @media (min-width: $grid-float-breakpoint) {[m
[32m+[m[32m    display: none;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Navbar nav links[m
[32m+[m[32m//[m
[32m+[m[32m// Builds on top of the `.nav` components with its own modifier class to make[m
[32m+[m[32m// the nav the full height of the horizontal nav (above 768px).[m
[32m+[m
[32m+[m[32m.navbar-nav {[m
[32m+[m[32m  margin: ($navbar-padding-vertical / 2) (-$navbar-padding-horizontal);[m
[32m+[m
[32m+[m[32m  > li > a {[m
[32m+[m[32m    padding-top:    10px;[m
[32m+[m[32m    padding-bottom: 10px;[m
[32m+[m[32m    line-height: $line-height-computed;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  @media (max-width: $grid-float-breakpoint-max) {[m
[32m+[m[32m    // Dropdowns get custom display when collapsed[m
[32m+[m[32m    .open .dropdown-menu {[m
[32m+[m[32m      position: static;[m
[32m+[m[32m      float: none;[m
[32m+[m[32m      width: auto;[m
[32m+[m[32m      margin-top: 0;[m
[32m+[m[32m      background-color: transparent;[m
[32m+[m[32m      border: 0;[m
[32m+[m[32m      box-shadow: none;[m
[32m+[m[32m      > li > a,[m
[32m+[m[32m      .dropdown-header {[m
[32m+[m[32m        padding: 5px 15px 5px 25px;[m
[32m+[m[32m      }[m
[32m+[m[32m      > li > a {[m
[32m+[m[32m        line-height: $line-height-computed;[m
[32m+[m[32m        &:hover,[m
[32m+[m[32m        &:focus {[m
[32m+[m[32m          background-image: none;[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Uncollapse the nav[m
[32m+[m[32m  @media (min-width: $grid-float-breakpoint) {[m
[32m+[m[32m    float: left;[m
[32m+[m[32m    margin: 0;[m
[32m+[m
[32m+[m[32m    > li {[m
[32m+[m[32m      float: left;[m
[32m+[m[32m      > a {[m
[32m+[m[32m        padding-top:    $navbar-padding-vertical;[m
[32m+[m[32m        padding-bottom: $navbar-padding-vertical;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Navbar form[m
[32m+[m[32m//[m
[32m+[m[32m// Extension of the `.form-inline` with some extra flavor for optimum display in[m
[32m+[m[32m// our navbars.[m
[32m+[m
[32m+[m[32m.navbar-form {[m
[32m+[m[32m  margin-left: -$navbar-padding-horizontal;[m
[32m+[m[32m  margin-right: -$navbar-padding-horizontal;[m
[32m+[m[32m  padding: 10px $navbar-padding-horizontal;[m
[32m+[m[32m  border-top: 1px solid transparent;[m
[32m+[m[32m  border-bottom: 1px solid transparent;[m
[32m+[m[32m  $shadow: inset 0 1px 0 rgba(255,255,255,.1), 0 1px 0 rgba(255,255,255,.1);[m
[32m+[m[32m  @include box-shadow($shadow);[m
[32m+[m
[32m+[m[32m  // Mixin behavior for optimum display[m
[32m+[m[32m  @include form-inline;[m
[32m+[m
[32m+[m[32m  .form-group {[m
[32m+[m[32m    @media (max-width: $grid-float-breakpoint-max) {[m
[32m+[m[32m      margin-bottom: 5px;[m
[32m+[m
[32m+[m[32m      &:last-child {[m
[32m+[m[32m        margin-bottom: 0;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Vertically center in expanded, horizontal navbar[m
[32m+[m[32m  @include navbar-vertical-align($input-height-base);[m
[32m+[m
[32m+[m[32m  // Undo 100% width for pull classes[m
[32m+[m[32m  @media (min-width: $grid-float-breakpoint) {[m
[32m+[m[32m    width: auto;[m
[32m+[m[32m    border: 0;[m
[32m+[m[32m    margin-left: 0;[m
[32m+[m[32m    margin-right: 0;[m
[32m+[m[32m    padding-top: 0;[m
[32m+[m[32m    padding-bottom: 0;[m
[32m+[m[32m    @include box-shadow(none);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Dropdown menus[m
[32m+[m
[32m+[m[32m// Menu position and menu carets[m
[32m+[m[32m.navbar-nav > li > .dropdown-menu {[m
[32m+[m[32m  margin-top: 0;[m
[32m+[m[32m  @include border-top-radius(0);[m
[32m+[m[32m}[m
[32m+[m[32m// Menu position and menu caret support for dropups via extra dropup class[m
[32m+[m[32m.navbar-fixed-bottom .navbar-nav > li > .dropdown-menu {[m
[32m+[m[32m  margin-bottom: 0;[m
[32m+[m[32m  @include border-top-radius($navbar-border-radius);[m
[32m+[m[32m  @include border-bottom-radius(0);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Buttons in navbars[m
[32m+[m[32m//[m
[32m+[m[32m// Vertically center a button within a navbar (when *not* in a form).[m
[32m+[m
[32m+[m[32m.navbar-btn {[m
[32m+[m[32m  @include navbar-vertical-align($input-height-base);[m
[32m+[m
[32m+[m[32m  &.btn-sm {[m
[32m+[m[32m    @include navbar-vertical-align($input-height-small);[m
[32m+[m[32m  }[m
[32m+[m[32m  &.btn-xs {[m
[32m+[m[32m    @include navbar-vertical-align(22);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Text in navbars[m
[32m+[m[32m//[m
[32m+[m[32m// Add a class to make any element properly align itself vertically within the navbars.[m
[32m+[m
[32m+[m[32m.navbar-text {[m
[32m+[m[32m  @include navbar-vertical-align($line-height-computed);[m
[32m+[m
[32m+[m[32m  @media (min-width: $grid-float-breakpoint) {[m
[32m+[m[32m    float: left;[m
[32m+[m[32m    margin-left: $navbar-padding-horizontal;[m
[32m+[m[32m    margin-right: $navbar-padding-horizontal;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Component alignment[m
[32m+[m[32m//[m
[32m+[m[32m// Repurpose the pull utilities as their own navbar utilities to avoid specificity[m
[32m+[m[32m// issues with parents and chaining. Only do this when the navbar is uncollapsed[m
[32m+[m[32m// though so that navbar contents properly stack and align in mobile.[m
[32m+[m[32m//[m
[32m+[m[32m// Declared after the navbar components to ensure more specificity on the margins.[m
[32m+[m
[32m+[m[32m@media (min-width: $grid-float-breakpoint) {[m
[32m+[m[32m  .navbar-left {[m
[32m+[m[32m    float: left !important;[m
[32m+[m[32m  }[m
[32m+[m[32m  .navbar-right {[m
[32m+[m[32m    float: right !important;[m
[32m+[m[32m  margin-right: -$navbar-padding-horizontal;[m
[32m+[m
[32m+[m[32m    ~ .navbar-right {[m
[32m+[m[32m      margin-right: 0;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Alternate navbars[m
[32m+[m[32m// --------------------------------------------------[m
[32m+[m
[32m+[m[32m// Default navbar[m
[32m+[m[32m.navbar-default {[m
[32m+[m[32m  background-color: $navbar-default-bg;[m
[32m+[m[32m  border-color: $navbar-default-border;[m
[32m+[m
[32m+[m[32m  .navbar-brand {[m
[32m+[m[32m    color: $navbar-default-brand-color;[m
[32m+[m[32m    &:hover,[m
[32m+[m[32m    &:focus {[m
[32m+[m[32m      color: $navbar-default-brand-hover-color;[m
[32m+[m[32m      background-color: $navbar-default-brand-hover-bg;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  .navbar-text {[m
[32m+[m[32m    color: $navbar-default-color;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  .navbar-nav {[m
[32m+[m[32m    > li > a {[m
[32m+[m[32m      color: $navbar-default-link-color;[m
[32m+[m
[32m+[m[32m      &:hover,[m
[32m+[m[32m      &:focus {[m
[32m+[m[32m        color: $navbar-default-link-hover-color;[m
[32m+[m[32m        background-color: $navbar-default-link-hover-bg;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    > .active > a {[m
[32m+[m[32m      &,[m
[32m+[m[32m      &:hover,[m
[32m+[m[32m      &:focus {[m
[32m+[m[32m        color: $navbar-default-link-active-color;[m
[32m+[m[32m        background-color: $navbar-default-link-active-bg;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    > .disabled > a {[m
[32m+[m[32m      &,[m
[32m+[m[32m      &:hover,[m
[32m+[m[32m      &:focus {[m
[32m+[m[32m        color: $navbar-default-link-disabled-color;[m
[32m+[m[32m        background-color: $navbar-default-link-disabled-bg;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  .navbar-toggle {[m
[32m+[m[32m    border-color: $navbar-default-toggle-border-color;[m
[32m+[m[32m    &:hover,[m
[32m+[m[32m    &:focus {[m
[32m+[m[32m      background-color: $navbar-default-toggle-hover-bg;[m
[32m+[m[32m    }[m
[32m+[m[32m    .icon-bar {[m
[32m+[m[32m      background-color: $navbar-default-toggle-icon-bar-bg;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  .navbar-collapse,[m
[32m+[m[32m  .navbar-form {[m
[32m+[m[32m    border-color: $navbar-default-border;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Dropdown menu items[m
[32m+[m[32m  .navbar-nav {[m
[32m+[m[32m    // Remove background color from open dropdown[m
[32m+[m[32m    > .open > a {[m
[32m+[m[32m      &,[m
[32m+[m[32m      &:hover,[m
[32m+[m[32m      &:focus {[m
[32m+[m[32m        background-color: $navbar-default-link-active-bg;[m
[32m+[m[32m        color: $navbar-default-link-active-color;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @media (max-width: $grid-float-breakpoint-max) {[m
[32m+[m[32m      // Dropdowns get custom display when collapsed[m
[32m+[m[32m      .open .dropdown-menu {[m
[32m+[m[32m        > li > a {[m
[32m+[m[32m          color: $navbar-default-link-color;[m
[32m+[m[32m          &:hover,[m
[32m+[m[32m          &:focus {[m
[32m+[m[32m            color: $navbar-default-link-hover-color;[m
[32m+[m[32m            background-color: $navbar-default-link-hover-bg;[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m        > .active > a {[m
[32m+[m[32m          &,[m
[32m+[m[32m          &:hover,[m
[32m+[m[32m          &:focus {[m
[32m+[m[32m            color: $navbar-default-link-active-color;[m
[32m+[m[32m            background-color: $navbar-default-link-active-bg;[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m        > .disabled > a {[m
[32m+[m[32m          &,[m
[32m+[m[32m          &:hover,[m
[32m+[m[32m          &:focus {[m
[32m+[m[32m            color: $navbar-default-link-disabled-color;[m
[32m+[m[32m            background-color: $navbar-default-link-disabled-bg;[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m  // Links in navbars[m
[32m+[m[32m  //[m
[32m+[m[32m  // Add a class to ensure links outside the navbar nav are colored correctly.[m
[32m+[m
[32m+[m[32m  .navbar-link {[m
[32m+[m[32m    color: $navbar-default-link-color;[m
[32m+[m[32m    &:hover {[m
[32m+[m[32m      color: $navbar-default-link-hover-color;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  .btn-link {[m
[32m+[m[32m    color: $navbar-default-link-color;[m
[32m+[m[32m    &:hover,[m
[32m+[m[32m    &:focus {[m
[32m+[m[32m      color: $navbar-default-link-hover-color;[m
[32m+[m[32m    }[m
[32m+[m[32m    &[disabled],[m
[32m+[m[32m    fieldset[disabled] & {[m
[32m+[m[32m      &:hover,[m
[32m+[m[32m      &:focus {[m
[32m+[m[32m        color: $navbar-default-link-disabled-color;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Inverse navbar[m
[32m+[m
[32m+[m[32m.navbar-inverse {[m
[32m+[m[32m  background-color: $navbar-inverse-bg;[m
[32m+[m[32m  border-color: $navbar-inverse-border;[m
[32m+[m
[32m+[m[32m  .navbar-brand {[m
[32m+[m[32m    color: $navbar-inverse-brand-color;[m
[32m+[m[32m    &:hover,[m
[32m+[m[32m    &:focus {[m
[32m+[m[32m      color: $navbar-inverse-brand-hover-color;[m
[32m+[m[32m      background-color: $navbar-inverse-brand-hover-bg;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  .navbar-text {[m
[32m+[m[32m    color: $navbar-inverse-color;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  .navbar-nav {[m
[32m+[m[32m    > li > a {[m
[32m+[m[32m      color: $navbar-inverse-link-color;[m
[32m+[m
[32m+[m[32m      &:hover,[m
[32m+[m[32m      &:focus {[m
[32m+[m[32m        color: $navbar-inverse-link-hover-color;[m
[32m+[m[32m        background-color: $navbar-inverse-link-hover-bg;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    > .active > a {[m
[32m+[m[32m      &,[m
[32m+[m[32m      &:hover,[m
[32m+[m[32m      &:focus {[m
[32m+[m[32m        color: $navbar-inverse-link-active-color;[m
[32m+[m[32m        background-color: $navbar-inverse-link-active-bg;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    > .disabled > a {[m
[32m+[m[32m      &,[m
[32m+[m[32m      &:hover,[m
[32m+[m[32m      &:focus {[m
[32m+[m[32m        color: $navbar-inverse-link-disabled-color;[m
[32m+[m[32m        background-color: $navbar-inverse-link-disabled-bg;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Darken the responsive nav toggle[m
[32m+[m[32m  .navbar-toggle {[m
[32m+[m[32m    border-color: $navbar-inverse-toggle-border-color;[m
[32m+[m[32m    &:hover,[m
[32m+[m[32m    &:focus {[m
[32m+[m[32m      background-color: $navbar-inverse-toggle-hover-bg;[m
[32m+[m[32m    }[m
[32m+[m[32m    .icon-bar {[m
[32m+[m[32m      background-color: $navbar-inverse-toggle-icon-bar-bg;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  .navbar-collapse,[m
[32m+[m[32m  .navbar-form {[m
[32m+[m[32m    border-color: darken($navbar-inverse-bg, 7%);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Dropdowns[m
[32m+[m[32m  .navbar-nav {[m
[32m+[m[32m    > .open > a {[m
[32m+[m[32m      &,[m
[32m+[m[32m      &:hover,[m
[32m+[m[32m      &:focus {[m
[32m+[m[32m        background-color: $navbar-inverse-link-active-bg;[m
[32m+[m[32m        color: $navbar-inverse-link-active-color;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @media (max-width: $grid-float-breakpoint-max) {[m
[32m+[m[32m      // Dropdowns get custom display[m
[32m+[m[32m      .open .dropdown-menu {[m
[32m+[m[32m        > .dropdown-header {[m
[32m+[m[32m          border-color: $navbar-inverse-border;[m
[32m+[m[32m        }[m
