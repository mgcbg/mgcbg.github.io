[32m+[m[32m      border: 1px solid transparent;[m
[32m+[m[32m      border-radius: $border-radius-base $border-radius-base 0 0;[m
[32m+[m[32m      &:hover {[m
[32m+[m[32m        border-color: $nav-tabs-link-hover-border-color $nav-tabs-link-hover-border-color $nav-tabs-border-color;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Active state, and its :hover to override normal :hover[m
[32m+[m[32m    &.active > a {[m
[32m+[m[32m      &,[m
[32m+[m[32m      &:hover,[m
[32m+[m[32m      &:focus {[m
[32m+[m[32m        color: $nav-tabs-active-link-hover-color;[m
[32m+[m[32m        background-color: $nav-tabs-active-link-hover-bg;[m
[32m+[m[32m        border: 1px solid $nav-tabs-active-link-hover-border-color;[m
[32m+[m[32m        border-bottom-color: transparent;[m
[32m+[m[32m        cursor: default;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  // pulling this in mainly for less shorthand[m
[32m+[m[32m  &.nav-justified {[m
[32m+[m[32m    @extend .nav-justified;[m
[32m+[m[32m    @extend .nav-tabs-justified;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
