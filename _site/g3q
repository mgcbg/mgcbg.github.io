[32m+[m[32m    > .active > a:hover,[m
[32m+[m[32m    > .active > a:focus {[m
[32m+[m[32m      border-bottom-color: $nav-tabs-justified-active-link-border-color;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
