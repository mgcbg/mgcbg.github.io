[1mnew file mode 100644[m
[1mindex 0000000..07a0fc9[m
[1m--- /dev/null[m
[1m+++ b/_sass/bootstrap/_glyphicons.scss[m
[36m@@ -0,0 +1,307 @@[m
[32m+[m[32m//[m
[32m+[m[32m// Glyphicons for Bootstrap[m
[32m+[m[32m//[m
[32m+[m[32m// Since icons are fonts, they can be placed anywhere text is placed and are[m
[32m+[m[32m// thus automatically sized to match the surrounding child. To use, create an[m
[32m+[m[32m// inline element with the appropriate classes, like so:[m
[32m+[m[32m//[m
[32m+[m[32m// <a href="#"><span class="glyphicon glyphicon-star"></span> Star</a>[m
[32m+[m
[32m+[m[32m@at-root {[m
[32m+[m[32m  // Import the fonts[m
[32m+[m[32m  @font-face {[m
[32m+[m[32m    font-family: 'Glyphicons Halflings';[m
[32m+[m[32m    src: url(if($bootstrap-sass-asset-helper, twbs-font-path('#{$icon-font-path}#{$icon-font-name}.eot'), '#{$icon-font-path}#{$icon-font-name}.eot'));[m
[32m+[m[32m    src: url(if($bootstrap-sass-asset-helper, twbs-font-path('#{$icon-font-path}#{$icon-font-name}.eot?#iefix'), '#{$icon-font-path}#{$icon-font-name}.eot?#iefix')) format('embedded-opentype'),[m
[32m+[m[32m         url(if($bootstrap-sass-asset-helper, twbs-font-path('#{$icon-font-path}#{$icon-font-name}.woff2'), '#{$icon-font-path}#{$icon-font-name}.woff2')) format('woff2'),[m
[32m+[m[32m         url(if($bootstrap-sass-asset-helper, twbs-font-path('#{$icon-font-path}#{$icon-font-name}.woff'), '#{$icon-font-path}#{$icon-font-name}.woff')) format('woff'),[m
[32m+[m[32m         url(if($bootstrap-sass-asset-helper, twbs-font-path('#{$icon-font-path}#{$icon-font-name}.ttf'), '#{$icon-font-path}#{$icon-font-name}.ttf')) format('truetype'),[m
[32m+[m[32m         url(if($bootstrap-sass-asset-helper, twbs-font-path('#{$icon-font-path}#{$icon-font-name}.svg##{$icon-font-svg-id}'), '#{$icon-font-path}#{$icon-font-name}.svg##{$icon-font-svg-id}')) format('svg');[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Catchall baseclass[m
[32m+[m[32m.glyphicon {[m
[32m+[m[32m  position: relative;[m
[32m+[m[32m  top: 1px;[m
[32m+[m[32m  display: inline-block;[m
[32m+[m[32m  font-family: 'Glyphicons Halflings';[m
[32m+[m[32m  font-style: normal;[m
[32m+[m[32m  font-weight: normal;[m
[32m+[m[32m  line-height: 1;[m
[32m+[m[32m  -webkit-font-smoothing: antialiased;[m
[32m+[m[32m  -moz-osx-font-smoothing: grayscale;[m
[32m+[m[32m}[m
