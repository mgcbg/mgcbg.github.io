[32m+[m[32m//[m
[32m+[m[32m// Tooltips[m
[32m+[m[32m// --------------------------------------------------[m
[32m+[m
[32m+[m
[32m+[m[32m// Base class[m
[32m+[m[32m.tooltip {[m
[32m+[m[32m  position: absolute;[m
[32m+[m[32m  z-index: $zindex-tooltip;[m
[32m+[m[32m  display: block;[m
[32m+[m[32m  // Our parent element can be arbitrary since tooltips are by default inserted as a sibling of their target element.[m
[32m+[m[32m  // So reset our font and text properties to avoid inheriting weird values.[m
[32m+[m[32m  @include reset-text;[m
[32m+[m[32m  font-size: $font-size-small;[m
[32m+[m
[32m+[m[32m  @include opacity(0);[m
[32m+[m
[32m+[m[32m  &.in     { @include opacity($tooltip-opacity); }[m
[32m+[m[32m  &.top    { margin-top:  -3px; padding: $tooltip-arrow-width 0; }[m
[32m+[m[32m  &.right  { margin-left:  3px; padding: 0 $tooltip-arrow-width; }[m
[32m+[m[32m  &.bottom { margin-top:   3px; padding: $tooltip-arrow-width 0; }[m
[32m+[m[32m  &.left   { margin-left: -3px; padding: 0 $tooltip-arrow-width; }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Wrapper for the tooltip content[m
[32m+[m[32m.tooltip-inner {[m
[32m+[m[32m  max-width: $tooltip-max-width;[m
[32m+[m[32m  padding: 3px 8px;[m
[32m+[m[32m  color: $tooltip-color;[m
[32m+[m[32m  text-align: center;[m
[32m+[m[32m  background-color: $tooltip-bg;[m
[32m+[m[32m  border-radius: $border-radius-base;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Arrows[m
[32m+[m[32m.tooltip-arrow {[m
[32m+[m[32m  position: absolute;[m
[32m+[m[32m  width: 0;[m
[32m+[m[32m  height: 0;[m
[32m+[m[32m  border-color: transparent;[m
[32m+[m[32m  border-style: solid;[m
[32m+[m[32m}[m
[32m+[m[32m// Note: Deprecated .top-left, .top-right, .bottom-left, and .bottom-right as of v3.3.1[m
[32m+[m[32m.tooltip {[m
[32m+[m[32m  &.top .tooltip-arrow {[m
[32m+[m[32m    bottom: 0;[m
[32m+[m[32m    left: 50%;[m
[32m+[m[32m    margin-left: -$tooltip-arrow-width;[m
[32m+[m[32m    border-width: $tooltip-arrow-width $tooltip-arrow-width 0;[m
[32m+[m[32m    border-top-color: $tooltip-arrow-color;[m
[32m+[m[32m  }[m
[32m+[m[32m  &.top-left .tooltip-arrow {[m
[32m+[m[32m    bottom: 0;[m
[32m+[m[32m    right: $tooltip-arrow-width;[m
[32m+[m[32m    margin-bottom: -$tooltip-arrow-width;[m
[32m+[m[32m    border-width: $tooltip-arrow-width $tooltip-arrow-width 0;[m
[32m+[m[32m    border-top-color: $tooltip-arrow-color;[m
[32m+[m[32m  }[m
[32m+[m[32m  &.top-right .tooltip-arrow {[m
[32m+[m[32m    bottom: 0;[m
[32m+[m[32m    left: $tooltip-arrow-width;[m
[32m+[m[32m    margin-bottom: -$tooltip-arrow-width;[m
[32m+[m[32m    border-width: $tooltip-arrow-width $tooltip-arrow-width 0;[m
[32m+[m[32m    border-top-color: $tooltip-arrow-color;[m
[32m+[m[32m  }[m
[32m+[m[32m  &.right .tooltip-arrow {[m
[32m+[m[32m    top: 50%;[m
[32m+[m[32m    left: 0;[m
[32m+[m[32m    margin-top: -$tooltip-arrow-width;[m
[32m+[m[32m    border-width: $tooltip-arrow-width $tooltip-arrow-width $tooltip-arrow-width 0;[m
[32m+[m[32m    border-right-color: $tooltip-arrow-color;[m
[32m+[m[32m  }[m
[32m+[m[32m  &.left .tooltip-arrow {[m
[32m+[m[32m    top: 50%;[m
[32m+[m[32m    right: 0;[m
[32m+[m[32m    margin-top: -$tooltip-arrow-width;[m
[32m+[m[32m    border-width: $tooltip-arrow-width 0 $tooltip-arrow-width $tooltip-arrow-width;[m
[32m+[m[32m    border-left-color: $tooltip-arrow-color;[m
[32m+[m[32m  }[m
[32m+[m[32m  &.bottom .tooltip-arrow {[m
[32m+[m[32m    top: 0;[m
[32m+[m[32m    left: 50%;[m
[32m+[m[32m    margin-left: -$tooltip-arrow-width;[m
[32m+[m[32m    border-width: 0 $tooltip-arrow-width $tooltip-arrow-width;[m
[32m+[m[32m    border-bottom-color: $tooltip-arrow-color;[m
[32m+[m[32m  }[m
[32m+[m[32m  &.bottom-left .tooltip-arrow {[m
[32m+[m[32m    top: 0;[m
[32m+[m[32m    right: $tooltip-arrow-width;[m
[32m+[m[32m    margin-top: -$tooltip-arrow-width;[m
[32m+[m[32m    border-width: 0 $tooltip-arrow-width $tooltip-arrow-width;[m
[32m+[m[32m    border-bottom-color: $tooltip-arrow-color;[m
[32m+[m[32m  }[m
[32m+[m[32m  &.bottom-right .tooltip-arrow {[m
[32m+[m[32m    top: 0;[m
[32m+[m[32m    left: $tooltip-arrow-width;[m
[32m+[m[32m    margin-top: -$tooltip-arrow-width;[m
[32m+[m[32m    border-width: 0 $tooltip-arrow-width $tooltip-arrow-width;[m
[32m+[m[32m    border-bottom-color: $tooltip-arrow-color;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
