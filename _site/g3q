[32m+[m[32m/*! normalize.css v3.0.3 | MIT License | github.com/necolas/normalize.css */[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// 1. Set default font family to sans-serif.[m
[32m+[m[32m// 2. Prevent iOS and IE text size adjust after device orientation change,[m
[32m+[m[32m//    without disabling user zoom.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32mhtml {[m
[32m+[m[32m  font-family: sans-serif; // 1[m
[32m+[m[32m  -ms-text-size-adjust: 100%; // 2[m
[32m+[m[32m  -webkit-text-size-adjust: 100%; // 2[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Remove default margin.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32mbody {[m
[32m+[m[32m  margin: 0;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// HTML5 display definitions[m
[32m+[m[32m// ==========================================================================[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Correct `block` display not defined for any HTML5 element in IE 8/9.[m
[32m+[m[32m// Correct `block` display not defined for `details` or `summary` in IE 10/11[m
[32m+[m[32m// and Firefox.[m
[32m+[m[32m// Correct `block` display not defined for `main` in IE 11.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32marticle,[m
[32m+[m[32maside,[m
[32m+[m[32mdetails,[m
[32m+[m[32mfigcaption,[m
[32m+[m[32mfigure,[m
[32m+[m[32mfooter,[m
[32m+[m[32mheader,[m
[32m+[m[32mhgroup,[m
[32m+[m[32mmain,[m
[32m+[m[32mmenu,[m
[32m+[m[32mnav,[m
[32m+[m[32msection,[m
[32m+[m[32msummary {[m
[32m+[m[32m  display: block;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// 1. Correct `inline-block` display not defined in IE 8/9.[m
[32m+[m[32m// 2. Normalize vertical alignment of `progress` in Chrome, Firefox, and Opera.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32maudio,[m
[32m+[m[32mcanvas,[m
[32m+[m[32mprogress,[m
[32m+[m[32mvideo {[m
[32m+[m[32m  display: inline-block; // 1[m
[32m+[m[32m  vertical-align: baseline; // 2[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Prevent modern browsers from displaying `audio` without controls.[m
[32m+[m[32m// Remove excess height in iOS 5 devices.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32maudio:not([controls]) {[m
[32m+[m[32m  display: none;[m
[32m+[m[32m  height: 0;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Address `[hidden]` styling not present in IE 8/9/10.[m
[32m+[m[32m// Hide the `template` element in IE 8/9/10/11, Safari, and Firefox < 22.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32m[hidden],[m
[32m+[m[32mtemplate {[m
[32m+[m[32m  display: none;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Links[m
[32m+[m[32m// ==========================================================================[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Remove the gray background color from active links in IE 10.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32ma {[m
[32m+[m[32m  background-color: transparent;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Improve readability of focused elements when they are also in an[m
[32m+[m[32m// active/hover state.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32ma:active,[m
[32m+[m[32ma:hover {[m
[32m+[m[32m  outline: 0;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Text-level semantics[m
[32m+[m[32m// ==========================================================================[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Address styling not present in IE 8/9/10/11, Safari, and Chrome.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32mabbr[title] {[m
[32m+[m[32m  border-bottom: 1px dotted;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Address style set to `bolder` in Firefox 4+, Safari, and Chrome.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32mb,[m
[32m+[m[32mstrong {[m
[32m+[m[32m  font-weight: bold;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Address styling not present in Safari and Chrome.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32mdfn {[m
[32m+[m[32m  font-style: italic;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Address variable `h1` font-size and margin within `section` and `article`[m
[32m+[m[32m// contexts in Firefox 4+, Safari, and Chrome.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32mh1 {[m
[32m+[m[32m  font-size: 2em;[m
[32m+[m[32m  margin: 0.67em 0;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Address styling not present in IE 8/9.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32mmark {[m
[32m+[m[32m  background: #ff0;[m
[32m+[m[32m  color: #000;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Address inconsistent and variable font size in all browsers.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32msmall {[m
[32m+[m[32m  font-size: 80%;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Prevent `sub` and `sup` affecting `line-height` in all browsers.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32msub,[m
[32m+[m[32msup {[m
[32m+[m[32m  font-size: 75%;[m
[32m+[m[32m  line-height: 0;[m
[32m+[m[32m  position: relative;[m
[32m+[m[32m  vertical-align: baseline;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32msup {[m
[32m+[m[32m  top: -0.5em;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32msub {[m
[32m+[m[32m  bottom: -0.25em;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Embedded content[m
[32m+[m[32m// ==========================================================================[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Remove border when inside `a` element in IE 8/9/10.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32mimg {[m
[32m+[m[32m  border: 0;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Correct overflow not hidden in IE 9/10/11.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32msvg:not(:root) {[m
[32m+[m[32m  overflow: hidden;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Grouping content[m
[32m+[m[32m// ==========================================================================[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Address margin not present in IE 8/9 and Safari.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32mfigure {[m
[32m+[m[32m  margin: 1em 40px;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Address differences between Firefox and other browsers.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32mhr {[m
[32m+[m[32m  box-sizing: content-box;[m
[32m+[m[32m  height: 0;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Contain overflow in all browsers.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32mpre {[m
[32m+[m[32m  overflow: auto;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Address odd `em`-unit font size rendering in all browsers.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32mcode,[m
[32m+[m[32mkbd,[m
[32m+[m[32mpre,[m
[32m+[m[32msamp {[m
[32m+[m[32m  font-family: monospace, monospace;[m
[32m+[m[32m  font-size: 1em;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Forms[m
[32m+[m[32m// ==========================================================================[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Known limitation: by default, Chrome and Safari on OS X allow very limited[m
[32m+[m[32m// styling of `select`, unless a `border` property is set.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// 1. Correct color not being inherited.[m
[32m+[m[32m//    Known issue: affects color of disabled elements.[m
[32m+[m[32m// 2. Correct font properties not being inherited.[m
[32m+[m[32m// 3. Address margins set differently in Firefox 4+, Safari, and Chrome.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32mbutton,[m
[32m+[m[32minput,[m
[32m+[m[32moptgroup,[m
[32m+[m[32mselect,[m
[32m+[m[32mtextarea {[m
[32m+[m[32m  color: inherit; // 1[m
[32m+[m[32m  font: inherit; // 2[m
[32m+[m[32m  margin: 0; // 3[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Address `overflow` set to `hidden` in IE 8/9/10/11.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32mbutton {[m
[32m+[m[32m  overflow: visible;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Address inconsistent `text-transform` inheritance for `button` and `select`.[m
[32m+[m[32m// All other form control elements do not inherit `text-transform` values.[m
[32m+[m[32m// Correct `button` style inheritance in Firefox, IE 8/9/10/11, and Opera.[m
[32m+[m[32m// Correct `select` style inheritance in Firefox.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32mbutton,[m
[32m+[m[32mselect {[m
[32m+[m[32m  text-transform: none;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// 1. Avoid the WebKit bug in Android 4.0.* where (2) destroys native `audio`[m
[32m+[m[32m//    and `video` controls.[m
[32m+[m[32m// 2. Correct inability to style clickable `input` types in iOS.[m
[32m+[m[32m// 3. Improve usability and consistency of cursor style between image-type[m
[32m+[m[32m//    `input` and others.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32mbutton,[m
[32m+[m[32mhtml input[type="button"], // 1[m
[32m+[m[32minput[type="reset"],[m
[32m+[m[32minput[type="submit"] {[m
[32m+[m[32m  -webkit-appearance: button; // 2[m
[32m+[m[32m  cursor: pointer; // 3[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Re-set default cursor for disabled elements.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32mbutton[disabled],[m
[32m+[m[32mhtml input[disabled] {[m
[32m+[m[32m  cursor: default;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Remove inner padding and border in Firefox 4+.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32mbutton::-moz-focus-inner,[m
[32m+[m[32minput::-moz-focus-inner {[m
[32m+[m[32m  border: 0;[m
[32m+[m[32m  padding: 0;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Address Firefox 4+ setting `line-height` on `input` using `!important` in[m
[32m+[m[32m// the UA stylesheet.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32minput {[m
[32m+[m[32m  line-height: normal;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// It's recommended that you don't attempt to style these elements.[m
[32m+[m[32m// Firefox's implementation doesn't respect box-sizing, padding, or width.[m
[32m+[m[32m//[m
[32m+[m[32m// 1. Address box sizing set to `content-box` in IE 8/9/10.[m
[32m+[m[32m// 2. Remove excess padding in IE 8/9/10.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32minput[type="checkbox"],[m
[32m+[m[32minput[type="radio"] {[m
[32m+[m[32m  box-sizing: border-box; // 1[m
[32m+[m[32m  padding: 0; // 2[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Fix the cursor style for Chrome's increment/decrement buttons. For certain[m
[32m+[m[32m// `font-size` values of the `input`, it causes the cursor style of the[m
[32m+[m[32m// decrement button to change from `default` to `text`.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32minput[type="number"]::-webkit-inner-spin-button,[m
[32m+[m[32minput[type="number"]::-webkit-outer-spin-button {[m
[32m+[m[32m  height: auto;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// 1. Address `appearance` set to `searchfield` in Safari and Chrome.[m
[32m+[m[32m// 2. Address `box-sizing` set to `border-box` in Safari and Chrome.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32minput[type="search"] {[m
[32m+[m[32m  -webkit-appearance: textfield; // 1[m
[32m+[m[32m  box-sizing: content-box; //2[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Remove inner padding and search cancel button in Safari and Chrome on OS X.[m
[32m+[m[32m// Safari (but not Chrome) clips the cancel button when the search input has[m
[32m+[m[32m// padding (and `textfield` appearance).[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32minput[type="search"]::-webkit-search-cancel-button,[m
[32m+[m[32minput[type="search"]::-webkit-search-decoration {[m
[32m+[m[32m  -webkit-appearance: none;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Define consistent border, margin, and padding.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32mfieldset {[m
[32m+[m[32m  border: 1px solid #c0c0c0;[m
[32m+[m[32m  margin: 0 2px;[m
[32m+[m[32m  padding: 0.35em 0.625em 0.75em;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// 1. Correct `color` not being inherited in IE 8/9/10/11.[m
[32m+[m[32m// 2. Remove padding so people aren't caught out if they zero out fieldsets.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32mlegend {[m
[32m+[m[32m  border: 0; // 1[m
[32m+[m[32m  padding: 0; // 2[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Remove default vertical scrollbar in IE 8/9/10/11.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32mtextarea {[m
[32m+[m[32m  overflow: auto;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Don't inherit the `font-weight` (applied by a rule above).[m
[32m+[m[32m// NOTE: the default cannot safely be changed in Chrome and Safari on OS X.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32moptgroup {[m
[32m+[m[32m  font-weight: bold;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Tables[m
[32m+[m[32m// ==========================================================================[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Remove most spacing between table cells.[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32mtable {[m
[32m+[m[32m  border-collapse: collapse;[m
[32m+[m[32m  border-spacing: 0;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mtd,[m
