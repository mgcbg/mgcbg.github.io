[32m+[m[32m@mixin list-unstyled {[m
[32m+[m[32m  padding-left: 0;[m
[32m+[m[32m  list-style: none;[m
[32m+[m[32m}[m
[32m+[m[32m// [converter] extracted as `@mixin list-unstyled` for libsass compatibility[m
[32m+[m[32m.list-unstyled {[m
[32m+[m[32m  @include list-unstyled;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Inline turns list items into inline-block[m
[32m+[m[32m.list-inline {[m
[32m+[m[32m  @include list-unstyled;[m
[32m+[m[32m  margin-left: -5px;[m
[32m+[m
[32m+[m[32m  > li {[m
[32m+[m[32m    display: inline-block;[m
[32m+[m[32m    padding-left: 5px;[m
[32m+[m[32m    padding-right: 5px;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Description Lists[m
[32m+[m[32mdl {[m
[32m+[m[32m  margin-top: 0; // Remove browser default[m
[32m+[m[32m  margin-bottom: $line-height-computed;[m
[32m+[m[32m}[m
[32m+[m[32mdt,[m
[32m+[m[32mdd {[m
[32m+[m[32m  line-height: $line-height-base;[m
[32m+[m[32m}[m
[32m+[m[32mdt {[m
[32m+[m[32m  font-weight: bold;[m
[32m+[m[32m}[m
[32m+[m[32mdd {[m
[32m+[m[32m  margin-left: 0; // Undo browser default[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Horizontal description lists[m
[32m+[m[32m//[m
[32m+[m[32m// Defaults to being stacked without any of the below styles applied, until the[m
[32m+[m[32m// grid breakpoint is reached (default of ~768px).[m
[32m+[m
[32m+[m[32m.dl-horizontal {[m
[32m+[m[32m  dd {[m
[32m+[m[32m    @include clearfix; // Clear the floated `dt` if an empty `dd` is present[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  @media (min-width: $dl-horizontal-breakpoint) {[m
[32m+[m[32m    dt {[m
[32m+[m[32m      float: left;[m
[32m+[m[32m      width: ($dl-horizontal-offset - 20);[m
[32m+[m[32m      clear: left;[m
[32m+[m[32m      text-align: right;[m
[32m+[m[32m      @include text-overflow;[m
[32m+[m[32m    }[m
[32m+[m[32m    dd {[m
[32m+[m[32m      margin-left: $dl-horizontal-offset;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
