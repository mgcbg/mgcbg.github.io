[32m+[m[32m          background-color: $navbar-inverse-border;[m
[32m+[m[32m        }[m
[32m+[m[32m        > li > a {[m
[32m+[m[32m          color: $navbar-inverse-link-color;[m
[32m+[m[32m          &:hover,[m
[32m+[m[32m          &:focus {[m
[32m+[m[32m            color: $navbar-inverse-link-hover-color;[m
[32m+[m[32m            background-color: $navbar-inverse-link-hover-bg;[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m        > .active > a {[m
[32m+[m[32m          &,[m
[32m+[m[32m          &:hover,[m
[32m+[m[32m          &:focus {[m
