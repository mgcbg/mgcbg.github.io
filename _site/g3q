[32m+[m[32m    > li {[m
[32m+[m[32m      display: table-cell;[m
[32m+[m[32m      width: 1%;[m
[32m+[m[32m      > a {[m
[32m+[m[32m        margin-bottom: 0;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
