[32m+[m[32m//[m
[32m+[m[32m// Dropdown menus[m
[32m+[m[32m// --------------------------------------------------[m
[32m+[m
[32m+[m
[32m+[m[32m// Dropdown arrow/caret[m
[32m+[m[32m.caret {[m
[32m+[m[32m  display: inline-block;[m
[32m+[m[32m  width: 0;[m
[32m+[m[32m  height: 0;[m
[32m+[m[32m  margin-left: 2px;[m
[32m+[m[32m  vertical-align: middle;[m
[32m+[m[32m  border-top:   $caret-width-base dashed;[m
[32m+[m[32m  border-top:   $caret-width-base solid \9; // IE8[m
[32m+[m[32m  border-right: $caret-width-base solid transparent;[m
[32m+[m[32m  border-left:  $caret-width-base solid transparent;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// The dropdown wrapper (div)[m
[32m+[m[32m.dropup,[m
[32m+[m[32m.dropdown {[m
[32m+[m[32m  position: relative;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Prevent the focus on the dropdown toggle when closing dropdowns[m
[32m+[m[32m.dropdown-toggle:focus {[m
[32m+[m[32m  outline: 0;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// The dropdown menu (ul)[m
[32m+[m[32m.dropdown-menu {[m
[32m+[m[32m  position: absolute;[m
[32m+[m[32m  top: 100%;[m
[32m+[m[32m  left: 0;[m
[32m+[m[32m  z-index: $zindex-dropdown;[m
[32m+[m[32m  display: none; // none by default, but block on "open" of the menu[m
[32m+[m[32m  float: left;[m
[32m+[m[32m  min-width: 160px;[m
[32m+[m[32m  padding: 5px 0;[m
[32m+[m[32m  margin: 2px 0 0; // override default ul[m
[32m+[m[32m  list-style: none;[m
[32m+[m[32m  font-size: $font-size-base;[m
[32m+[m[32m  text-align: left; // Ensures proper alignment if parent has it changed (e.g., modal footer)[m
[32m+[m[32m  background-color: $dropdown-bg;[m
[32m+[m[32m  border: 1px solid $dropdown-fallback-border; // IE8 fallback[m
[32m+[m[32m  border: 1px solid $dropdown-border;[m
[32m+[m[32m  border-radius: $border-radius-base;[m
[32m+[m[32m  @include box-shadow(0 6px 12px rgba(0,0,0,.175));[m
[32m+[m[32m  background-clip: padding-box;[m
[32m+[m
[32m+[m[32m  // Aligns the dropdown menu to right[m
[32m+[m[32m  //[m
[32m+[m[32m  // Deprecated as of 3.1.0 in favor of `.dropdown-menu-[dir]`[m
[32m+[m[32m  &.pull-right {[m
[32m+[m[32m    right: 0;[m
[32m+[m[32m    left: auto;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Dividers (basically an hr) within the dropdown[m
[32m+[m[32m  .divider {[m
[32m+[m[32m    @include nav-divider($dropdown-divider-bg);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Links within the dropdown menu[m
[32m+[m[32m  > li > a {[m
[32m+[m[32m    display: block;[m
[32m+[m[32m    padding: 3px 20px;[m
[32m+[m[32m    clear: both;[m
[32m+[m[32m    font-weight: normal;[m
[32m+[m[32m    line-height: $line-height-base;[m
[32m+[m[32m    color: $dropdown-link-color;[m
[32m+[m[32m    white-space: nowrap; // prevent links from randomly breaking onto new lines[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Hover/Focus state[m
[32m+[m[32m.dropdown-menu > li > a {[m
[32m+[m[32m  &:hover,[m
[32m+[m[32m  &:focus {[m
[32m+[m[32m    text-decoration: none;[m
[32m+[m[32m    color: $dropdown-link-hover-color;[m
[32m+[m[32m    background-color: $dropdown-link-hover-bg;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Active state[m
[32m+[m[32m.dropdown-menu > .active > a {[m
[32m+[m[32m  &,[m
[32m+[m[32m  &:hover,[m
[32m+[m[32m  &:focus {[m
[32m+[m[32m    color: $dropdown-link-active-color;[m
[32m+[m[32m    text-decoration: none;[m
[32m+[m[32m    outline: 0;[m
[32m+[m[32m    background-color: $dropdown-link-active-bg;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Disabled state[m
[32m+[m[32m//[m
[32m+[m[32m// Gray out text and ensure the hover/focus state remains gray[m
[32m+[m
[32m+[m[32m.dropdown-menu > .disabled > a {[m
[32m+[m[32m  &,[m
[32m+[m[32m  &:hover,[m
[32m+[m[32m  &:focus {[m
[32m+[m[32m    color: $dropdown-link-disabled-color;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Nuke hover/focus effects[m
[32m+[m[32m  &:hover,[m
[32m+[m[32m  &:focus {[m
[32m+[m[32m    text-decoration: none;[m
[32m+[m[32m    background-color: transparent;[m
[32m+[m[32m    background-image: none; // Remove CSS gradient[m
[32m+[m[32m    @include reset-filter;[m
[32m+[m[32m    cursor: $cursor-disabled;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Open state for the dropdown[m
[32m+[m[32m.open {[m
[32m+[m[32m  // Show the menu[m
[32m+[m[32m  > .dropdown-menu {[m
[32m+[m[32m    display: block;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Remove the outline when :focus is triggered[m
[32m+[m[32m  > a {[m
[32m+[m[32m    outline: 0;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Menu positioning[m
[32m+[m[32m//[m
[32m+[m[32m// Add extra class to `.dropdown-menu` to flip the alignment of the dropdown[m
[32m+[m[32m// menu with the parent.[m
[32m+[m[32m.dropdown-menu-right {[m
[32m+[m[32m  left: auto; // Reset the default from `.dropdown-menu`[m
[32m+[m[32m  right: 0;[m
[32m+[m[32m}[m
[32m+[m[32m// With v3, we enabled auto-flipping if you have a dropdown within a right[m
[32m+[m[32m// aligned nav component. To enable the undoing of that, we provide an override[m
[32m+[m[32m// to restore the default dropdown menu alignment.[m
[32m+[m[32m//[m
[32m+[m[32m// This is only for left-aligning a dropdown menu within a `.navbar-right` or[m
[32m+[m[32m// `.pull-right` nav component.[m
[32m+[m[32m.dropdown-menu-left {[m
[32m+[m[32m  left: 0;[m
[32m+[m[32m  right: auto;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Dropdown section headers[m
[32m+[m[32m.dropdown-header {[m
[32m+[m[32m  display: block;[m
[32m+[m[32m  padding: 3px 20px;[m
[32m+[m[32m  font-size: $font-size-small;[m
[32m+[m[32m  line-height: $line-height-base;[m
[32m+[m[32m  color: $dropdown-header-color;[m
[32m+[m[32m  white-space: nowrap; // as with > li > a[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Backdrop to catch body clicks on mobile, etc.[m
[32m+[m[32m.dropdown-backdrop {[m
[32m+[m[32m  position: fixed;[m
[32m+[m[32m  left: 0;[m
[32m+[m[32m  right: 0;[m
[32m+[m[32m  bottom: 0;[m
[32m+[m[32m  top: 0;[m
[32m+[m[32m  z-index: ($zindex-dropdown - 10);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Right aligned dropdowns[m
[32m+[m[32m.pull-right > .dropdown-menu {[m
[32m+[m[32m  right: 0;[m
[32m+[m[32m  left: auto;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Allow for dropdowns to go bottom up (aka, dropup-menu)[m
[32m+[m[32m//[m
[32m+[m[32m// Just add .dropup after the standard .dropdown class and you're set, bro.[m
[32m+[m[32m// TODO: abstract this so that the navbar fixed styles are not placed here?[m
[32m+[m
[32m+[m[32m.dropup,[m
[32m+[m[32m.navbar-fixed-bottom .dropdown {[m
[32m+[m[32m  // Reverse the caret[m
[32m+[m[32m  .caret {[m
[32m+[m[32m    border-top: 0;[m
[32m+[m[32m    border-bottom: $caret-width-base dashed;[m
[32m+[m[32m    border-bottom: $caret-width-base solid \9; // IE8[m
[32m+[m[32m    content: "";[m
[32m+[m[32m  }[m
[32m+[m[32m  // Different positioning for bottom up menu[m
[32m+[m[32m  .dropdown-menu {[m
[32m+[m[32m    top: auto;[m
[32m+[m[32m    bottom: 100%;[m
[32m+[m[32m    margin-bottom: 2px;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Component alignment[m
[32m+[m[32m//[m
[32m+[m[32m// Reiterate per navbar.less and the modified component alignment there.[m
[32m+[m
[32m+[m[32m@media (min-width: $grid-float-breakpoint) {[m
[32m+[m[32m  .navbar-right {[m
[32m+[m[32m    .dropdown-menu {[m
[32m+[m[32m      right: 0; left: auto;[m
[32m+[m[32m    }[m
[32m+[m[32m    // Necessary for overrides of the default right aligned menu.[m
[32m+[m[32m    // Will remove come v4 in all likelihood.[m
[32m+[m[32m    .dropdown-menu-left {[m
[32m+[m[32m      left: 0; right: auto;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[1mdiff --git a/_sass/bootstrap/_forms.scss b/_sass/bootstrap/_forms.scss[m
[1mnew file mode 100644[m
[1mindex 0000000..ac26a6a[m
[1m--- /dev/null[m
[1m+++ b/_sass/bootstrap/_forms.scss[m
[36m@@ -0,0 +1,617 @@[m
[32m+[m[32m//[m
[32m+[m[32m// Forms[m
[32m+[m[32m// --------------------------------------------------[m
[32m+[m
[32m+[m
[32m+[m[32m// Normalize non-controls[m
[32m+[m[32m//[m
[32m+[m[32m// Restyle and baseline non-control form elements.[m
[32m+[m
[32m+[m[32mfieldset {[m
[32m+[m[32m  padding: 0;[m
[32m+[m[32m  margin: 0;[m
[32m+[m[32m  border: 0;[m
[32m+[m[32m  // Chrome and Firefox set a `min-width: min-content;` on fieldsets,[m
[32m+[m[32m  // so we reset that to ensure it behaves more like a standard block element.[m
[32m+[m[32m  // See https://github.com/twbs/bootstrap/issues/12359.[m
[32m+[m[32m  min-width: 0;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mlegend {[m
[32m+[m[32m  display: block;[m
[32m+[m[32m  width: 100%;[m
[32m+[m[32m  padding: 0;[m
[32m+[m[32m  margin-bottom: $line-height-computed;[m
[32m+[m[32m  font-size: ($font-size-base * 1.5);[m
[32m+[m[32m  line-height: inherit;[m
[32m+[m[32m  color: $legend-color;[m
[32m+[m[32m  border: 0;[m
[32m+[m[32m  border-bottom: 1px solid $legend-border-color;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mlabel {[m
[32m+[m[32m  display: inline-block;[m
[32m+[m[32m  max-width: 100%; // Force IE8 to wrap long content (see https://github.com/twbs/bootstrap/issues/13141)[m
[32m+[m[32m  margin-bottom: 5px;[m
[32m+[m[32m  font-weight: bold;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Normalize form controls[m
[32m+[m[32m//[m
[32m+[m[32m// While most of our form styles require extra classes, some basic normalization[m
[32m+[m[32m// is required to ensure optimum display with or without those classes to better[m
[32m+[m[32m// address browser inconsistencies.[m
[32m+[m
[32m+[m[32m// Override content-box in Normalize (* isn't specific enough)[m
[32m+[m[32minput[type="search"] {[m
[32m+[m[32m  @include box-sizing(border-box);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Position radios and checkboxes better[m
[32m+[m[32minput[type="radio"],[m
[32m+[m[32minput[type="checkbox"] {[m
[32m+[m[32m  margin: 4px 0 0;[m
[32m+[m[32m  margin-top: 1px \9; // IE8-9[m
[32m+[m[32m  line-height: normal;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32minput[type="file"] {[m
[32m+[m[32m  display: block;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Make range inputs behave like textual form controls[m
[32m+[m[32minput[type="range"] {[m
[32m+[m[32m  display: block;[m
[32m+[m[32m  width: 100%;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Make multiple select elements height not fixed[m
[32m+[m[32mselect[multiple],[m
[32m+[m[32mselect[size] {[m
[32m+[m[32m  height: auto;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Focus for file, radio, and checkbox[m
[32m+[m[32minput[type="file"]:focus,[m
[32m+[m[32minput[type="radio"]:focus,[m
[32m+[m[32minput[type="checkbox"]:focus {[m
[32m+[m[32m  @include tab-focus;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Adjust output element[m
[32m+[m[32moutput {[m
[32m+[m[32m  display: block;[m
[32m+[m[32m  padding-top: ($padding-base-vertical + 1);[m
[32m+[m[32m  font-size: $font-size-base;[m
[32m+[m[32m  line-height: $line-height-base;[m
[32m+[m[32m  color: $input-color;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Common form controls[m
[32m+[m[32m//[m
[32m+[m[32m// Shared size and type resets for form controls. Apply `.form-control` to any[m
[32m+[m[32m// of the following form controls:[m
[32m+[m[32m//[m
[32m+[m[32m// select[m
[32m+[m[32m// textarea[m
[32m+[m[32m// input[type="text"][m
[32m+[m[32m// input[type="password"][m
[32m+[m[32m// input[type="datetime"][m
[32m+[m[32m// input[type="datetime-local"][m
[32m+[m[32m// input[type="date"][m
[32m+[m[32m// input[type="month"][m
[32m+[m[32m// input[type="time"][m
[32m+[m[32m// input[type="week"][m
[32m+[m[32m// input[type="number"][m
[32m+[m[32m// input[type="email"][m
[32m+[m[32m// input[type="url"][m
[32m+[m[32m// input[type="search"][m
[32m+[m[32m// input[type="tel"][m
[32m+[m[32m// input[type="color"][m
[32m+[m
[32m+[m[32m.form-control {[m
[32m+[m[32m  display: block;[m
[32m+[m[32m  width: 100%;[m
[32m+[m[32m  height: $input-height-base; // Make inputs at least the height of their button counterpart (base line-height + padding + border)[m
[32m+[m[32m  padding: $padding-base-vertical $padding-base-horizontal;[m
[32m+[m[32m  font-size: $font-size-base;[m
[32m+[m[32m  line-height: $line-height-base;[m
[32m+[m[32m  color: $input-color;[m
[32m+[m[32m  background-color: $input-bg;[m
[32m+[m[32m  background-image: none; // Reset unusual Firefox-on-Android default style; see https://github.com/necolas/normalize.css/issues/214[m
[32m+[m[32m  border: 1px solid $input-border;[m
[32m+[m[32m  border-radius: $input-border-radius; // Note: This has no effect on <select>s in some browsers, due to the limited stylability of <select>s in CSS.[m
[32m+[m[32m  @include box-shadow(inset 0 1px 1px rgba(0,0,0,.075));[m
[32m+[m[32m  @include transition(border-color ease-in-out .15s, box-shadow ease-in-out .15s);[m
[32m+[m
[32m+[m[32m  // Customize the `:focus` state to imitate native WebKit styles.[m
[32m+[m[32m  @include form-control-focus;[m
[32m+[m
[32m+[m[32m  // Placeholder[m
[32m+[m[32m  @include placeholder;[m
[32m+[m
[32m+[m[32m  // Unstyle the caret on `<select>`s in IE10+.[m
[32m+[m[32m  &::-ms-expand {[m
[32m+[m[32m    border: 0;[m
[32m+[m[32m    background-color: transparent;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Disabled and read-only inputs[m
[32m+[m[32m  //[m
[32m+[m[32m  // HTML5 says that controls under a fieldset > legend:first-child won't be[m
[32m+[m[32m  // disabled if the fieldset is disabled. Due to implementation difficulty, we[m
[32m+[m[32m  // don't honor that edge case; we style them as disabled anyway.[m
[32m+[m[32m  &[disabled],[m
[32m+[m[32m  &[readonly],[m
[32m+[m[32m  fieldset[disabled] & {[m
[32m+[m[32m    background-color: $input-bg-disabled;[m
[32m+[m[32m    opacity: 1; // iOS fix for unreadable disabled content; see https://github.com/twbs/bootstrap/issues/11655[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  &[disabled],[m
[32m+[m[32m  fieldset[disabled] & {[m
[32m+[m[32m    cursor: $cursor-disabled;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // [converter] extracted textarea& to textarea.form-control[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Reset height for `textarea`s[m
[32m+[m[32mtextarea.form-control {[m
[32m+[m[32m  height: auto;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Search inputs in iOS[m
[32m+[m[32m//[m
[32m+[m[32m// This overrides the extra rounded corners on search inputs in iOS so that our[m
[32m+[m[32m// `.form-control` class can properly style them. Note that this cannot simply[m
[32m+[m[32m// be added to `.form-control` as it's not specific enough. For details, see[m
[32m+[m[32m// https://github.com/twbs/bootstrap/issues/11586.[m
[32m+[m
[32m+[m[32minput[type="search"] {[m
[32m+[m[32m  -webkit-appearance: none;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Special styles for iOS temporal inputs[m
[32m+[m[32m//[m
[32m+[m[32m// In Mobile Safari, setting `display: block` on temporal inputs causes the[m
[32m+[m[32m// text within the input to become vertically misaligned. As a workaround, we[m
[32m+[m[32m// set a pixel line-height that matches the given height of the input, but only[m
[32m+[m[32m// for Safari. See https://bugs.webkit.org/show_bug.cgi?id=139848[m
[32m+[m[32m//[m
[32m+[m[32m// Note that as of 9.3, iOS doesn't support `week`.[m
[32m+[m
[32m+[m[32m@media screen and (-webkit-min-device-pixel-ratio: 0) {[m
[32m+[m[32m  input[type="date"],[m
[32m+[m[32m  input[type="time"],[m
[32m+[m[32m  input[type="datetime-local"],[m
[32m+[m[32m  input[type="month"] {[m
[32m+[m[32m    &.form-control {[m
[32m+[m[32m      line-height: $input-height-base;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    &.input-sm,[m
[32m+[m[32m    .input-group-sm & {[m
[32m+[m[32m      line-height: $input-height-small;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    &.input-lg,[m
[32m+[m[32m    .input-group-lg & {[m
[32m+[m[32m      line-height: $input-height-large;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Form groups[m
[32m+[m[32m//[m
[32m+[m[32m// Designed to help with the organization and spacing of vertical forms. For[m
[32m+[m[32m// horizontal forms, use the predefined grid classes.[m
[32m+[m
[32m+[m[32m.form-group {[m
[32m+[m[32m  margin-bottom: $form-group-margin-bottom;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Checkboxes and radios[m
[32m+[m[32m//[m
[32m+[m[32m// Indent the labels to position radios/checkboxes as hanging controls.[m
[32m+[m
[32m+[m[32m.radio,[m
[32m+[m[32m.checkbox {[m
[32m+[m[32m  position: relative;[m
[32m+[m[32m  display: block;[m
[32m+[m[32m  margin-top: 10px;[m
[32m+[m[32m  margin-bottom: 10px;[m
[32m+[m
[32m+[m[32m  label {[m
[32m+[m[32m    min-height: $line-height-computed; // Ensure the input doesn't jump when there is no text[m
[32m+[m[32m    padding-left: 20px;[m
[32m+[m[32m    margin-bottom: 0;[m
[32m+[m[32m    font-weight: normal;[m
[32m+[m[32m    cursor: pointer;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m.radio input[type="radio"],[m
[32m+[m[32m.radio-inline input[type="radio"],[m
[32m+[m[32m.checkbox input[type="checkbox"],[m
[32m+[m[32m.checkbox-inline input[type="checkbox"] {[m
[32m+[m[32m  position: absolute;[m
[32m+[m[32m  margin-left: -20px;[m
[32m+[m[32m  margin-top: 4px \9;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.radio + .radio,[m
[32m+[m[32m.checkbox + .checkbox {[m
[32m+[m[32m  margin-top: -5px; // Move up sibling radios or checkboxes for tighter spacing[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Radios and checkboxes on same line[m
[32m+[m[32m.radio-inline,[m
[32m+[m[32m.checkbox-inline {[m
[32m+[m[32m  position: relative;[m
[32m+[m[32m  display: inline-block;[m
[32m+[m[32m  padding-left: 20px;[m
[32m+[m[32m  margin-bottom: 0;[m
[32m+[m[32m  vertical-align: middle;[m
[32m+[m[32m  font-weight: normal;[m
[32m+[m[32m  cursor: pointer;[m
[32m+[m[32m}[m
[32m+[m[32m.radio-inline + .radio-inline,[m
[32m+[m[32m.checkbox-inline + .checkbox-inline {[m
[32m+[m[32m  margin-top: 0;[m
[32m+[m[32m  margin-left: 10px; // space out consecutive inline controls[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Apply same disabled cursor tweak as for inputs[m
[32m+[m[32m// Some special care is needed because <label>s don't inherit their parent's `cursor`.[m
[32m+[m[32m//[m
[32m+[m[32m// Note: Neither radios nor checkboxes can be readonly.[m
[32m+[m[32minput[type="radio"],[m
[32m+[m[32minput[type="checkbox"] {[m
[32m+[m[32m  &[disabled],[m
[32m+[m[32m  &.disabled,[m
[32m+[m[32m  fieldset[disabled] & {[m
[32m+[m[32m    cursor: $cursor-disabled;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m// These classes are used directly on <label>s[m
[32m+[m[32m.radio-inline,[m
[32m+[m[32m.checkbox-inline {[m
[32m+[m[32m  &.disabled,[m
[32m+[m[32m  fieldset[disabled] & {[m
[32m+[m[32m    cursor: $cursor-disabled;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m// These classes are used on elements with <label> descendants[m
[32m+[m[32m.radio,[m
[32m+[m[32m.checkbox {[m
[32m+[m[32m  &.disabled,[m
[32m+[m[32m  fieldset[disabled] & {[m
[32m+[m[32m    label {[m
[32m+[m[32m      cursor: $cursor-disabled;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Static form control text[m
[32m+[m[32m//[m
[32m+[m[32m// Apply class to a `p` element to make any string of text align with labels in[m
[32m+[m[32m// a horizontal form layout.[m
[32m+[m
[32m+[m[32m.form-control-static {[m
[32m+[m[32m  // Size it appropriately next to real form controls[m
[32m+[m[32m  padding-top: ($padding-base-vertical + 1);[m
[32m+[m[32m  padding-bottom: ($padding-base-vertical + 1);[m
[32m+[m[32m  // Remove default margin from `p`[m
[32m+[m[32m  margin-bottom: 0;[m
[32m+[m[32m  min-height: ($line-height-computed + $font-size-base);[m
[32m+[m
[32m+[m[32m  &.input-lg,[m
[32m+[m[32m  &.input-sm {[m
[32m+[m[32m    padding-left: 0;[m
[32m+[m[32m    padding-right: 0;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Form control sizing[m
[32m+[m[32m//[m
[32m+[m[32m// Build on `.form-control` with modifier classes to decrease or increase the[m
[32m+[m[32m// height and font-size of form controls.[m
[32m+[m[32m//[m
[32m+[m[32m// The `.form-group-* form-control` variations are sadly duplicated to avoid the[m
[32m+[m[32m// issue documented in https://github.com/twbs/bootstrap/issues/15074.[m
[32m+[m
[32m+[m[32m@include input-size('.input-sm', $input-height-small, $padding-small-vertical, $padding-small-horizontal, $font-size-small, $line-height-small, $input-border-radius-small);[m
[32m+[m[32m.form-group-sm {[m
[32m+[m[32m  .form-control {[m
[32m+[m[32m    height: $input-height-small;[m
[32m+[m[32m    padding: $padding-small-vertical $padding-small-horizontal;[m
[32m+[m[32m    font-size: $font-size-small;[m
[32m+[m[32m    line-height: $line-height-small;[m
[32m+[m[32m    border-radius: $input-border-radius-small;[m
[32m+[m[32m  }[m
[32m+[m[32m  select.form-control {[m
[32m+[m[32m    height: $input-height-small;[m
[32m+[m[32m    line-height: $input-height-small;[m
[32m+[m[32m  }[m
[32m+[m[32m  textarea.form-control,[m
[32m+[m[32m  select[multiple].form-control {[m
[32m+[m[32m    height: auto;[m
[32m+[m[32m  }[m
[32m+[m[32m  .form-control-static {[m
[32m+[m[32m    height: $input-height-small;[m
[32m+[m[32m    min-height: ($line-height-computed + $font-size-small);[m
[32m+[m[32m    padding: ($padding-small-vertical + 1) $padding-small-horizontal;[m
[32m+[m[32m    font-size: $font-size-small;[m
[32m+[m[32m    line-height: $line-height-small;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m@include input-size('.input-lg', $input-height-large, $padding-large-vertical, $padding-large-horizontal, $font-size-large, $line-height-large, $input-border-radius-large);[m
[32m+[m[32m.form-group-lg {[m
[32m+[m[32m  .form-control {[m
[32m+[m[32m    height: $input-height-large;[m
[32m+[m[32m    padding: $padding-large-vertical $padding-large-horizontal;[m
[32m+[m[32m    font-size: $font-size-large;[m
[32m+[m[32m    line-height: $line-height-large;[m
[32m+[m[32m    border-radius: $input-border-radius-large;[m
[32m+[m[32m  }[m
[32m+[m[32m  select.form-control {[m
[32m+[m[32m    height: $input-height-large;[m
[32m+[m[32m    line-height: $input-height-large;[m
[32m+[m[32m  }[m
[32m+[m[32m  textarea.form-control,[m
[32m+[m[32m  select[multiple].form-control {[m
[32m+[m[32m    height: auto;[m
[32m+[m[32m  }[m
[32m+[m[32m  .form-control-static {[m
[32m+[m[32m    height: $input-height-large;[m
[32m+[m[32m    min-height: ($line-height-computed + $font-size-large);[m
[32m+[m[32m    padding: ($padding-large-vertical + 1) $padding-large-horizontal;[m
[32m+[m[32m    font-size: $font-size-large;[m
[32m+[m[32m    line-height: $line-height-large;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Form control feedback states[m
[32m+[m[32m//[m
[32m+[m[32m// Apply contextual and semantic states to individual form controls.[m
[32m+[m
[32m+[m[32m.has-feedback {[m
[32m+[m[32m  // Enable absolute positioning[m
[32m+[m[32m  position: relative;[m
[32m+[m
[32m+[m[32m  // Ensure icons don't overlap text[m
[32m+[m[32m  .form-control {[m
[32m+[m[32m    padding-right: ($input-height-base * 1.25);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m// Feedback icon (requires .glyphicon classes)[m
[32m+[m[32m.form-control-feedback {[m
[32m+[m[32m  position: absolute;[m
[32m+[m[32m  top: 0;[m
[32m+[m[32m  right: 0;[m
[32m+[m[32m  z-index: 2; // Ensure icon is above input groups[m
[32m+[m[32m  display: block;[m
[32m+[m[32m  width: $input-height-base;[m
[32m+[m[32m  height: $input-height-base;[m
[32m+[m[32m  line-height: $input-height-base;[m
[32m+[m[32m  text-align: center;[m
[32m+[m[32m  pointer-events: none;[m
[32m+[m[32m}[m
[32m+[m[32m.input-lg + .form-control-feedback,[m
[32m+[m[32m.input-group-lg + .form-control-feedback,[m
[32m+[m[32m.form-group-lg .form-control + .form-control-feedback {[m
[32m+[m[32m  width: $input-height-large;[m
[32m+[m[32m  height: $input-height-large;[m
[32m+[m[32m  line-height: $input-height-large;[m
[32m+[m[32m}[m
[32m+[m[32m.input-sm + .form-control-feedback,[m
[32m+[m[32m.input-group-sm + .form-control-feedback,[m
[32m+[m[32m.form-group-sm .form-control + .form-control-feedback {[m
[32m+[m[32m  width: $input-height-small;[m
[32m+[m[32m  height: $input-height-small;[m
[32m+[m[32m  line-height: $input-height-small;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Feedback states[m
[32m+[m[32m.has-success {[m
[32m+[m[32m  @include form-control-validation($state-success-text, $state-success-text, $state-success-bg);[m
[32m+[m[32m}[m
[32m+[m[32m.has-warning {[m
[32m+[m[32m  @include form-control-validation($state-warning-text, $state-warning-text, $state-warning-bg);[m
[32m+[m[32m}[m
[32m+[m[32m.has-error {[m
[32m+[m[32m  @include form-control-validation($state-danger-text, $state-danger-text, $state-danger-bg);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Reposition feedback icon if input has visible label above[m
[32m+[m[32m.has-feedback label {[m
[32m+[m
[32m+[m[32m  & ~ .form-control-feedback {[m
[32m+[m[32m    top: ($line-height-computed + 5); // Height of the `label` and its margin[m
[32m+[m[32m  }[m
[32m+[m[32m  &.sr-only ~ .form-control-feedback {[m
[32m+[m[32m    top: 0;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Help text[m
[32m+[m[32m//[m
[32m+[m[32m// Apply to any element you wish to create light text for placement immediately[m
[32m+[m[32m// below a form control. Use for general help, formatting, or instructional text.[m
[32m+[m
[32m+[m[32m.help-block {[m
[32m+[m[32m  display: block; // account for any element using help-block[m
[32m+[m[32m  margin-top: 5px;[m
[32m+[m[32m  margin-bottom: 10px;[m
[32m+[m[32m  color: lighten($text-color, 25%); // lighten the text some for contrast[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Inline forms[m
[32m+[m[32m//[m
[32m+[m[32m// Make forms appear inline(-block) by adding the `.form-inline` class. Inline[m
[32m+[m[32m// forms begin stacked on extra small (mobile) devices and then go inline when[m
[32m+[m[32m// viewports reach <768px.[m
[32m+[m[32m//[m
[32m+[m[32m// Requires wrapping inputs and labels with `.form-group` for proper display of[m
[32m+[m[32m// default HTML form controls and our custom form controls (e.g., input groups).[m
[32m+[m[32m//[m
[32m+[m[32m// Heads up! This is mixin-ed into `.navbar-form` in navbars.less.[m
[32m+[m
[32m+[m[32m// [converter] extracted from `.form-inline` for libsass compatibility[m
[32m+[m[32m@mixin form-inline {[m
[32m+[m
[32m+[m[32m  // Kick in the inline[m
[32m+[m[32m  @media (min-width: $screen-sm-min) {[m
[32m+[m[32m    // Inline-block all the things for "inline"[m
[32m+[m[32m    .form-group {[m
[32m+[m[32m      display: inline-block;[m
[32m+[m[32m      margin-bottom: 0;[m
[32m+[m[32m      vertical-align: middle;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // In navbar-form, allow folks to *not* use `.form-group`[m
[32m+[m[32m    .form-control {[m
[32m+[m[32m      display: inline-block;[m
[32m+[m[32m      width: auto; // Prevent labels from stacking above inputs in `.form-group`[m
[32m+[m[32m      vertical-align: middle;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Make static controls behave like regular ones[m
[32m+[m[32m    .form-control-static {[m
[32m+[m[32m      display: inline-block;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    .input-group {[m
[32m+[m[32m      display: inline-table;[m
[32m+[m[32m      vertical-align: middle;[m
[32m+[m
[32m+[m[32m      .input-group-addon,[m
[32m+[m[32m      .input-group-btn,[m
[32m+[m[32m      .form-control {[m
[32m+[m[32m        width: auto;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Input groups need that 100% width though[m
[32m+[m[32m    .input-group > .form-control {[m
[32m+[m[32m      width: 100%;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    .control-label {[m
[32m+[m[32m      margin-bottom: 0;[m
[32m+[m[32m      vertical-align: middle;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Remove default margin on radios/checkboxes that were used for stacking, and[m
[32m+[m[32m    // then undo the floating of radios and checkboxes to match.[m
[32m+[m[32m    .radio,[m
[32m+[m[32m    .checkbox {[m
[32m+[m[32m      display: inline-block;[m
[32m+[m[32m      margin-top: 0;[m
[32m+[m[32m      margin-bottom: 0;[m
[32m+[m[32m      vertical-align: middle;[m
[32m+[m
[32m+[m[32m      label {[m
[32m+[m[32m        padding-left: 0;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    .radio input[type="radio"],[m
[32m+[m[32m    .checkbox input[type="checkbox"] {[m
[32m+[m[32m      position: relative;[m
[32m+[m[32m      margin-left: 0;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Re-override the feedback icon.[m
[32m+[m[32m    .has-feedback .form-control-feedback {[m
[32m+[m[32m      top: 0;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m// [converter] extracted as `@mixin form-inline` for libsass compatibility[m
[32m+[m[32m.form-inline {[m
[32m+[m[32m  @include form-inline;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m// Horizontal forms[m
[32m+[m[32m//[m
[32m+[m[32m// Horizontal forms are built on grid classes and allow you to create forms with[m
[32m+[m[32m// labels on the left and inputs on the right.[m
[32m+[m
[32m+[m[32m.form-horizontal {[m
[32m+[m
[32m+[m[32m  // Consistent vertical alignment of radios and checkboxes[m
[32m+[m[32m  //[m
[32m+[m[32m  // Labels also get some reset styles, but that is scoped to a media query below.[m
[32m+[m[32m  .radio,[m
[32m+[m[32m  .checkbox,[m
[32m+[m[32m  .radio-inline,[m
[32m+[m[32m  .checkbox-inline {[m
[32m+[m[32m    margin-top: 0;[m
[32m+[m[32m    margin-bottom: 0;[m
[32m+[m[32m    padding-top: ($padding-base-vertical + 1); // Default padding plus a border[m
[32m+[m[32m  }[m
[32m+[m[32m  // Account for padding we're adding to ensure the alignment and of help text[m
[32m+[m[32m  // and other content below items[m
[32m+[m[32m  .radio,[m
[32m+[m[32m  .checkbox {[m
[32m+[m[32m    min-height: ($line-height-computed + ($padding-base-vertical + 1));[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Make form groups behave like rows[m
[32m+[m[32m  .form-group {[m
[32m+[m[32m    @include make-row;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Reset spacing and right align labels, but scope to media queries so that[m
[32m+[m[32m  // labels on narrow viewports stack the same as a default form example.[m
[32m+[m[32m  @media (min-width: $screen-sm-min) {[m
[32m+[m[32m    .control-label {[m
[32m+[m[32m      text-align: right;[m
[32m+[m[32m      margin-bottom: 0;[m
[32m+[m[32m      padding-top: ($padding-base-vertical + 1); // Default padding plus a border[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Validation states[m
[32m+[m[32m  //[m
[32m+[m[32m  // Reposition the icon because it's now within a grid column and columns have[m
[32m+[m[32m  // `position: relative;` on them. Also accounts for the grid gutter padding.[m
[32m+[m[32m  .has-feedback .form-control-feedback {[m
[32m+[m[32m    right: floor(($grid-gutter-width / 2));[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Form group sizes[m
[32m+[m[32m  //[m
[32m+[m[32m  // Quick utility class for applying `.input-lg` and `.input-sm` styles to the[m
[32m+[m[32m  // inputs and labels within a `.form-group`.[m
[32m+[m[32m  .form-group-lg {[m
[32m+[m[32m    @media (min-width: $screen-sm-min) {[m
[32m+[m[32m      .control-label {[m
[32m+[m[32m        padding-top: ($padding-large-vertical + 1);[m
[32m+[m[32m        font-size: $font-size-large;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  .form-group-sm {[m
[32m+[m[32m    @media (min-width: $screen-sm-min) {[m
[32m+[m[32m      .control-label {[m
[32m+[m[32m        padding-top: ($padding-small-vertical + 1);[m
[32m+[m[32m        font-size: $font-size-small;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
