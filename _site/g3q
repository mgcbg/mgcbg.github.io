[32m+[m[32m// ==========================================================================[m
[32m+[m[32m// Print styles.[m
[32m+[m[32m// Inlined to avoid the additional HTTP request: h5bp.com/r[m
[32m+[m[32m// ==========================================================================[m
[32m+[m
[32m+[m[32m@media print {[m
[32m+[m[32m    *,[m
[32m+[m[32m    *:before,[m
[32m+[m[32m    *:after {[m
[32m+[m[32m        background: transparent !important;[m
[32m+[m[32m        color: #000 !important; // Black prints faster: h5bp.com/s[m
[32m+[m[32m        box-shadow: none !important;[m
[32m+[m[32m        text-shadow: none !important;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    a,[m
[32m+[m[32m    a:visited {[m
[32m+[m[32m        text-decoration: underline;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    a[href]:after {[m
[32m+[m[32m        content: " (" attr(href) ")";[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    abbr[title]:after {[m
[32m+[m[32m        content: " (" attr(title) ")";[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Don't show links that are fragment identifiers,[m
[32m+[m[32m    // or use the `javascript:` pseudo protocol[m
[32m+[m[32m    a[href^="#"]:after,[m
[32m+[m[32m    a[href^="javascript:"]:after {[m
[32m+[m[32m        content: "";[m
[32m+[m[32m    }[m
[32m+[m
