[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m.btn-group-vertical > .btn-group:last-child:not(:first-child) > .btn:first-child {[m
[32m+[m[32m  @include border-top-radius(0);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Justified button groups[m
[32m+[m[32m// ----------------------[m
[32m+[m
[32m+[m[32m.btn-group-justified {[m
[32m+[m[32m  display: table;[m
[32m+[m[32m  width: 100%;[m
[32m+[m[32m  table-layout: fixed;[m
[32m+[m[32m  border-collapse: separate;[m
[32m+[m[32m  > .btn,[m
[32m+[m[32m  > .btn-group {[m
[32m+[m[32m    float: none;[m
[32m+[m[32m    display: table-cell;[m
[32m+[m[32m    width: 1%;[m
[32m+[m[32m  }[m
[32m+[m[32m  > .btn-group .btn {[m
[32m+[m[32m    width: 100%;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  > .btn-group .dropdown-menu {[m
[32m+[m[32m    left: auto;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Checkbox and radio options[m
[32m+[m[32m//[m
[32m+[m[32m// In order to support the browser's form validation feedback, powered by the[m
[32m+[m[32m// `required` attribute, we have to "hide" the inputs via `clip`. We cannot use[m
[32m+[m[32m// `display: none;` or `visibility: hidden;` as that also hides the popover.[m
[32m+[m[32m// Simply visually hiding the inputs via `opacity` would leave them clickable in[m
[32m+[m[32m// certain cases which is prevented by using `clip` and `pointer-events`.[m
[32m+[m[32m// This way, we ensure a DOM element is visible to position the popover from.[m
[32m+[m[32m//[m
[32m+[m[32m// See https://github.com/twbs/bootstrap/pull/12794 and[m
[32m+[m[32m// https://github.com/twbs/bootstrap/pull/14559 for more information.[m
[32m+[m
[32m+[m[32m[data-toggle="buttons"] {[m
[32m+[m[32m  > .btn,[m
[32m+[m[32m  > .btn-group > .btn {[m
[32m+[m[32m    input[type="radio"],[m
[32m+[m[32m    input[type="checkbox"] {[m
[32m+[m[32m      position: absolute;[m
[32m+[m[32m      clip: rect(0,0,0,0);[m
[32m+[m[32m      pointer-events: none;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[1mdiff --git a/_sass/bootstrap/_buttons.scss b/_sass/bootstrap/_buttons.scss[m
[1mnew file mode 100644[m
[1mindex 0000000..6452b70[m
[1m--- /dev/null[m
[1m+++ b/_sass/bootstrap/_buttons.scss[m
[36m@@ -0,0 +1,168 @@[m
