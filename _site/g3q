[32m+[m[32m$bootstrap-sass-asset-helper: false !default;[m
[32m+[m[32m// Lumen 3.3.7[m
[32m+[m[32m// Variables[m
[32m+[m[32m// --------------------------------------------------[m
[32m+[m
[32m+[m
[32m+[m[32m//== Colors[m
[32m+[m[32m//[m
[32m+[m[32m//## Gray and brand colors for use across Bootstrap.[m
[32m+[m
[32m+[m[32m$gray-base:              #FFE4B5 !default;[m
[32m+[m[32m$gray-darker:            lighten($gray-base, 13.5%) !default; // #222[m
[32m+[m[32m$gray-dark:              lighten($gray-base, 20%) !default;   // #333[m
[32m+[m[32m$gray:                   lighten($gray-base, 33.5%) !default; // #555[m
[32m+[m[32m$gray-light:             lighten($gray-base, 60%) !default;   // #999[m
[32m+[m[32m$gray-lighter:           lighten($gray-base, 93.5%) !default; // #eee[m
[32m+[m
[32m+[m[32m$brand-primary:         #158CBA !default;[m
[32m+[m[32m$brand-success:         #28B62C !default;[m
[32m+[m[32m$brand-info:            #75CAEB !default;[m
[32m+[m[32m$brand-warning:         #FF851B !default;[m
[32m+[m[32m$brand-danger:          #FF4136 !default;[m
[32m+[m
[32m+[m
[32m+[m[32m//== Scaffolding[m
[32m+[m[32m//[m
[32m+[m[32m//## Settings for some of the most global styles.[m
[32m+[m
[32m+[m[32m//** Background color for `<body>`.[m
[32m+[m[32m$body-bg:               #fff !default;[m
[32m+[m[32m//** Global text color on `<body>`.[m
[32m+[m[32m$text-color:            $gray !default;[m
[32m+[m
[32m+[m[32m//** Global textual link color.[m
[32m+[m[32m$link-color:            #0000FF !default;[m
[32m+[m[32m//** Link hover color set via `darken()` function.[m
[32m+[m[32m$link-hover-color:      $link-color !default;[m
[32m+[m[32m//** Link hover decoration.[m
[32m+[m[32m$link-hover-decoration: underline !default;[m
[32m+[m
[32m+[m
[32m+[m[32m//== Typography[m
[32m+[m[32m//[m
[32m+[m[32m//## Font, line-height, and color for body text, headings, and more.[m
[32m+[m
[32m+[m[32m$font-family-sans-serif:   'Prompt', "Source Sans Pro", "Helvetica Neue", Helvetica, Arial, sans-serif !default;[m
[32m+[m[32m$font-family-serif:       'Sawarabi Mincho', Georgia, "Times New Roman", Times, serif !default;[m
[32m+[m[32m//** Default monospace fonts for `<code>`, `<kbd>`, and `<pre>`.[m
[32m+[m[32m$font-family-monospace:   "Anonymous Pro", Menlo, Monaco, Consolas, "Courier New", monospace !default;[m
[32m+[m[32m$font-family-base:        $font-family-sans-serif !default;[m
[32m+[m
[32m+[m[32m$font-size-base:          14px !default;[m
[32m+[m[32m$font-size-large:         ceil(($font-size-base * 1.25)) !default; // ~18px[m
[32m+[m[32m$font-size-small:         ceil(($font-size-base * 0.85)) !default; // ~12px[m
[32m+[m
[32m+[m[32m$font-size-h1:            floor(($font-size-base * 2.6)) !default; // ~36px[m
[32m+[m[32m$font-size-h2:            floor(($font-size-base * 2.15)) !default; // ~30px[m
[32m+[m[32m$font-size-h3:            ceil(($font-size-base * 1.7)) !default; // ~24px[m
[32m+[m[32m$font-size-h4:            ceil(($font-size-base * 1.25)) !default; // ~18px[m
[32m+[m[32m$font-size-h5:            $font-size-base !default;[m
[32m+[m[32m$font-size-h6:            ceil(($font-size-base * 0.85)) !default; // ~12px[m
[32m+[m
[32m+[m[32m//** Unit-less `line-height` for use in components like buttons.[m
[32m+[m[32m$line-height-base:        1.428571429 !default; // 20/14[m
[32m+[m[32m//** Computed "line-height" (`font-size` * `line-height`) for use with `margin`, `padding`, etc.[m
[32m+[m[32m$line-height-computed:    floor(($font-size-base * $line-height-base)) !default; // ~20px[m
[32m+[m
[32m+[m[32m//** By default, this inherits from the `<body>`.[m
[32m+[m[32m$headings-font-family:    inherit !default;[m
[32m+[m[32m$headings-font-weight:    400 !default;[m
[32m+[m[32m$headings-line-height:    1.1 !default;[m
[32m+[m[32m$headings-color:          #191970 !default;[m
[32m+[m
[32m+[m
[32m+[m[32m//== Iconography[m
[32m+[m[32m//[m
[32m+[m[32m//## Specify custom location and filename of the included Glyphicons icon font. Useful for those including Bootstrap via Bower.[m
[32m+[m
[32m+[m[32m//** Load fonts from this directory.[m
[32m+[m[32m$icon-font-path: if($bootstrap-sass-asset-helper, "bootstrap/", "../fonts/") !default;[m
[32m+[m[32m//** File name for all font files.[m
[32m+[m[32m$icon-font-name:          "glyphicons-halflings-regular" !default;[m
[32m+[m[32m//** Element ID within SVG icon file.[m
[32m+[m[32m$icon-font-svg-id:        "glyphicons_halflingsregular" !default;[m
[32m+[m
[32m+[m
[32m+[m[32m//== Components[m
[32m+[m[32m//[m
[32m+[m[32m//## Define common padding and border radius sizes and more. Values based on 14px text and 1.428 line-height (~20px to start).[m
[32m+[m
[32m+[m[32m$padding-base-vertical:     7px !default;[m
[32m+[m[32m$padding-base-horizontal:   12px !default;[m
[32m+[m
[32m+[m[32m$padding-large-vertical:    13px !default;[m
[32m+[m[32m$padding-large-horizontal:  16px !default;[m
[32m+[m
[32m+[m[32m$padding-small-vertical:    4px !default;[m
[32m+[m[32m$padding-small-horizontal:  10px !default;[m
[32m+[m
[32m+[m[32m$padding-xs-vertical:       1px !default;[m
[32m+[m[32m$padding-xs-horizontal:     5px !default;[m
[32m+[m
[32m+[m[32m$line-height-large:         1.3333333 !default; // extra decimals for Win 8.1 Chrome[m
[32m+[m[32m$line-height-small:         1.5 !default;[m
[32m+[m
[32m+[m[32m$border-radius-base:        4px !default;[m
[32m+[m[32m$border-radius-large:       5px !default;[m
[32m+[m[32m$border-radius-small:       2px !default;[m
[32m+[m
[32m+[m[32m//** Global color for active items (e.g., navs or dropdowns).[m
[32m+[m[32m$component-active-color:    #fff !default;[m
[32m+[m[32m//** Global background color for active items (e.g., navs or dropdowns).[m
[32m+[m[32m$component-active-bg:       $brand-primary !default;[m
[32m+[m
[32m+[m[32m//** Width of the `border` for generating carets that indicate dropdowns.[m
[32m+[m[32m$caret-width-base:          4px !default;[m
[32m+[m[32m//** Carets increase slightly in size for larger components.[m
[32m+[m[32m$caret-width-large:         5px !default;[m
[32m+[m
[32m+[m
[32m+[m[32m//== Tables[m
[32m+[m[32m//[m
[32m+[m[32m//## Customizes the `.table` component with basic values, each used across all table variations.[m
[32m+[m
[32m+[m[32m//** Padding for `<th>`s and `<td>`s.[m
[32m+[m[32m$table-cell-padding:            8px !default;[m
[32m+[m[32m//** Padding for cells in `.table-condensed`.[m
[32m+[m[32m$table-condensed-cell-padding:  5px !default;[m
[32m+[m
[32m+[m[32m//** Default background color used for all tables.[m
[32m+[m[32m$table-bg:                      transparent !default;[m
[32m+[m[32m//** Background color used for `.table-striped`.[m
[32m+[m[32m$table-bg-accent:               #f9f9f9 !default;[m
[32m+[m[32m//** Background color used for `.table-hover`.[m
[32m+[m[32m$table-bg-hover:                #f5f5f5 !default;[m
[32m+[m[32m$table-bg-active:               $table-bg-hover !default;[m
[32m+[m
[32m+[m[32m//** Border color for table and cell borders.[m
[32m+[m[32m$table-border-color:            $gray-lighter !default;[m
[32m+[m
[32m+[m
[32m+[m[32m//== Buttons[m
[32m+[m[32m//[m
[32m+[m[32m//## For each of Bootstrap's buttons, define text, background and border color.[m
[32m+[m
[32m+[m[32m$btn-font-weight:                normal !default;[m
[32m+[m
[32m+[m[32m$btn-default-color:              $gray !default;[m
[32m+[m[32m$btn-default-bg:                 $gray-lighter !default;[m
[32m+[m[32m$btn-default-border:             darken($btn-default-bg, 5%) !default;[m
[32m+[m
[32m+[m[32m$btn-primary-color:              #fff !default;[m
[32m+[m[32m$btn-primary-bg:                 $brand-primary !default;[m
[32m+[m[32m$btn-primary-border:             darken($btn-primary-bg, 5%) !default;[m
[32m+[m
[32m+[m[32m$btn-success-color:              #fff !default;[m
[32m+[m[32m$btn-success-bg:                 $brand-success !default;[m
[32m+[m[32m$btn-success-border:             darken($btn-success-bg, 5%) !default;[m
[32m+[m
[32m+[m[32m$btn-info-color:                 #fff !default;[m
[32m+[m[32m$btn-info-bg:                    $brand-info !default;[m
[32m+[m[32m$btn-info-border:                darken($btn-info-bg, 5%) !default;[m
[32m+[m
[32m+[m[32m$btn-warning-color:              #fff !default;[m
[32m+[m[32m$btn-warning-bg:                 $brand-warning !default;[m
[32m+[m[32m$btn-warning-border:             darken($btn-warning-bg, 5%) !default;[m
[32m+[m
[32m+[m[32m$btn-danger-color:               #fff !default;[m
[32m+[m[32m$btn-danger-bg:                  $brand-danger !default;[m
[32m+[m[32m$btn-danger-border:              darken($btn-danger-bg, 5%) !default;[m
[32m+[m
[32m+[m[32m$btn-link-disabled-color:        $gray-light !default;[m
[32m+[m
[32m+[m[32m// Allows for customizing button radius independently from global border radius[m
[32m+[m[32m$btn-border-radius-base:         $border-radius-base !default;[m
[32m+[m[32m$btn-border-radius-large:        $border-radius-large !default;[m
[32m+[m[32m$btn-border-radius-small:        $border-radius-small !default;[m
[32m+[m
[32m+[m
[32m+[m[32m//== Forms[m
[32m+[m[32m//[m
[32m+[m[32m//##[m
[32m+[m
[32m+[m[32m//** `<input>` background color[m
[32m+[m[32m$input-bg:                       #fff !default;[m
[32m+[m[32m//** `<input disabled>` background color[m
[32m+[m[32m$input-bg-disabled:              $gray-lighter !default;[m
[32m+[m
[32m+[m[32m//** Text color for `<input>`s[m
[32m+[m[32m$input-color:                    $gray !default;[m
[32m+[m[32m//** `<input>` border color[m
[32m+[m[32m$input-border:                   dar,  ken(#f8f8f8, 6.5%) !default;[m
[32m+[m
[32m+[m[32m// TODO: Rename `$input-border-radius` to `$input-border-radius-base` in v4[m
[32m+[m[32m//** Default `.form-control` border radius[m
[32m+[m[32m// This has no effect on `<select>`s in some browsers, due to the limited stylability of `<select>`s in CSS.[m
[32m+[m[32m$input-border-radius:            $border-radius-base !default;[m
[32m+[m[32m//** Large `.form-control` border radius[m
[32m+[m[32m$input-border-radius-large:      $border-radius-large !default;[m
[32m+[m[32m//** Small `.form-control` border radius[m
[32m+[m[32m$input-border-radius-small:      $border-radius-small !default;[m
[32m+[m
[32m+[m[32m//** Border color for inputs on focus[m
[32m+[m[32m$input-border-focus:             #66afe9 !default;[m
[32m+[m
[32m+[m[32m//** Placeholder text color[m
[32m+[m[32m$input-color-placeholder:        $gray-light !default;[m
[32m+[m
[32m+[m[32m//** Default `.form-control` height[m
[32m+[m[32m$input-height-base:              ($line-height-computed + ($padding-base-vertical * 2) + 4) !default;[m
[32m+[m[32m//** Large `.form-control` height[m
[32m+[m[32m$input-height-large:             (ceil($font-size-large * $line-height-large) + ($padding-large-vertical * 2) + 2) !default;[m
[32m+[m[32m//** Small `.form-control` height[m
[32m+[m[32m$input-height-small:             (floor($font-size-small * $line-height-small) + ($padding-small-vertical * 2) + 2) !default;[m
[32m+[m
[32m+[m[32m//** `.form-group` margin[m
[32m+[m[32m$form-group-margin-bottom:       15px !default;[m
[32m+[m
[32m+[m[32m$legend-color:                   $gray-dark !default;[m
[32m+[m[32m$legend-border-color:            #e5e5e5 !default;[m
[32m+[m
[32m+[m[32m//** Background color for textual input addons[m
[32m+[m[32m$input-group-addon-bg:           $gray-lighter !default;[m
[32m+[m[32m//** Border color for textual input addons[m
[32m+[m[32m$input-group-addon-border-color: $input-border !default;[m
[32m+[m
[32m+[m[32m//** Disabled cursor for form controls and buttons.[m
[32m+[m[32m$cursor-disabled:                not-allowed !default;[m
[32m+[m
[32m+[m
[32m+[m[32m//== Dropdowns[m
[32m+[m[32m//[m
[32m+[m[32m//## Dropdown menu container and contents.[m
[32m+[m
[32m+[m[32m//** Background for the dropdown menu.[m
[32m+[m[32m$dropdown-bg:                    #fff !default;[m
[32m+[m[32m//** Dropdown menu `border-color`.[m
[32m+[m[32m$dropdown-border:                $input-border !default;[m
[32m+[m[32m//** Dropdown menu `border-color` **for IE8**.[m
[32m+[m[32m$dropdown-fallback-border:       #ccc !default;[m
[32m+[m[32m//** Divider color for between dropdown items.[m
[32m+[m[32m$dropdown-divider-bg:            $gray-lighter !default;[m
[32m+[m
[32m+[m[32m//** Dropdown link text color.[m
[32m+[m[32m$dropdown-link-color:            $gray-light !default;[m
[32m+[m[32m//** Hover color for dropdown links.[m
[32m+[m[32m$dropdown-link-hover-color:      $gray-dark !default;[m
[32m+[m[32m//** Hover background for dropdown links.[m
[32m+[m[32m$dropdown-link-hover-bg:         transparent !default;[m
[32m+[m
[32m+[m[32m//** Active dropdown menu item text color.[m
[32m+[m[32m$dropdown-link-active-color:     $component-active-color !default;[m
[32m+[m[32m//** Active dropdown menu item background color.[m
[32m+[m[32m$dropdown-link-active-bg:        $component-active-bg !default;[m
[32m+[m
[32m+[m[32m//** Disabled dropdown menu item background color.[m
[32m+[m[32m$dropdown-link-disabled-color:   $gray-lighter !default;[m
[32m+[m
[32m+[m[32m//** Text color for headers within dropdown menus.[m
[32m+[m[32m$dropdown-header-color:          $gray-light !default;[m
[32m+[m
[32m+[m[32m//** Deprecated `$dropdown-caret-color` as of v3.1.0[m
[32m+[m[32m$dropdown-caret-color:           #000 !default;[m
[32m+[m
[32m+[m
[32m+[m[32m//-- Z-index master list[m
[32m+[m[32m//[m
[32m+[m[32m// Warning: Avoid customizing these values. They're used for a bird's eye view[m
[32m+[m[32m// of components dependent on the z-axis and are designed to all work together.[m
[32m+[m[32m//[m
[32m+[m[32m// Note: These variables are not generated into the Customizer.[m
[32m+[m
[32m+[m[32m$zindex-navbar:            1000 !default;[m
[32m+[m[32m$zindex-dropdown:          1000 !default;[m
[32m+[m[32m$zindex-popover:           1060 !default;[m
[32m+[m[32m$zindex-tooltip:           1070 !default;[m
[32m+[m[32m$zindex-navbar-fixed:      1030 !default;[m
[32m+[m[32m$zindex-modal-background:  1040 !default;[m
[32m+[m[32m$zindex-modal:             1050 !default;[m
[32m+[m
[32m+[m
[32m+[m[32m//== Media queries breakpoints[m
[32m+[m[32m//[m
[32m+[m[32m//## Define the breakpoints at which your layout will change, adapting to different screen sizes.[m
[32m+[m
[32m+[m[32m// Extra small screen / phone[m
[32m+[m[32m//** Deprecated `$screen-xs` as of v3.0.1[m
[32m+[m[32m$screen-xs:                  480px !default;[m
[32m+[m[32m//** Deprecated `$screen-xs-min` as of v3.2.0[m
[32m+[m[32m$screen-xs-min:              $screen-xs !default;[m
[32m+[m[32m//** Deprecated `$screen-phone` as of v3.0.1[m
[32m+[m[32m$screen-phone:               $screen-xs-min !default;[m
[32m+[m
[32m+[m[32m// Small screen / tablet[m
[32m+[m[32m//** Deprecated `$screen-sm` as of v3.0.1[m
[32m+[m[32m$screen-sm:                  768px !default;[m
[32m+[m[32m$screen-sm-min:              $screen-sm !default;[m
[32m+[m[32m//** Deprecated `$screen-tablet` as of v3.0.1[m
[32m+[m[32m$screen-tablet:              $screen-sm-min !default;[m
[32m+[m
[32m+[m[32m// Medium screen / desktop[m
[32m+[m[32m//** Deprecated `$screen-md` as of v3.0.1[m
[32m+[m[32m$screen-md:                  992px !default;[m
[32m+[m[32m$screen-md-min:              $screen-md !default;[m
[32m+[m[32m//** Deprecated `$screen-desktop` as of v3.0.1[m
[32m+[m[32m$screen-desktop:             $screen-md-min !default;[m
[32m+[m
[32m+[m[32m// Large screen / wide desktop[m
[32m+[m[32m//** Deprecated `$screen-lg` as of v3.0.1[m
[32m+[m[32m$screen-lg:                  200px !default;[m
[32m+[m[32m$screen-lg-min:              $screen-lg !default;[m
[32m+[m[32m//** Deprecated `$screen-lg-desktop` as of v3.0.1[m
[32m+[m[32m$screen-lg-desktop:          $screen-lg-min !default;[m
[32m+[m
[32m+[m[32m// So media queries don't overlap when required, provide a maximum[m
[32m+[m[32m$screen-xs-max:              ($screen-sm-min - 1) !default;[m
[32m+[m[32m$screen-sm-max:              ($screen-md-min - 1) !default;[m
[32m+[m[32m$screen-md-max:              ($screen-lg-min - 1) !default;[m
[32m+[m
[32m+[m
[32m+[m[32m//== Grid system[m
[32m+[m[32m//[m
[32m+[m[32m//## Define your custom responsive grid.[m
[32m+[m
[32m+[m[32m//** Number of columns in the grid.[m
[32m+[m[32m$grid-columns:              12 !default;[m
[32m+[m[32m//** Padding between columns. Gets divided in half for the left and right.[m
[32m+[m[32m$grid-gutter-width:         30px !default;[m
[32m+[m[32m// Navbar collapse[m
[32m+[m[32m//** Point at which the navbar becomes uncollapsed.[m
[32m+[m[32m$grid-float-breakpoint:     $screen-sm-min !default;[m
[32m+[m[32m//** Point at which the navbar begins collapsing.[m
[32m+[m[32m$grid-float-breakpoint-max: ($grid-float-breakpoint - 1) !default;[m
[32m+[m
[32m+[m
[32m+[m[32m//== Container sizes[m
[32m+[m[32m//[m
[32m+[m[32m//## Define the maximum width of `.container` for different screen sizes.[m
[32m+[m
[32m+[m[32m// Small screen / tablet[m
[32m+[m[32m$container-tablet:             (720px + $grid-gutter-width) !default;[m
[32m+[m[32m//** For `$screen-sm-min` and up.[m
[32m+[m[32m$container-sm:                 $container-tablet !default;[m
[32m+[m
[32m+[m[32m// Medium screen / desktop[m
[32m+[m[32m$container-desktop:            (940px + $grid-gutter-width) !default;[m
[32m+[m[32m//** For `$screen-md-min` and up.[m
[32m+[m[32m$container-md:                 $container-desktop !default;[m
[32m+[m
[32m+[m[32m// Large screen / wide desktop[m
[32m+[m[32m$container-large-desktop:      (1140px + $grid-gutter-width) !default;[m
[32m+[m[32m//** For `$screen-lg-min` and up.[m
[32m+[m[32m$container-lg:                 $container-large-desktop !default;[m
[32m+[m
[32m+[m
[32m+[m[32m//== Navbar[m
[32m+[m[32m//[m
[32m+[m[32m//##[m
[32m+[m
[32m+[m[32m// Basics of a navbar[m
[32m+[m[32m$navbar-height:                    50px !default;[m
[32m+[m[32m$navbar-margin-bottom:             $line-height-computed !default;[m
[32m+[m[32m$navbar-border-radius:             $border-radius-base !default;[m
[32m+[m[32m$navbar-padding-horizontal:        5px !default;[m
[32m+[m[32m$navbar-padding-vertical:          (($navbar-height - $line-height-computed) / 2) !default;[m
[32m+[m[32m$navbar-collapse-max-height:       340px !default;[m
[32m+[m
[32m+[m
[32m+[m[32m$navbar-default-color:             $text-color !default;[m
[32m+[m[32m$navbar-default-bg:                #DAA520 !default;[m
[32m+[m[32m$navbar-default-border:            $input-border !default;[m
[32m+[m
[32m+[m[32m// Navbar links[m
[32m+[m[32m$navbar-default-link-color:               #CD853F !default;[m
[32m+[m[32m$navbar-default-link-hover-color:          $gray-dark !default;[m
[32m+[m[32m$navbar-default-link-hover-bg:             transparent !default;[m
[32m+[m[32m$navbar-default-link-active-color:         $navbar-default-link-hover-color !default;[m
[32m+[m[32m$navbar-default-link-active-bg:            transparent !default;[m
[32m+[m[32m$navbar-default-link-disabled-color:       $gray-lighter !default;[m
[32m+[m[32m$navbar-default-link-disabled-bg:          transparent !default;[m
[32m+[m
[32m+[m[32m// Navbar brand label[m
[32m+[m[32m$navbar-default-brand-color:               #CD853F !default;[m
[32m+[m[32m$navbar-default-brand-hover-color:         $navbar-default-link-hover-color !default;[m
[32m+[m[32m$navbar-default-brand-hover-bg:            transparent !default;[m
[32m+[m
[32m+[m[32m// Navbar toggle[m
[32m+[m[32m$navbar-default-toggle-hover-bg:           #fff !default;[m
[32m+[m[32m$navbar-default-toggle-icon-bar-bg:        $gray-light !default;[m
[32m+[m[32m$navbar-default-toggle-border-color:       $gray-lighter !default;[m
[32m+[m
[32m+[m
[32m+[m[32m//=== Inverted navbar[m
[32m+[m[32m// Reset inverted navbar basics[m
[32m+[m[32m$navbar-inverse-color:                      $gray-light !default;[m
[32m+[m[32m$navbar-inverse-bg:                         #fff !default;[m
[32m+[m[32m$navbar-inverse-border:                     darken($navbar-inverse-bg, 10%) !default;[m
[32m+[m
[32m+[m[32m// Inverted navbar links[m
[32m+[m[32m$navbar-inverse-link-color:                 $gray-light !default;[m
[32m+[m[32m$navbar-inverse-link-hover-color:           $gray-dark !default;[m
[32m+[m[32m$navbar-inverse-link-hover-bg:              transparent !default;[m
[32m+[m[32m$navbar-inverse-link-active-color:          $navbar-inverse-link-hover-color !default;[m
[32m+[m[32m$navbar-inverse-link-active-bg:             transparent !default;[m
[32m+[m[32m$navbar-inverse-link-disabled-color:        $gray-lighter !default;[m
[32m+[m[32m$navbar-inverse-link-disabled-bg:           transparent !default;[m
[32m+[m
[32m+[m[32m// Inverted navbar brand label[m
[32m+[m[32m$navbar-inverse-brand-color:                $navbar-inverse-link-color !default;[m
[32m+[m[32m$navbar-inverse-brand-hover-color:          $navbar-inverse-link-hover-color !default;[m
