[32m+[m
[32m+[m[32m// Reset the box-sizing[m
[32m+[m[32m//[m
[32m+[m[32m// Heads up! This reset may cause conflicts with some third-party widgets.[m
[32m+[m[32m// For recommendations on resolving such conflicts, see[m
[32m+[m[32m// http://getbootstrap.com/getting-started/#third-box-sizing[m
[32m+[m[32m* {[m
[32m+[m[32m  @include box-sizing(border-box);[m
[32m+[m[32m}[m
[32m+[m[32m*:before,[m
[32m+[m[32m*:after {[m
[32m+[m[32m  @include box-sizing(border-box);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Body reset[m
[32m+[m
[32m+[m[32mhtml {[m
[32m+[m[32m  font-size: 10px;[m
[32m+[m[32m  -webkit-tap-highlight-color: rgba(0,0,0,0);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mbody {[m
[32m+[m[32m  font-family: $font-family-base;[m
[32m+[m[32m  font-size: $font-size-base;[m
[32m+[m[32m  line-height: $line-height-base;[m
[32m+[m[32m  color: $text-color;[m
[32m+[m[32m  background-color: $body-bg;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Reset fonts for relevant elements[m
[32m+[m[32minput,[m
[32m+[m[32mbutton,[m
[32m+[m[32mselect,[m
[32m+[m[32mtextarea {[m
[32m+[m[32m  font-family: inherit;[m
[32m+[m[32m  font-size: inherit;[m
[32m+[m[32m  line-height: inherit;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Links[m
[32m+[m
[32m+[m[32ma {[m
[32m+[m[32m  color: $link-color;[m
[32m+[m[32m  text-decoration: none;[m
[32m+[m
[32m+[m[32m  &:hover,[m
[32m+[m[32m  &:focus {[m
[32m+[m[32m    color: $link-hover-color;[m
[32m+[m[32m    text-decoration: $link-hover-decoration;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  &:focus {[m
[32m+[m[32m    @include tab-focus;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Figures[m
[32m+[m[32m//[m
[32m+[m[32m// We reset this here because previously Normalize had no `figure` margins. This[m
[32m+[m[32m// ensures we don't break anyone's use of the element.[m
[32m+[m
[32m+[m[32mfigure {[m
[32m+[m[32m  margin: 0;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Images[m
[32m+[m
[32m+[m[32mimg {[m
[32m+[m[32m  vertical-align: middle;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Responsive images (ensure images don't scale beyond their parents)[m
[32m+[m[32m.img-responsive {[m
[32m+[m[32m  @include img-responsive;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Rounded corners[m
[32m+[m[32m.img-rounded {[m
[32m+[m[32m  border-radius: $border-radius-large;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Image thumbnails[m
[32m+[m[32m//[m
[32m+[m[32m// Heads up! This is mixin-ed into thumbnails.less for `.thumbnail`.[m
[32m+[m[32m.img-thumbnail {[m
[32m+[m[32m  padding: $thumbnail-padding;[m
[32m+[m[32m  line-height: $line-height-base;[m
[32m+[m[32m  background-color: $thumbnail-bg;[m
[32m+[m[32m  border: 1px solid $thumbnail-border;[m
[32m+[m[32m  border-radius: $thumbnail-border-radius;[m
[32m+[m[32m  @include transition(all .2s ease-in-out);[m
[32m+[m
[32m+[m[32m  // Keep them at most 100% wide[m
[32m+[m[32m  @include img-responsive(inline-block);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Perfect circle[m
[32m+[m[32m.img-circle {[m
[32m+[m[32m  border-radius: 50%; // set radius in percents[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Horizontal rules[m
[32m+[m
[32m+[m[32mhr {[m
[32m+[m[32m  margin-top:    $line-height-computed;[m
[32m+[m[32m  margin-bottom: $line-height-computed;[m
[32m+[m[32m  border: 0;[m
[32m+[m[32m  border-top: 1px solid $hr-border;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Only display content to screen readers[m
[32m+[m[32m//[m
[32m+[m[32m// See: http://a11yproject.com/posts/how-to-hide-content[m
[32m+[m
[32m+[m[32m.sr-only {[m
[32m+[m[32m  position: absolute;[m
[32m+[m[32m  width: 1px;[m
[32m+[m[32m  height: 1px;[m
[32m+[m[32m  margin: -1px;[m
[32m+[m[32m  padding: 0;[m
[32m+[m[32m  overflow: hidden;[m
[32m+[m[32m  clip: rect(0,0,0,0);[m
[32m+[m[32m  border: 0;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Use in conjunction with .sr-only to only display content when it's focused.[m
[32m+[m[32m// Useful for "Skip to main content" links; see http://www.w3.org/TR/2013/NOTE-WCAG20-TECHS-20130905/G1[m
[32m+[m[32m// Credit: HTML5 Boilerplate[m
[32m+[m
[32m+[m[32m.sr-only-focusable {[m
[32m+[m[32m  &:active,[m
[32m+[m[32m  &:focus {[m
[32m+[m[32m    position: static;[m
[32m+[m[32m    width: auto;[m
[32m+[m[32m    height: auto;[m
[32m+[m[32m    margin: 0;[m
[32m+[m[32m    overflow: visible;[m
[32m+[m[32m    clip: auto;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// iOS "clickable elements" fix for role="button"[m
[32m+[m[32m//[m
[32m+[m[32m// Fixes "clickability" issue (and more generally, the firing of events such as focus as well)[m
[32m+[m[32m// for traditionally non-focusable elements with role="button"[m
[32m+[m[32m// see https://developer.mozilla.org/en-US/docs/Web/Events/click#Safari_Mobile[m
[32m+[m
[32m+[m[32m[role="button"] {[m
[32m+[m[32m  cursor: pointer;[m
[32m+[m[32m}[m
