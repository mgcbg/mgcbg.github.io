[32m+[m[32m@keyframes sk-stretchdelay {[m[41m[m
[32m+[m[32m  0%, 40%, 100% {[m[41m[m
[32m+[m[32m    transform: scaleY(0.5);[m[41m[m
[32m+[m[32m    -webkit-transform: scaleY(0.5);[m[41m[m
[32m+[m[32m  }  20% {[m[41m[m
[32m+[m[32m    transform: scaleY(1.0);[m[41m[m
[32m+[m[32m    -webkit-transform: scaleY(1.0);[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
