[32m+[m[32m    top: 0;[m
[32m+[m[32m    padding: 1px 5px;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // [converter] extracted a& to a.badge[m
[32m+[m
[32m+[m[32m  // Account for badges in navs[m
[32m+[m[32m  .list-group-item.active > &,[m
[32m+[m[32m  .nav-pills > .active > a > & {[m
[32m+[m[32m    color: $badge-active-color;[m
[32m+[m[32m    background-color: $badge-active-bg;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  .list-group-item > & {[m
[32m+[m[32m    float: right;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  .list-group-item > & + & {[m
[32m+[m[32m    margin-right: 5px;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  .nav-pills > li > a > & {[m
[32m+[m[32m    margin-left: 3px;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
