[1mdiff --git a/_sass/bootstrap/mixins/_vendor-prefixes.scss b/_sass/bootstrap/mixins/_vendor-prefixes.scss[m
[1mnew file mode 100644[m
[1mindex 0000000..b3d0371[m
[1m--- /dev/null[m
[1m+++ b/_sass/bootstrap/mixins/_vendor-prefixes.scss[m
[36m@@ -0,0 +1,222 @@[m
[32m+[m[32m// Vendor Prefixes[m
[32m+[m[32m//[m
[32m+[m[32m// All vendor mixins are deprecated as of v3.2.0 due to the introduction of[m
[32m+[m[32m// Autoprefixer in our Gruntfile. They have been removed in v4.[m
[32m+[m
[32m+[m[32m// - Animations[m
[32m+[m[32m// - Backface visibility[m
[32m+[m[32m// - Box shadow[m
[32m+[m[32m// - Box sizing[m
[32m+[m[32m// - Content columns[m
[32m+[m[32m// - Hyphens[m
[32m+[m[32m// - Placeholder text[m
[32m+[m[32m// - Transformations[m
[32m+[m[32m// - Transitions[m
[32m+[m[32m// - User Select[m
[32m+[m
[32m+[m
[32m+[m[32m// Animations[m
[32m+[m[32m@mixin animation($animation) {[m
[32m+[m[32m  -webkit-animation: $animation;[m
[32m+[m[32m       -o-animation: $animation;[m
[32m+[m[32m          animation: $animation;[m
[32m+[m[32m}[m
[32m+[m[32m@mixin animation-name($name) {[m
[32m+[m[32m  -webkit-animation-name: $name;[m
[32m+[m[32m          animation-name: $name;[m
[32m+[m[32m}[m
[32m+[m[32m@mixin animation-duration($duration) {[m
[32m+[m[32m  -webkit-animation-duration: $duration;[m
[32m+[m[32m          animation-duration: $duration;[m
[32m+[m[32m}[m
[32m+[m[32m@mixin animation-timing-function($timing-function) {[m
[32m+[m[32m  -webkit-animation-timing-function: $timing-function;[m
[32m+[m[32m          animation-timing-function: $timing-function;[m
[32m+[m[32m}[m
[32m+[m[32m@mixin animation-delay($delay) {[m
[32m+[m[32m  -webkit-animation-delay: $delay;[m
[32m+[m[32m          animation-delay: $delay;[m
[32m+[m[32m}[m
[32m+[m[32m@mixin animation-iteration-count($iteration-count) {[m
[32m+[m[32m  -webkit-animation-iteration-count: $iteration-count;[m
[32m+[m[32m          animation-iteration-count: $iteration-count;[m
[32m+[m[32m}[m
[32m+[m[32m@mixin animation-direction($direction) {[m
[32m+[m[32m  -webkit-animation-direction: $direction;[m
[32m+[m[32m          animation-direction: $direction;[m
[32m+[m[32m}[m
[32m+[m[32m@mixin animation-fill-mode($fill-mode) {[m
[32m+[m[32m  -webkit-animation-fill-mode: $fill-mode;[m
[32m+[m[32m          animation-fill-mode: $fill-mode;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Backface visibility[m
[32m+[m[32m// Prevent browsers from flickering when using CSS 3D transforms.[m
[32m+[m[32m// Default value is `visible`, but can be changed to `hidden`[m
[32m+[m
[32m+[m[32m@mixin backface-visibility($visibility) {[m
[32m+[m[32m  -webkit-backface-visibility: $visibility;[m
[32m+[m[32m     -moz-backface-visibility: $visibility;[m
[32m+[m[32m          backface-visibility: $visibility;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Drop shadows[m
[32m+[m[32m//[m
[32m+[m[32m// Note: Deprecated `.box-shadow()` as of v3.1.0 since all of Bootstrap's[m
[32m+[m[32m// supported browsers that have box shadow capabilities now support it.[m
[32m+[m
[32m+[m[32m@mixin box-shadow($shadow...) {[m
[32m+[m[32m  -webkit-box-shadow: $shadow; // iOS <4.3 & Android <4.1[m
[32m+[m[32m          box-shadow: $shadow;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Box sizing[m
[32m+[m[32m@mixin box-sizing($boxmodel) {[m
[32m+[m[32m  -webkit-box-sizing: $boxmodel;[m
[32m+[m[32m     -moz-box-sizing: $boxmodel;[m
[32m+[m[32m          box-sizing: $boxmodel;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// CSS3 Content Columns[m
[32m+[m[32m@mixin content-columns($column-count, $column-gap: $grid-gutter-width) {[m
[32m+[m[32m  -webkit-column-count: $column-count;[m
[32m+[m[32m     -moz-column-count: $column-count;[m
[32m+[m[32m          column-count: $column-count;[m
[32m+[m[32m  -webkit-column-gap: $column-gap;[m
[32m+[m[32m     -moz-column-gap: $column-gap;[m
[32m+[m[32m          column-gap: $column-gap;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Optional hyphenation[m
[32m+[m[32m@mixin hyphens($mode: auto) {[m
[32m+[m[32m  word-wrap: break-word;[m
[32m+[m[32m  -webkit-hyphens: $mode;[m
[32m+[m[32m     -moz-hyphens: $mode;[m
[32m+[m[32m      -ms-hyphens: $mode; // IE10+[m
[32m+[m[32m       -o-hyphens: $mode;[m
[32m+[m[32m          hyphens: $mode;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Placeholder text[m
[32m+[m[32m@mixin placeholder($color: $input-color-placeholder) {[m
[32m+[m[32m  // Firefox[m
[32m+[m[32m  &::-moz-placeholder {[m
[32m+[m[32m    color: $color;[m
[32m+[m[32m    opacity: 1; // Override Firefox's unusual default opacity; see https://github.com/twbs/bootstrap/pull/11526[m
[32m+[m[32m  }[m
[32m+[m[32m  &:-ms-input-placeholder { color: $color; } // Internet Explorer 10+[m
[32m+[m[32m  &::-webkit-input-placeholder  { color: $color; } // Safari and Chrome[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Transformations[m
[32m+[m[32m@mixin scale($ratio...) {[m
[32m+[m[32m  -webkit-transform: scale($ratio);[m
[32m+[m[32m      -ms-transform: scale($ratio); // IE9 only[m
[32m+[m[32m       -o-transform: scale($ratio);[m
[32m+[m[32m          transform: scale($ratio);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m@mixin scaleX($ratio) {[m
[32m+[m[32m  -webkit-transform: scaleX($ratio);[m
[32m+[m[32m      -ms-transform: scaleX($ratio); // IE9 only[m
[32m+[m[32m       -o-transform: scaleX($ratio);[m
[32m+[m[32m          transform: scaleX($ratio);[m
[32m+[m[32m}[m
[32m+[m[32m@mixin scaleY($ratio) {[m
[32m+[m[32m  -webkit-transform: scaleY($ratio);[m
[32m+[m[32m      -ms-transform: scaleY($ratio); // IE9 only[m
[32m+[m[32m       -o-transform: scaleY($ratio);[m
[32m+[m[32m          transform: scaleY($ratio);[m
[32m+[m[32m}[m
[32m+[m[32m@mixin skew($x, $y) {[m
[32m+[m[32m  -webkit-transform: skewX($x) skewY($y);[m
[32m+[m[32m      -ms-transform: skewX($x) skewY($y); // See https://github.com/twbs/bootstrap/issues/4885; IE9+[m
[32m+[m[32m       -o-transform: skewX($x) skewY($y);[m
[32m+[m[32m          transform: skewX($x) skewY($y);[m
[32m+[m[32m}[m
[32m+[m[32m@mixin translate($x, $y) {[m
[32m+[m[32m  -webkit-transform: translate($x, $y);[m
[32m+[m[32m      -ms-transform: translate($x, $y); // IE9 only[m
[32m+[m[32m       -o-transform: translate($x, $y);[m
[32m+[m[32m          transform: translate($x, $y);[m
[32m+[m[32m}[m
[32m+[m[32m@mixin translate3d($x, $y, $z) {[m
[32m+[m[32m  -webkit-transform: translate3d($x, $y, $z);[m
[32m+[m[32m          transform: translate3d($x, $y, $z);[m
[32m+[m[32m}[m
[32m+[m[32m@mixin rotate($degrees) {[m
[32m+[m[32m  -webkit-transform: rotate($degrees);[m
[32m+[m[32m      -ms-transform: rotate($degrees); // IE9 only[m
[32m+[m[32m       -o-transform: rotate($degrees);[m
[32m+[m[32m          transform: rotate($degrees);[m
[32m+[m[32m}[m
[32m+[m[32m@mixin rotateX($degrees) {[m
[32m+[m[32m  -webkit-transform: rotateX($degrees);[m
[32m+[m[32m      -ms-transform: rotateX($degrees); // IE9 only[m
[32m+[m[32m       -o-transform: rotateX($degrees);[m
[32m+[m[32m          transform: rotateX($degrees);[m
[32m+[m[32m}[m
[32m+[m[32m@mixin rotateY($degrees) {[m
[32m+[m[32m  -webkit-transform: rotateY($degrees);[m
[32m+[m[32m      -ms-transform: rotateY($degrees); // IE9 only[m
[32m+[m[32m       -o-transform: rotateY($degrees);[m
[32m+[m[32m          transform: rotateY($degrees);[m
[32m+[m[32m}[m
[32m+[m[32m@mixin perspective($perspective) {[m
[32m+[m[32m  -webkit-perspective: $perspective;[m
[32m+[m[32m     -moz-perspective: $perspective;[m
[32m+[m[32m          perspective: $perspective;[m
[32m+[m[32m}[m
[32m+[m[32m@mixin perspective-origin($perspective) {[m
[32m+[m[32m  -webkit-perspective-origin: $perspective;[m
[32m+[m[32m     -moz-perspective-origin: $perspective;[m
[32m+[m[32m          perspective-origin: $perspective;[m
[32m+[m[32m}[m
[32m+[m[32m@mixin transform-origin($origin) {[m
[32m+[m[32m  -webkit-transform-origin: $origin;[m
[32m+[m[32m     -moz-transform-origin: $origin;[m
[32m+[m[32m      -ms-transform-origin: $origin; // IE9 only[m
[32m+[m[32m          transform-origin: $origin;[m
[32m+[m[32m}[m
