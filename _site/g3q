[32m+[m
[32m+[m[32m  &:hover,[m
[32m+[m[32m  &:focus {[m
[32m+[m[32m    color: $close-color;[m
[32m+[m[32m    text-decoration: none;[m
[32m+[m[32m    cursor: pointer;[m
[32m+[m[32m    @include opacity(.5);[m
[32m+[m[32m  }[m
