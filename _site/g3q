[32m+[m
[32m+[m[32m@include text-emphasis-variant('.text-warning', $state-warning-text);[m
[32m+[m
[32m+[m[32m@include text-emphasis-variant('.text-danger', $state-danger-text);[m
[32m+[m
[32m+[m[32m// Contextual backgrounds[m
[32m+[m[32m// For now we'll leave these alongside the text classes until v4 when we can[m
[32m+[m[32m// safely shift things around (per SemVer rules).[m
[32m+[m[32m.bg-primary {[m
[32m+[m[32m  // Given the contrast here, this is the only class to have its color inverted[m
[32m+[m[32m  // automatically.[m
[32m+[m[32m  color: #fff;[m
[32m+[m[32m}[m
[32m+[m[32m@include bg-variant('.bg-primary', $brand-primary);[m
[32m+[m
[32m+[m[32m@include bg-variant('.bg-success', $state-success-bg);[m
[32m+[m
[32m+[m[32m@include bg-variant('.bg-info', $state-info-bg);[m
[32m+[m
[32m+[m[32m@include bg-variant('.bg-warning', $state-warning-bg);[m
[32m+[m
