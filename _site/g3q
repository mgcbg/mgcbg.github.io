[1mdiff --git a/_sass/bootstrap/mixins/_clearfix.scss b/_sass/bootstrap/mixins/_clearfix.scss[m
[1mnew file mode 100644[m
[1mindex 0000000..dc3e2ab[m
[1m--- /dev/null[m
[1m+++ b/_sass/bootstrap/mixins/_clearfix.scss[m
[36m@@ -0,0 +1,22 @@[m
[32m+[m[32m// Clearfix[m
[32m+[m[32m//[m
[32m+[m[32m// For modern browsers[m
[32m+[m[32m// 1. The space content is one way to avoid an Opera bug when the[m
[32m+[m[32m//    contenteditable attribute is included anywhere else in the document.[m
[32m+[m[32m//    Otherwise it causes space to appear at the top and bottom of elements[m
[32m+[m[32m//    that are clearfixed.[m
[32m+[m[32m// 2. The use of `table` rather than `block` is only necessary if using[m
[32m+[m[32m//    `:before` to contain the top-margins of child elements.[m
[32m+[m[32m//[m
[32m+[m[32m// Source: http://nicolasgallagher.com/micro-clearfix-hack/[m
[32m+[m
[32m+[m[32m@mixin clearfix() {[m
[32m+[m[32m  &:before,[m
[32m+[m[32m  &:after {[m
[32m+[m[32m    content: " "; // 1[m
[32m+[m[32m    display: table; // 2[m
[32m+[m[32m  }[m
[32m+[m[32m  &:after {[m
[32m+[m[32m    clear: both;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[1mdiff --git a/_sass/bootstrap/mixins/_forms.scss b/_sass/bootstrap/mixins/_forms.scss[m
[1mnew file mode 100644[m
[1mindex 0000000..277aa5f[m
[1m--- /dev/null[m
[1m+++ b/_sass/bootstrap/mixins/_forms.scss[m
[36m@@ -0,0 +1,88 @@[m
[32m+[m[32m// Form validation states[m
[32m+[m[32m//[m
[32m+[m[32m// Used in forms.less to generate the form validation CSS for warnings, errors,[m
[32m+[m[32m// and successes.[m
[32m+[m
[32m+[m[32m@mixin form-control-validation($text-color: #555, $border-color: #ccc, $background-color: #f5f5f5) {[m
[32m+[m[32m  // Color the label and help text[m
[32m+[m[32m  .help-block,[m
[32m+[m[32m  .control-label,[m
[32m+[m[32m  .radio,[m
[32m+[m[32m  .checkbox,[m
[32m+[m[32m  .radio-inline,[m
[32m+[m[32m  .checkbox-inline,[m
[32m+[m[32m  &.radio label,[m
[32m+[m[32m  &.checkbox label,[m
[32m+[m[32m  &.radio-inline label,[m
[32m+[m[32m  &.checkbox-inline label  {[m
[32m+[m[32m    color: $text-color;[m
[32m+[m[32m  }[m
[32m+[m[32m  // Set the border and box shadow on specific inputs to match[m
[32m+[m[32m  .form-control {[m
[32m+[m[32m    border-color: $border-color;[m
[32m+[m[32m    @include box-shadow(inset 0 1px 1px rgba(0,0,0,.075)); // Redeclare so transitions work[m
[32m+[m[32m    &:focus {[m
[32m+[m[32m      border-color: darken($border-color, 10%);[m
[32m+[m[32m      $shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 6px lighten($border-color, 20%);[m
[32m+[m[32m      @include box-shadow($shadow);[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  // Set validation states also for addons[m
[32m+[m[32m  .input-group-addon {[m
[32m+[m[32m    color: $text-color;[m
[32m+[m[32m    border-color: $border-color;[m
[32m+[m[32m    background-color: $background-color;[m
[32m+[m[32m  }[m
[32m+[m[32m  // Optional feedback icon[m
[32m+[m[32m  .form-control-feedback {[m
[32m+[m[32m    color: $text-color;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Form control focus state[m
[32m+[m[32m//[m
[32m+[m[32m// Generate a customized focus state and for any input with the specified color,[m
[32m+[m[32m// which defaults to the `$input-border-focus` variable.[m
[32m+[m[32m//[m
[32m+[m[32m// We highly encourage you to not customize the default value, but instead use[m
[32m+[m[32m// this to tweak colors on an as-needed basis. This aesthetic change is based on[m
[32m+[m[32m// WebKit's default styles, but applicable to a wider range of browsers. Its[m
[32m+[m[32m// usability and accessibility should be taken into account with any change.[m
[32m+[m[32m//[m
[32m+[m[32m// Example usage: change the default blue border and shadow to white for better[m
[32m+[m[32m// contrast against a dark gray background.[m
[32m+[m[32m@mixin form-control-focus($color: $input-border-focus) {[m
[32m+[m[32m  $color-rgba: rgba(red($color), green($color), blue($color), .6);[m
[32m+[m[32m  &:focus {[m
[32m+[m[32m    border-color: $color;[m
[32m+[m[32m    outline: 0;[m
[32m+[m[32m    @include box-shadow(inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px $color-rgba);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Form control sizing[m
[32m+[m[32m//[m
[32m+[m[32m// Relative text size, padding, and border-radii changes for form controls. For[m
[32m+[m[32m// horizontal sizing, wrap controls in the predefined grid classes. `<select>`[m
[32m+[m[32m// element gets special love because it's special, and that's a fact![m
[32m+[m[32m// [converter] $parent hack[m
[32m+[m[32m@mixin input-size($parent, $input-height, $padding-vertical, $padding-horizontal, $font-size, $line-height, $border-radius) {[m
[32m+[m[32m  #{$parent} {[m
[32m+[m[32m    height: $input-height;[m
[32m+[m[32m    padding: $padding-vertical $padding-horizontal;[m
[32m+[m[32m    font-size: $font-size;[m
[32m+[m[32m    line-height: $line-height;[m
[32m+[m[32m    border-radius: $border-radius;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  select#{$parent} {[m
[32m+[m[32m    height: $input-height;[m
[32m+[m[32m    line-height: $input-height;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  textarea#{$parent},[m
[32m+[m[32m  select[multiple]#{$parent} {[m
[32m+[m[32m    height: auto;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[1mdiff --git a/_sass/bootstrap/mixins/_gradients.scss b/_sass/bootstrap/mixins/_gradients.scss[m
[1mnew file mode 100644[m
[1mindex 0000000..a8939f5[m
[1m--- /dev/null[m
[1m+++ b/_sass/bootstrap/mixins/_gradients.scss[m
[36m@@ -0,0 +1,58 @@[m
[32m+[m[32m// Gradients[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m// Horizontal gradient, from left to right[m
[32m+[m[32m//[m
[32m+[m[32m// Creates two color stops, start and end, by specifying a color and position for each color stop.[m
[32m+[m[32m// Color stops are not available in IE9 and below.[m
[32m+[m[32m@mixin gradient-horizontal($start-color: #555, $end-color: #333, $start-percent: 0%, $end-percent: 100%) {[m
[32m+[m[32m  background-image: -webkit-linear-gradient(left, $start-color $start-percent, $end-color $end-percent); // Safari 5.1-6, Chrome 10+[m
[32m+[m[32m  background-image: -o-linear-gradient(left, $start-color $start-percent, $end-color $end-percent); // Opera 12[m
[32m+[m[32m  background-image: linear-gradient(to right, $start-color $start-percent, $end-color $end-percent); // Standard, IE10, Firefox 16+, Opera 12.10+, Safari 7+, Chrome 26+[m
[32m+[m[32m  background-repeat: repeat-x;[m
[32m+[m[32m  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#{ie-hex-str($start-color)}', endColorstr='#{ie-hex-str($end-color)}', GradientType=1); // IE9 and down[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Vertical gradient, from top to bottom[m
[32m+[m[32m//[m
[32m+[m[32m// Creates two color stops, start and end, by specifying a color and position for each color stop.[m
[32m+[m[32m// Color stops are not available in IE9 and below.[m
[32m+[m[32m@mixin gradient-vertical($start-color: #555, $end-color: #333, $start-percent: 0%, $end-percent: 100%) {[m
[32m+[m[32m  background-image: -webkit-linear-gradient(top, $start-color $start-percent, $end-color $end-percent);  // Safari 5.1-6, Chrome 10+[m
[32m+[m[32m  background-image: -o-linear-gradient(top, $start-color $start-percent, $end-color $end-percent);  // Opera 12[m
[32m+[m[32m  background-image: linear-gradient(to bottom, $start-color $start-percent, $end-color $end-percent); // Standard, IE10, Firefox 16+, Opera 12.10+, Safari 7+, Chrome 26+[m
[32m+[m[32m  background-repeat: repeat-x;[m
[32m+[m[32m  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#{ie-hex-str($start-color)}', endColorstr='#{ie-hex-str($end-color)}', GradientType=0); // IE9 and down[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m@mixin gradient-directional($start-color: #555, $end-color: #333, $deg: 45deg) {[m
[32m+[m[32m  background-repeat: repeat-x;[m
[32m+[m[32m  background-image: -webkit-linear-gradient($deg, $start-color, $end-color); // Safari 5.1-6, Chrome 10+[m
[32m+[m[32m  background-image: -o-linear-gradient($deg, $start-color, $end-color); // Opera 12[m
[32m+[m[32m  background-image: linear-gradient($deg, $start-color, $end-color); // Standard, IE10, Firefox 16+, Opera 12.10+, Safari 7+, Chrome 26+[m
[32m+[m[32m}[m
[32m+[m[32m@mixin gradient-horizontal-three-colors($start-color: #00b3ee, $mid-color: #7a43b6, $color-stop: 50%, $end-color: #c3325f) {[m
[32m+[m[32m  background-image: -webkit-linear-gradient(left, $start-color, $mid-color $color-stop, $end-color);[m
[32m+[m[32m  background-image: -o-linear-gradient(left, $start-color, $mid-color $color-stop, $end-color);[m
[32m+[m[32m  background-image: linear-gradient(to right, $start-color, $mid-color $color-stop, $end-color);[m
[32m+[m[32m  background-repeat: no-repeat;[m
[32m+[m[32m  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#{ie-hex-str($start-color)}', endColorstr='#{ie-hex-str($end-color)}', GradientType=1); // IE9 and down, gets no color-stop at all for proper fallback[m
[32m+[m[32m}[m
[32m+[m[32m@mixin gradient-vertical-three-colors($start-color: #00b3ee, $mid-color: #7a43b6, $color-stop: 50%, $end-color: #c3325f) {[m
[32m+[m[32m  background-image: -webkit-linear-gradient($start-color, $mid-color $color-stop, $end-color);[m
[32m+[m[32m  background-image: -o-linear-gradient($start-color, $mid-color $color-stop, $end-color);[m
[32m+[m[32m  background-image: linear-gradient($start-color, $mid-color $color-stop, $end-color);[m
[32m+[m[32m  background-repeat: no-repeat;[m
[32m+[m[32m  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#{ie-hex-str($start-color)}', endColorstr='#{ie-hex-str($end-color)}', GradientType=0); // IE9 and down, gets no color-stop at all for proper fallback[m
[32m+[m[32m}[m
[32m+[m[32m@mixin gradient-radial($inner-color: #555, $outer-color: #333) {[m
[32m+[m[32m  background-image: -webkit-radial-gradient(circle, $inner-color, $outer-color);[m
[32m+[m[32m  background-image: radial-gradient(circle, $inner-color, $outer-color);[m
[32m+[m[32m  background-repeat: no-repeat;[m
[32m+[m[32m}[m
[32m+[m[32m@mixin gradient-striped($color: rgba(255,255,255,.15), $angle: 45deg) {[m
[32m+[m[32m  background-image: -webkit-linear-gradient($angle, $color 25%, transparent 25%, transparent 50%, $color 50%, $color 75%, transparent 75%, transparent);[m
[32m+[m[32m  background-image: -o-linear-gradient($angle, $color 25%, transparent 25%, transparent 50%, $color 50%, $color 75%, transparent 75%, transparent);[m
[32m+[m[32m  background-image: linear-gradient($angle, $color 25%, transparent 25%, transparent 50%, $color 50%, $color 75%, transparent 75%, transparent);[m
[32m+[m[32m}[m
[1mdiff --git a/_sass/bootstrap/mixins/_grid-framework.scss b/_sass/bootstrap/mixins/_grid-framework.scss[m
[1mnew file mode 100644[m
[1mindex 0000000..16d038c[m
[1m--- /dev/null[m
[1m+++ b/_sass/bootstrap/mixins/_grid-framework.scss[m
[36m@@ -0,0 +1,81 @@[m
[32m+[m[32m// Framework grid generation[m
[32m+[m[32m//[m
[32m+[m[32m// Used only by Bootstrap to generate the correct number of grid classes given[m
[32m+[m[32m// any value of `$grid-columns`.[m
[32m+[m
[32m+[m[32m// [converter] This is defined recursively in LESS, but Sass supports real loops[m
[32m+[m[32m@mixin make-grid-columns($i: 1, $list: ".col-xs-#{$i}, .col-sm-#{$i}, .col-md-#{$i}, .col-lg-#{$i}") {[m
[32m+[m[32m  @for $i from (1 + 1) through $grid-columns {[m
[32m+[m[32m    $list: "#{$list}, .col-xs-#{$i}, .col-sm-#{$i}, .col-md-#{$i}, .col-lg-#{$i}";[m
[32m+[m[32m  }[m
[32m+[m[32m  #{$list} {[m
[32m+[m[32m    position: relative;[m
[32m+[m[32m    // Prevent columns from collapsing when empty[m
[32m+[m[32m    min-height: 1px;[m
[32m+[m[32m    // Inner gutter via padding[m
[32m+[m[32m    padding-left:  ceil(($grid-gutter-width / 2));[m
[32m+[m[32m    padding-right: floor(($grid-gutter-width / 2));[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// [converter] This is defined recursively in LESS, but Sass supports real loops[m
[32m+[m[32m@mixin float-grid-columns($class, $i: 1, $list: ".col-#{$class}-#{$i}") {[m
[32m+[m[32m  @for $i from (1 + 1) through $grid-columns {[m
[32m+[m[32m    $list: "#{$list}, .col-#{$class}-#{$i}";[m
[32m+[m[32m  }[m
[32m+[m[32m  #{$list} {[m
[32m+[m[32m    float: left;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m@mixin calc-grid-column($index, $class, $type) {[m
[32m+[m[32m  @if ($type == width) and ($index > 0) {[m
[32m+[m[32m    .col-#{$class}-#{$index} {[m
[32m+[m[32m      width: percentage(($index / $grid-columns));[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  @if ($type == push) and ($index > 0) {[m
[32m+[m[32m    .col-#{$class}-push-#{$index} {[m
[32m+[m[32m      left: percentage(($index / $grid-columns));[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  @if ($type == push) and ($index == 0) {[m
[32m+[m[32m    .col-#{$class}-push-0 {[m
[32m+[m[32m      left: auto;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  @if ($type == pull) and ($index > 0) {[m
[32m+[m[32m    .col-#{$class}-pull-#{$index} {[m
[32m+[m[32m      right: percentage(($index / $grid-columns));[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  @if ($type == pull) and ($index == 0) {[m
[32m+[m[32m    .col-#{$class}-pull-0 {[m
[32m+[m[32m      right: auto;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  @if ($type == offset) {[m
[32m+[m[32m    .col-#{$class}-offset-#{$index} {[m
[32m+[m[32m      margin-left: percentage(($index / $grid-columns));[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// [converter] This is defined recursively in LESS, but Sass supports real loops[m
[32m+[m[32m@mixin loop-grid-columns($columns, $class, $type) {[m
[32m+[m[32m  @for $i from 0 through $columns {[m
[32m+[m[32m    @include calc-grid-column($i, $class, $type);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Create grid for specific class[m
[32m+[m[32m@mixin make-grid($class) {[m
[32m+[m[32m  @include float-grid-columns($class);[m
[32m+[m[32m  @include loop-grid-columns($grid-columns, $class, width);[m
[32m+[m[32m  @include loop-grid-columns($grid-columns, $class, pull);[m
[32m+[m[32m  @include loop-grid-columns($grid-columns, $class, push);[m
[32m+[m[32m  @include loop-grid-columns($grid-columns, $class, offset);[m
[32m+[m[32m}[m
[1mdiff --git a/_sass/bootstrap/mixins/_grid.scss b/_sass/bootstrap/mixins/_grid.scss[m
[1mnew file mode 100644[m
[1mindex 0000000..59551da[m
[1m--- /dev/null[m
[1m+++ b/_sass/bootstrap/mixins/_grid.scss[m
[36m@@ -0,0 +1,122 @@[m
[32m+[m[32m// Grid system[m
[32m+[m[32m//[m
[32m+[m[32m// Generate semantic grid columns with these mixins.[m
[32m+[m
[32m+[m[32m// Centered container element[m
[32m+[m[32m@mixin container-fixed($gutter: $grid-gutter-width) {[m
[32m+[m[32m  margin-right: auto;[m
[32m+[m[32m  margin-left: auto;[m
[32m+[m[32m  padding-left:  floor(($gutter / 2));[m
[32m+[m[32m  padding-right: ceil(($gutter / 2));[m
[32m+[m[32m  @include clearfix;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Creates a wrapper for a series of columns[m
[32m+[m[32m@mixin make-row($gutter: $grid-gutter-width) {[m
[32m+[m[32m  margin-left:  ceil(($gutter / -2));[m
[32m+[m[32m  margin-right: floor(($gutter / -2));[m
[32m+[m[32m  @include clearfix;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Generate the extra small columns[m
[32m+[m[32m@mixin make-xs-column($columns, $gutter: $grid-gutter-width) {[m
[32m+[m[32m  position: relative;[m
[32m+[m[32m  float: left;[m
[32m+[m[32m  width: percentage(($columns / $grid-columns));[m
[32m+[m[32m  min-height: 1px;[m
[32m+[m[32m  padding-left:  ($gutter / 2);[m
[32m+[m[32m  padding-right: ($gutter / 2);[m
[32m+[m[32m}[m
[32m+[m[32m@mixin make-xs-column-offset($columns) {[m
[32m+[m[32m  margin-left: percentage(($columns / $grid-columns));[m
[32m+[m[32m}[m
[32m+[m[32m@mixin make-xs-column-push($columns) {[m
[32m+[m[32m  left: percentage(($columns / $grid-columns));[m
[32m+[m[32m}[m
[32m+[m[32m@mixin make-xs-column-pull($columns) {[m
[32m+[m[32m  right: percentage(($columns / $grid-columns));[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Generate the small columns[m
[32m+[m[32m@mixin make-sm-column($columns, $gutter: $grid-gutter-width) {[m
[32m+[m[32m  position: relative;[m
[32m+[m[32m  min-height: 1px;[m
[32m+[m[32m  padding-left:  ($gutter / 2);[m
[32m+[m[32m  padding-right: ($gutter / 2);[m
[32m+[m
[32m+[m[32m  @media (min-width: $screen-sm-min) {[m
[32m+[m[32m    float: left;[m
[32m+[m[32m    width: percentage(($columns / $grid-columns));[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m@mixin make-sm-column-offset($columns) {[m
[32m+[m[32m  @media (min-width: $screen-sm-min) {[m
[32m+[m[32m    margin-left: percentage(($columns / $grid-columns));[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m@mixin make-sm-column-push($columns) {[m
[32m+[m[32m  @media (min-width: $screen-sm-min) {[m
[32m+[m[32m    left: percentage(($columns / $grid-columns));[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m@mixin make-sm-column-pull($columns) {[m
[32m+[m[32m  @media (min-width: $screen-sm-min) {[m
[32m+[m[32m    right: percentage(($columns / $grid-columns));[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Generate the medium columns[m
[32m+[m[32m@mixin make-md-column($columns, $gutter: $grid-gutter-width) {[m
[32m+[m[32m  position: relative;[m
[32m+[m[32m  min-height: 1px;[m
[32m+[m[32m  padding-left:  ($gutter / 2);[m
[32m+[m[32m  padding-right: ($gutter / 2);[m
[32m+[m
[32m+[m[32m  @media (min-width: $screen-md-min) {[m
[32m+[m[32m    float: left;[m
[32m+[m[32m    width: percentage(($columns / $grid-columns));[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m@mixin make-md-column-offset($columns) {[m
[32m+[m[32m  @media (min-width: $screen-md-min) {[m
[32m+[m[32m    margin-left: percentage(($columns / $grid-columns));[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m@mixin make-md-column-push($columns) {[m
[32m+[m[32m  @media (min-width: $screen-md-min) {[m
[32m+[m[32m    left: percentage(($columns / $grid-columns));[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m@mixin make-md-column-pull($columns) {[m
[32m+[m[32m  @media (min-width: $screen-md-min) {[m
[32m+[m[32m    right: percentage(($columns / $grid-columns));[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Generate the large columns[m
[32m+[m[32m@mixin make-lg-column($columns, $gutter: $grid-gutter-width) {[m
[32m+[m[32m  position: relative;[m
[32m+[m[32m  min-height: 1px;[m
[32m+[m[32m  padding-left:  ($gutter / 2);[m
[32m+[m[32m  padding-right: ($gutter / 2);[m
