[32m+[m[32m// Transitions[m
[32m+[m
[32m+[m[32m@mixin transition($transition...) {[m
[32m+[m[32m  -webkit-transition: $transition;[m
[32m+[m[32m       -o-transition: $transition;[m
[32m+[m[32m          transition: $transition;[m
[32m+[m[32m}[m
[32m+[m[32m@mixin transition-property($transition-property...) {[m
[32m+[m[32m  -webkit-transition-property: $transition-property;[m
[32m+[m[32m          transition-property: $transition-property;[m
[32m+[m[32m}[m
[32m+[m[32m@mixin transition-delay($transition-delay) {[m
[32m+[m[32m  -webkit-transition-delay: $transition-delay;[m
[32m+[m[32m          transition-delay: $transition-delay;[m
[32m+[m[32m}[m
[32m+[m[32m@mixin transition-duration($transition-duration...) {[m
[32m+[m[32m  -webkit-transition-duration: $transition-duration;[m
[32m+[m[32m          transition-duration: $transition-duration;[m
[32m+[m[32m}[m
[32m+[m[32m@mixin transition-timing-function($timing-function) {[m
[32m+[m[32m  -webkit-transition-timing-function: $timing-function;[m
[32m+[m[32m          transition-timing-function: $timing-function;[m
[32m+[m[32m}[m
[32m+[m[32m@mixin transition-transform($transition...) {[m
[32m+[m[32m  -webkit-transition: -webkit-transform $transition;[m
[32m+[m[32m     -moz-transition: -moz-transform $transition;[m
[32m+[m[32m       -o-transition: -o-transform $transition;[m
[32m+[m[32m          transition: transform $transition;[m
[32m+[m[32m}[m
