[1mdiff --git a/_plugins/markdown.rb b/_plugins/markdown.rb[m
[1mnew file mode 100644[m
[1mindex 0000000..727bf40[m
[1m--- /dev/null[m
[1m+++ b/_plugins/markdown.rb[m
[36m@@ -0,0 +1,23 @@[m
[32m+[m[32m=begin[m
[32m+[m[32m  Jekyll tag to include Markdown text from _includes directory preprocessing with Liquid.[m
[32m+[m[32m  Usage:[m
[32m+[m[32m    {% markdown <filename> %}[m
[32m+[m[32m  Dependency:[m
[32m+[m[32m    - kramdown[m
[32m+[m[32m=end[m
[32m+[m[32mmodule Jekyll[m
[32m+[m[32m  class MarkdownTag < Liquid::Tag[m
[32m+[m[32m    def initialize(tag_name, text, tokens)[m
[32m+[m[32m      super[m
[32m+[m[32m      @text = text.strip[m
[32m+[m[32m    end[m
[32m+[m[32m    require "kramdown"[m
[32m+[m[32m    def render(context)[m
[32m+[m[32m      tmpl = File.read File.join Dir.pwd, "_includes", @text[m
[32m+[m[32m      site = context.registers[:site][m
[32m+[m[32m      tmpl = (Liquid::Template.parse tmpl).render site.site_payload[m
[32m+[m[32m      html = Kramdown::Document.new(tmpl).to_html[m
[32m+[m[32m    end[m
[32m+[m[32m  end[m
[32m+[m[32mend[m
[32m+[m[32mLiquid::Template.register_tag('markdown', Jekyll::MarkdownTag)[m
