[32m+[m[32m// Button variants[m
[32m+[m[32m//[m
[32m+[m[32m// Easily pump out default styles, as well as :hover, :focus, :active,[m
[32m+[m[32m// and disabled options for all buttons[m
[32m+[m
[32m+[m[32m@mixin button-variant($color, $background, $border) {[m
[32m+[m[32m  color: $color;[m
[32m+[m[32m  background-color: $background;[m
[32m+[m[32m  border-color: $border;[m
[32m+[m
[32m+[m[32m  &:focus,[m
[32m+[m[32m  &.focus {[m
[32m+[m[32m    color: $color;[m
[32m+[m[32m    background-color: darken($background, 10%);[m
[32m+[m[32m        border-color: darken($border, 25%);[m
[32m+[m[32m  }[m
[32m+[m[32m  &:hover {[m
[32m+[m[32m    color: $color;[m
[32m+[m[32m    background-color: darken($background, 10%);[m
[32m+[m[32m        border-color: darken($border, 12%);[m
[32m+[m[32m  }[m
[32m+[m[32m  &:active,[m
[32m+[m[32m  &.active,[m
[32m+[m[32m  .open > &.dropdown-toggle {[m
[32m+[m[32m    color: $color;[m
[32m+[m[32m    background-color: darken($background, 10%);[m
[32m+[m[32m        border-color: darken($border, 12%);[m
[32m+[m
[32m+[m[32m    &:hover,[m
[32m+[m[32m    &:focus,[m
[32m+[m[32m    &.focus {[m
[32m+[m[32m      color: $color;[m
[32m+[m[32m      background-color: darken($background, 17%);[m
[32m+[m[32m          border-color: darken($border, 25%);[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  &:active,[m
[32m+[m[32m  &.active,[m
[32m+[m[32m  .open > &.dropdown-toggle {[m
[32m+[m[32m    background-image: none;[m
[32m+[m[32m  }[m
[32m+[m[32m  &.disabled,[m
